--	Data.Queue.IntQueue
	Data.MQueue.Class
	Data.MQueue.Heap
	Data.MQueue.SyncQueue
	Data.MQueue.Chan
	Data.MQueue
other-modules: 
	Data.Queue.QueueHelpers
	Data.MQueue.MonadHelpers
