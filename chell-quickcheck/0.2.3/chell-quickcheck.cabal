  ghc-options: -Wall -O2

  build-depends:
      base >= 4.0 && < 5.0
    , chell >= 0.3 && < 0.4
    , QuickCheck >= 2.3 && < 2.8
    , random

  exposed-modules:
    Test.Chell.QuickCheck


