build-type:          Simple
cabal-version:       >=1.10

library
  hs-source-dirs:      src
  exposed-modules:     Casadi.CMatrix
                       Casadi.Callback
                       Casadi.DMatrix
                       Casadi.Function
                       Casadi.GenericC
                       Casadi.IOSchemes
                       Casadi.MX
                       Casadi.MXFunction
                       Casadi.Option
                       Casadi.Overloading
                       Casadi.SharedObject
                       Casadi.Slice
                       Casadi.Sparsity
                       Casadi.SX
                       Casadi.SXFunction
  build-depends:       base >=4.6 && <5,
                       linear,
                       vector,
                       containers,
                       casadi-bindings-internal == 0.1.2.1,
                       casadi-bindings-core     == 2.2.0.2
  default-language:    Haskell2010

