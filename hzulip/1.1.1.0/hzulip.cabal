                     , http-types
                     , exceptions >=0.6 && <0.7
                     , mtl >=2.1 && <2.2
                     , transformers
                     , lens >=4.4 && <5
                     , aeson >=0.7 && <1
                     , scotty >=0.9 && <0.10
                     , lens-aeson >=1 && <2
                     , bytestring >=0.10 && <1
                     , hspec >=1.11 && <2
                     , raw-strings-qq >=1.0.2 && <2
                     , text >=1.2 &&<1.3
  default-language:    Haskell2010
