  exposed-modules: Language.SQL.SQLite,
                   Language.SQL.SQLite.Tools
  other-modules: Language.SQL.SQLite.Syntax,
                 Language.SQL.SQLite.Types
  build-depends: base >= 4.1 && < 5,
                 array >= 0.3.0.0,
                 bytestring >= 0.9.1.5,
                 
                 containers >= 0.2.0.1,
                 mtl >= 1.1.0.2,
                 template-haskell,
                 
                 utf8-string >= 0.3.6
