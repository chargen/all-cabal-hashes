  location: https://github.com/supki/xml-html-conduit-lens

source-repository this
  type: git
  tag: 0.3.2.0
  location: https://github.com/supki/xml-html-conduit-lens

library
  default-language:
    Haskell2010
  build-depends:
      base                    >= 4     && < 5
    , bytestring
    , lens                    >= 4.0.1
    , containers              >= 0.4.0 && < 0.6
    , text                    >= 0.11  && < 1.3
    , xml-conduit             >= 1.1   && < 1.3
    , html-conduit            >= 1.1   && < 1.3
  hs-source-dirs:
    src
  ghc-options:
    -Wall
  exposed-modules:
    Text.Xml.Lens
    Text.Xml.Lens.LowLevel

test-suite doctest
  type:                exitcode-stdio-1.0
  default-language:    Haskell2010
  hs-source-dirs:      test
  main-is:             Doctest.hs
  build-depends:
      base                    == 4.*
    , doctest                 >= 0.9.10

test-suite hspec
  type:                exitcode-stdio-1.0
  default-language:    Haskell2010
  hs-source-dirs:      test
  main-is:             Spec.hs
  build-depends:
      base                    == 4.*
    , hspec
    , hspec-expectations-lens >= 0.3
    , lens                    >= 4.0
    , xml-conduit             >= 1.1   && < 1.3
    , xml-html-conduit-lens
  other-modules:
    Text.Xml.LensSpec
