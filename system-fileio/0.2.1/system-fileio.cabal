name: system-fileio
version: 0.2.1
synopsis: High-level filesystem interaction
license: MIT
license-file: license.txt
author: John Millikin <jmillikin@gmail.com>
maintainer: jmillikin@gmail.com
copyright: Copyright (c) John Millikin 2011
build-type: Simple
cabal-version: >=1.6
category: System
stability: experimental
homepage: https://john-millikin.com/software/system-fileio/
bug-reports: mailto:jmillikin@gmail.com
tested-with: GHC==6.12.1

description:
  This is a small wrapper around the \"directory\", \"unix\", and \"Win32\"
  packages for use with \"system-filepath\".

source-repository head
  type: bzr
  location: https://john-millikin.com/software/system-fileio/

library
  ghc-options: -Wall -O2

  build-depends:
      base >= 3 && < 5
    , bytestring >= 0.9 && < 0.10
    , directory >= 1.0 && < 1.2
    , system-filepath >= 0.3 && < 0.4
    , text >= 0.1 && < 0.12
    , time >= 1.0 && < 1.3

  if os(windows)
    cpp-options: -DCABAL_OS_WINDOWS
    build-depends: Win32
  else
    build-depends: unix

  exposed-modules:
    Filesystem
    System.Directory
    System.File
