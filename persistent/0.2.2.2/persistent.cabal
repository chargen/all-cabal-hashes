                     utf8-string >= 0.3.4 && < 0.4,
                     text >= 0.7.1 && < 0.10,
                     hamlet >= 0.5 && < 0.6,
                     web-routes-quasi >= 0.6.0 && < 0.7,
                     containers >= 0.2 && < 0.5,
                     parsec >= 2.1 && < 4,
                     enumerator >= 0.4 && < 0.5,
                     stm >= 2.1 && < 2.2
    exposed-modules: Database.Persist
                     Database.Persist.Base
                     Database.Persist.TH
                     Database.Persist.Quasi
