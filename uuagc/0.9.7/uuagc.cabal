cabal-version: >=1.2
build-type: Simple
name: uuagc
version: 0.9.7
license: GPL
license-file: LICENSE
maintainer: Arie Middelkoop <ariem@cs.uu.nl>
homepage: http://www.cs.uu.nl/wiki/HUT/WebHome
description: Generates Haskell files from an attribute grammar specification
synopsis: Attribute Grammar System of Universiteit Utrecht
category: Development
stability: Stable
copyright: Universiteit Utrecht
extra-source-files: README, uuagc.cabal-for-ghc-6.6

flag small_base
  description: Choose the new smaller, split-up base package.
executable uuagc
  if flag(small_base)
    build-depends: base >= 3, containers, directory, array
  else
    build-depends: base < 3

  build-depends: haskell98, uulib
  main-is: Ag.hs
  other-modules: Ag
    , CommonTypes
    , DepTypes
    , GrammarInfo
    , HsTokenScanner
    , Options
    , PPUtil
    , Parser
    , Pretty
    , Scanner
    , SequentialComputation
    , SequentialTypes
    , Streaming
    , TokenDef
    , Version
    , AbstractSyntax
    , AbstractSyntaxDump
    , Code
    , CodeSyntax
    , CodeSyntaxDump
    , ConcreteSyntax
    , DeclBlocks
    , DefaultRules
    , Desugar
    , ErrorMessages
    , Expression
    , GenerateCode
    , HsToken
    , Interfaces
    , InterfacesRules
    , Order
    , Patterns
    , PrintCode
    , PrintErrorMessages
    , SemHsTokens
    , Transform
    , ATermWrite
    , ATermAbstractSyntax
    , TfmToVisage
    , Visage
    , VisageSyntax
    , VisagePatterns
  extensions: TypeSynonymInstances, MultiParamTypeClasses
  hs-source-dirs: src, src-derived



