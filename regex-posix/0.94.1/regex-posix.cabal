  Exposed-Modules:        Text.Regex.Posix
                          Text.Regex.Posix.Wrap
                          Text.Regex.Posix.String
                          Text.Regex.Posix.Sequence
                          Text.Regex.Posix.ByteString
                          Text.Regex.Posix.ByteString.Lazy
