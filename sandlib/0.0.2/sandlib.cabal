author: Mahmut Bulut <mahmutbulut0@gmail.com>
tested-with: GHC -any
data-dir: ""

source-repository head
    type: git
    location: git://github.com/regularlambda/sandlib.git

library
    build-depends: base >= 2 && < 4
    exposed-modules: AI.Planning.Swarm.SandData
    exposed: True
    buildable: True
    hs-source-dirs: . src



