name:                aterm-utils
version:             0.2.0.2
synopsis:            Utility functions for working with aterms as generated by Minitermite
-- description:         
license:             BSD3
license-file:        LICENSE
author:              Jason Dagit
maintainer:          dagit@galois.com
copyright:           (c) 2012 Galois, Inc 
category:            Language
build-type:          Simple
cabal-version:       >=1.8
homepage:            https://github.com/GaloisInc/aterm-utils

executable ppaterm
  hs-source-dirs:    src
  Main-is:           PPATerm.hs

  Build-depends:       base < 5
                     , aterm
                     , aterm-utils
                     , transformers
                     , wl-pprint

  ghc-options: -O2 -Wall
  if impl(ghc >= 7.0.1)
    ghc-prof-options: -rtsopts
  ghc-prof-options: -prof -auto-all -caf-all

library

  exposed-modules:   ATerm.Generics
                   , ATerm.Matching
                   , ATerm.Pretty
                   , ATerm.Utilities

  Build-depends:       base < 5
                     , aterm
                     , mtl
                     , transformers
                     , wl-pprint

  hs-source-dirs:      src
  ghc-options: -O2 -Wall
  if impl(ghc >= 7.0.1)
    ghc-prof-options: -rtsopts
  ghc-prof-options: -prof -auto-all -caf-all

source-repository head
  type:     git
  location: git://github.com/GaloisInc/aterm-utils.git
