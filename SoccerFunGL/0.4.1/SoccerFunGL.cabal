    GLUT >= 2.2 && < 2.3,
    OpenGL >= 2.4 && < 2.5,
    SoccerFun >= 0.4 && < 0.5
  Exposed-Modules:
    SoccerFun.UI.GL

Executable sfPlayTape
  Main-Is: SoccerFun/UI/GL/PlayTape.hs


