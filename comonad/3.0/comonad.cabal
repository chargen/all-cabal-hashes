source-repository head
  type: git
  location: git://github.com/ekmett/comonad.git

library
  other-extensions: CPP

  build-depends:
    base         >= 4       && < 4.7,
    transformers >= 0.2     && < 0.4,
    containers   >= 0.3     && < 0.6,
    semigroups   >= 0.8.3   && < 0.8.4

  exposed-modules:
    Control.Comonad

  ghc-options: -Wall


