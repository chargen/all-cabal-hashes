name:          data-lens
category:      Control, Comonads
version:       1.8.0.3
license:       BSD3
cabal-version: >= 1.6
license-file:  LICENSE
author:        Edward A. Kmett
maintainer:    Edward A. Kmett <ekmett@gmail.com>
stability:     provisional
homepage:      http://github.com/ekmett/data-lens/
copyright:     Copyright (C) 2008-2011 Edward A. Kmett
synopsis:      Haskell 98 Lenses
description:   Haskell 98 Lenses
build-type:    Simple

source-repository head
  type: git
  location: git://github.com/ekmett/data-lens.git

flag DeriveDataTypeable
  manual: False
  default: True

library
  build-depends: 
    base >= 4 && < 5,
    comonad >= 1.1 && < 1.2,
    comonad-transformers >= 1.8 && < 1.9,
    containers >= 0.3 && < 0.5,
    contravariant >= 0.1.2 && < 0.2,
    distributive >= 0.2 && < 0.3,
    semigroupoids >= 1.2.2 && < 1.3,
    semigroups >= 0.6 && < 0.7,
    transformers >= 0.2.0 && <= 0.3

  if flag(DeriveDataTypeable)
    extensions: DeriveDataTypeable
    cpp-options: -DLANGUAGE_DeriveDataTypeable

  extensions: CPP

  exposed-modules:
    Data.Lens.Common
    Data.Lens.Lazy
    Data.Lens.Strict

  ghc-options:      -Wall 
