name:                sdl2-image
version:             0.1.1.0
synopsis:            Haskell binding to sdl2-image.
description:         Haskell binding to sdl2-image.
license:             MIT
license-file:        LICENSE
author:              Cai Lei
maintainer:          cailei@live.com
category:            Graphics
build-type:          Simple
cabal-version:       >=1.10

source-repository head
  type:       git
  location:   https://github.com/ccll/hs-sdl2-image.git

source-repository this
  type:       git
  location:   https://github.com/ccll/hs-sdl2-image.git
  tag:        v0.1.1.0

library
  extra-libraries:
    sdl2

  pkgconfig-depends:
    sdl2 >= 2.0.1,
    SDL2_image >= 2.0.0

  build-depends:
    base >= 4.6 && < 4.8,
    sdl2 >= 1.0 && < 1.2

  hs-source-dirs:
    src

  ghc-options:
    -Wall

  default-extensions:
    CPP, EmptyDataDecls, ForeignFunctionInterface,
    MultiParamTypeClasses, FunctionalDependencies

  exposed-modules:
    Graphics.UI.SDL.Image

  other-modules:
    Graphics.UI.SDL.Utilities

  includes:
    SDL.h

  default-language:
    Haskell2010

