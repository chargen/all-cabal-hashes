  exposed-modules:
    Text.PrettyPrint.Mainland

  build-depends:
    base >= 4 && < 5,
    containers >= 0.2 && < 0.5,
    srcloc >= 0.1 && < 0.2,
    symbol >= 0.1 && < 0.4

