build-type:     Simple
cabal-version:  >=1.2

data-files: 
  instances.pl

library
  build-depends: 
    base >= 4 && < 4.2,
    containers >= 0.2.0.0 && < 0.3.0.0
  exposed-modules:
    Data.Set.Unboxed
  ghc-options: -fno-warn-overlapping-patterns -O2 -fdicts-cheap

executable unboxed-set-benchmark
  main-is: Benchmark.hs
  build-depends:
    base >= 4 && < 4.2,
    random >= 1.0.0.1 && < 1.1.0.0,
    containers >= 0.2.0.0 && < 0.3.0.0,
    timeit >= 0.9.0 && < 1.0
  other-modules:
    Data.Set.Unboxed
  ghc-options: -fno-warn-overlapping-patterns -O2 -fdicts-cheap


