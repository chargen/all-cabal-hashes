    text                      >= 1.1.0.1  && < 1.2,
    safe                      >= 0.3      && < 0.4,
    containers                >= 0.5.0.0  && < 0.6,
    hedis                     >= 0.6      && < 0.7,
    pipes                     >= 4.1.1    && < 4.2,
    pipes-bytestring          >= 2        && < 3,
    mtl                       >= 2.1      && < 2.3,
    system-filepath           >= 0.4.12   && < 0.5,
    groom                     >= 0.1.2    && < 0.2,
    time                      >= 1.4      && < 1.5,
    transformers              >= 0.4      && < 0.5

