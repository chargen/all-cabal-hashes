                 , Data.IntSet.Unicode
                 , Data.Map.Unicode
                 , Data.Sequence.Unicode
                 , Data.Set.Unicode

  build-depends: base >= 3.0.3.1 && < 4.3
               , base-unicode-symbols == 0.1.*
               , containers < 0.4
