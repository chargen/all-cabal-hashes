                 , Data.Map.Unicode
                 , Data.Sequence.Unicode
                 , Data.Set.Unicode
  build-depends: base                 >= 3.0.3.1 && < 4.4
               , base-unicode-symbols >= 0.1.1   && < 0.3
               , containers                         < 0.4
