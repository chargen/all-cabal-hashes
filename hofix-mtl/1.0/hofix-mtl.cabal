license-file: LICENSE

cabal-version: >= 1.2

library
  build-depends: base >= 4 && < 5
  build-depends: star-to-star >= 1 && < 2
  build-depends: mtl >= 1.1 && < 1.2
  build-depends: template-haskell >= 2.4 && < 2.5
  exposed-modules: Data.StarToStar.Iso, Data.StarToStar.Iso.TH, Control.Monad.StarToStar.Fix
