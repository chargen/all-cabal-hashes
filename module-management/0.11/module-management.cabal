    Language.Haskell.Modules.Util.Test,
    Language.Haskell.Modules.Util.Temp,
    Language.Haskell.Modules.Util.SrcLoc,
    Language.Haskell.Modules.Util.DryIO
  Other-Modules:
    Language.Haskell.Modules.Internal,
    Tests.Fold,
    Tests.Imports,
    Tests.Merge,
    Tests.Split
  Build-Depends:
    applicative-extras,
    base >= 4 && < 5,
    bytestring,
    Cabal,
    containers,
    data-default,
    directory,
    filepath,
    haskell-src-exts,
    HUnit,
    MonadCatchIO-mtl,
    mtl,
    pretty,
    process,
    pureMD5,
    set-extra >= 1.3.1,
    syb,
    system-fileio,
    temporary

Executable hmm
