Flag cgi
  description:       Compile cgi executable.
  default:           False

Flag test
  description:       Compile test executable.
  default:           False

Library
    Build-depends:       xml, parsec >= 2, containers
    if impl(ghc >= 6.10)
      Build-depends: base >= 4 && < 5, syb
    else
      Build-depends: base >= 3 && < 4
    Exposed-modules:     Text.TeXMath, Text.TeXMath.Parser, Text.TeXMath.MathMLWriter

    if impl(ghc >= 6.12)
      Ghc-Options:     -Wall -fno-warn-unused-do-bind
    else
      Ghc-Options:     -Wall
    Ghc-Prof-Options:  -auto-all -caf-all

Executable testTeXMathML
    Main-is:             testTeXMathML.hs
    if impl(ghc >= 6.12)
      Ghc-Options:     -Wall -fno-warn-unused-do-bind
    else
      Ghc-Options:     -Wall
    Ghc-Prof-Options:  -auto-all -caf-all
    if flag(test)
      Buildable:         True
    else
      Buildable:         False

Executable texmath-cgi
    Main-is:             cgi/texmath-cgi.hs
    if impl(ghc >= 6.12)
      Ghc-Options:     -Wall -fno-warn-unused-do-bind
    else
      Ghc-Options:     -Wall
    Ghc-Prof-Options:  -auto-all -caf-all
    if flag(cgi)
      Buildable:         True
      Build-depends:     cgi, json, utf8-string
    else
      Buildable:         False
