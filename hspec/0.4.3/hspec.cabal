build-depends: HUnit >=1 && <=2, QuickCheck >=2.4.0.1 && <=2.5,
               base >=4 && <=5, silently == 1.1.1,
               ansi-terminal == 0.5.5, transformers >= 0.2.0 && < 0.3.0
stability: experimental
