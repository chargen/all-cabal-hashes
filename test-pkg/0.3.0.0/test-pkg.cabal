name:                test-pkg
version:             0.3.0.0
category:            testing-hackage
synopsis:            Just tests Hackage
description:         Hackage does not have documentation,
                     so we have to test things.
license:             BSD3
license-file:        LICENSE
author:              Iavor S. Diatchki
maintainer:          iavor.diatchki@gmail.com
build-type:          Simple
cabal-version:       >=1.10

flag doCheckWarns
  Description: User -Werror when compiling
  Default:     False

library
  exposed-modules:     Test
  build-depends:       base >=4.6 && <4.7
  default-language:    Haskell2010
  if flag(doCheckWarns)
    GHC-options:       -Werror



