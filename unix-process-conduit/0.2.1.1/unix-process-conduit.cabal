                       Data.Conduit.LogFile
  build-depends:       base              >= 4         && < 5
                     , transformers
                     , bytestring
                     , conduit           >= 0.5       && < 1.1
                     , unix              >= 2.5
                     , process           >= 1.1
                     , time
                     , filepath
                     , stm               >= 2.4
                     , directory
  c-sources:           cbits/process-tracker.c

