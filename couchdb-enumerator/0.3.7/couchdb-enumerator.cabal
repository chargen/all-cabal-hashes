      test/Main.hs,
      test/Database/CouchDB/Enumerator/Test/Generic.hs,
      test/Database/CouchDB/Enumerator/Test/Basic.hs,
      test/Database/CouchDB/Enumerator/Test/View.hs,
      test/Database/CouchDB/Enumerator/Test/Util.hs

