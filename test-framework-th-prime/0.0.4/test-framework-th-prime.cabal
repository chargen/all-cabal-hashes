Library
  if impl(ghc >= 6.12)
    GHC-Options:        -Wall -fno-warn-unused-do-bind
  else
    GHC-Options:        -Wall
  Exposed-Modules:      Test.Framework.TH.Prime
  Other-Modules:        Test.Framework.TH.Prime.Parser
  Build-Depends:        base >= 4 && < 5
                      , cpphs >= 0.2.1
                      , haskell-src-exts
                      , template-haskell
                      , test-framework

