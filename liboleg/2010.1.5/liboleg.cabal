            Language.TEval.EvalN
            Language.TEval.EvalTaglessF
            Language.TEval.EvalTaglessI
            Language.TEval.TEvalNC
            Language.TEval.TEvalNR
            Language.TEval.TInfLetI
            Language.TEval.TInfLetP
            Language.TEval.TInfT
            Language.TEval.TInfTEnv
            Language.TEval.TInfTM
            Language.CB
            Language.CBAny
            Language.CPS
            Language.CB98
            Language.TTF
            Language.TTFdB
            Language.TDPE
            Language.ToTDPE
            Language.Typ
            Language.TypeCheck

            Text.PrintScan
            Text.PrintScanF
            Text.GenPrintF
            Text.TotalPrintF
            Text.TFTest

            System.SysOpen
            System.IterateeM
            System.LowLevelIO
            System.RandomIO
            System.SafeHandles

    ghc-options:
            -funbox-strict-fields 

    c-sources:         cbits/sys_open.c
    include-dirs:      include
