            System.SysOpen
            System.IterateeM
            System.LowLevelIO
            System.RandomIO
            System.SafeHandles

    ghc-options:
            -funbox-strict-fields 

    c-sources:         cbits/sys_open.c
    include-dirs:      include
    includes:          sys_open.h
    install-includes:  sys_open.h
