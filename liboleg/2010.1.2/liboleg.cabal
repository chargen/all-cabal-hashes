            Text.PrintScan
            Text.PrintScanF
            Text.GenPrintF
            Text.TotalPrintF
            Text.TFTest

            System.SysOpen
            System.IterateeM
            System.LowLevelIO
            System.RandomIO
            System.SafeHandles

    ghc-options:
            -funbox-strict-fields 

