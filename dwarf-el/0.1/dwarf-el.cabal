Stability:     unstable
Cabal-Version: >= 1.6
Build-Depends: base
Build-Type:    Simple
Synopsis:      Parser for DWARF debug format.
Description:   Parser for DWARF debug format.

library
    build-depends:   base >= 2 && < 5, transformers >= 0.3,
                     bytestring, containers, binary,
                     utf8-string >= 0.3.7
    hs-source-dirs:  src
    exposed-modules: Data.Dwarf
    other-modules:   Data.Dwarf.Types,
                     Data.Dwarf.Utils,
                     Data.Dwarf.Reader,
                     Data.Dwarf.Form,
                     Data.Dwarf.LNI,
                     Data.Dwarf.AT,
                     Data.Dwarf.ATE,
                     Data.Dwarf.OP,
                     Data.Dwarf.TAG,
                     Data.Dwarf.CFA
    ghc-options:     -Wall

source-repository head
  type: git
  location: https://github.com/Peaker/dwarf.git


