    build-depends: 
        base                     >= 4 && < 5,
        semigroups,
        monadplus,
        unordered-containers,
        bytestring,
        music-score              == 1.6,
        music-pitch-literal      == 1.6,
        music-preludes           == 1.6,
        aeson
    hs-source-dirs: src
    default-language: Haskell2010
    exposed-modules:
        Music.Sibelius
        Music.Score.Import.Sibelius

