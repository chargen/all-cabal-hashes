      build-depends:    base>=4
      cpp-options:      -DBASE4
  else
      build-depends:    base<4

  extensions:   CPP
  include-dirs:         include
  includes:       HsNet.h
  install-includes:
                HsNet.h HsNetworkConfig.h
  c-sources:    cbits/HsNet.c
