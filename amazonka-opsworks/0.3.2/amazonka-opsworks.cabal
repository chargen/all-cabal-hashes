        , Network.AWS.OpsWorks.RegisterElasticIp
        , Network.AWS.OpsWorks.RegisterInstance
        , Network.AWS.OpsWorks.RegisterRdsDbInstance
        , Network.AWS.OpsWorks.RegisterVolume
