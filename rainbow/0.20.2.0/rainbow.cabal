    Rainbow
    Rainbow.Colors
    Rainbow.Types
  default-language: Haskell2010
  ghc-options:
    -Wall
  build-depends:
      base >= 4.5.0.0 && < 4.8.0.0
    , terminfo >= 0.3.2 && < 0.5.0.0
    , text >= 0.11.2.0 && < 1.3.0.0
  hs-source-dirs:
