copyright:           Mark Wotton, 2013
category:            Language
build-type:          Simple

cabal-version:       >=1.10

source-repository head
  type:     git
  location: git://github.com/mwotton/dictparser.git



library
  exposed-modules:     NLP.DictParser,
                       NLP.DictParser.Internal
  -- other-modules:
  -- other-extensions:
  build-depends:       base <5,
                       parsec
  hs-source-dirs:      src

  default-language:    Haskell2010

test-suite tests
  type:            exitcode-stdio-1.0
  ghc-options:     -Wall -threaded
  hs-source-dirs:  test/
  main-is:         Spec.hs
  default-language:    Haskell2010
  build-depends:   base <5,
                   dictparser,
                   hspec,
                   parsec

executable dictparser
  main-is: src/main.hs
  default-language:    Haskell2010
  build-depends:   base,
                   dictparser,
                   parsec

