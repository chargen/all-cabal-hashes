name:                dictparser
version:             0.1.0.0
synopsis:            Parsec parsers for the DICT format produced by dictfmt -t
description:         Parsec parsers for DICT
license:             BSD3
license-file:        LICENSE
author:              Mark Wotton
maintainer:          mwotton@gmail.com
copyright:           Mark Wotton, 2013
category:            Language
build-type:          Simple
-- extra-source-files:
cabal-version:       >=1.10

source-repository head
  type:     github
  location: http://github.com/mwotton/dictparser

library
  exposed-modules:     NLP.DictParser,
                       NLP.DictParser.Internal
  -- other-modules:
  -- other-extensions:
  build-depends:       base <5,
                       parsec
  hs-source-dirs:      src

  default-language:    Haskell2010

test-suite tests
  type:            exitcode-stdio-1.0
  ghc-options:     -Wall -threaded
  hs-source-dirs:  test/
  main-is:         Spec.hs
  default-language:    Haskell2010
  build-depends:   base <5,
                   dictparser,
                   hspec,
                   parsec

executable dictparser
  main-is: src/main.hs
  default-language:    Haskell2010
  build-depends:   base,
                   dictparser,
                   parsec

