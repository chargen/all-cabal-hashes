        gtk,
        mtl,
        network,
        pango,
        process,
        unix,
        webkit,
        xdg-basedir,
        zeromq-haskell
    Exposed-modules:
        Hbro.Core,
        Hbro.Gui,
        Hbro.Hbro,
        Hbro.Keys,
        Hbro.Socket,
        Hbro.Types,
        Hbro.Util
    Ghc-options: -Wall

