        Hbro.Config,
        Hbro.Core,
        Hbro.Extra.Bookmarks,
        Hbro.Extra.BookmarksQueue,
        Hbro.Extra.Clipboard,
        Hbro.Extra.History,
        Hbro.Extra.Misc,
        Hbro.Extra.Prompt,
        Hbro.Extra.Session,
        Hbro.Extra.StatusBar,
        Hbro.Gui,
        Hbro.Socket,
        Hbro.Types,
        Hbro.Util
    Ghc-options: -Wall


Executable hbro
    Build-depends: 
        hbro,
        base == 4.*,
        glib,
        gtk,
        mtl,
        process,
        unix,
        webkit
