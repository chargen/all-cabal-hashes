  location: git://github.com/ekmett/representable-profunctors.git

library
  build-depends: 
    base >= 4 && < 5,
    comonad >= 1.1 && < 1.2,
    profunctors >= 0.1.1 && < 0.2,
    profunctor-extras >= 0.3 && < 0.4,
    transformers >= 0.2.2.0 && < 0.3

  exposed-modules:
    Data.Profunctor.Representable
    Data.Profunctor.Corepresentable

  ghc-options:      -Wall 


