                     , hxt                >= 9.3 && < 9.4
                     , HandsomeSoup       >= 0.3 && < 0.4
                     , lens               >= 3.10 && < 4.6
                     , mtl                >= 2 && < 3
                     , snap               >= 0.13 && < 0.14
                     , snap-core          >= 0.9 && < 0.10
                     , text               >= 0.11 && < 1.3
                     , transformers       >= 0.3 && < 0.5


Test-Suite test-hspec-snap
    type:       exitcode-stdio-1.0
    hs-source-dirs: spec
    main-is: Main.hs
    build-depends:     base               >= 4.6 && < 4.8
                     , aeson              >= 0.6 && < 1
                     , bytestring         >= 0.9 && < 0.11
                     , containers         >= 0.4 && < 0.6
                     , digestive-functors >= 0.7 && < 0.8
                     , hspec              >= 2.0 && < 2.2
                     , hspec-core         >= 2.0 && < 2.2
                     , hxt                >= 9.3 && < 9.4
