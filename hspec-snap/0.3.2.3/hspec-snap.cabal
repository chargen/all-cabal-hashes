                     , lens               >= 3.10 && < 4.5
                     , mtl                >= 2 && < 3
                     , snap               >= 0.13 && < 0.14
                     , snap-core          >= 0.9 && < 0.10
                     , snap-extras        > 0.4 && < 1
                     , text               >= 0.11 && < 1.3
                     , transformers       >= 0.3 && < 0.5
                     , directory          >= 1.2 && < 1.3
