    Build-Depends: base <4.7
    if flag(base4)
        Build-Depends: base >=4
        CPP-Options: -DBASE4
    else
        Build-Depends: base <4
    if impl(ghc >= 6.10)
        Build-Depends: base >=4
    Exposed-Modules:
        Test.HUnit.Base,
        Test.HUnit.Lang,
        Test.HUnit.Terminal,
        Test.HUnit.Text,
