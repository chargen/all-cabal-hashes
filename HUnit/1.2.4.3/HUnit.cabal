    tests/HUnitTestOptimize.hs
    tests/TerminalTest.lhs
Data-Files:
    doc/Guide.html
    examples/Example.hs
    prologue.txt
    README

source-repository head
    type:     darcs
    location: http://code.haskell.org/HUnit/

flag base4

Library
    Build-Depends: 
        base < 4.7,
        deepseq
    if flag(base4)
        Build-Depends: base >= 4
        CPP-Options: -DBASE4
        GHC-Options: -Wall
    else
        Build-Depends: base < 4
    if impl(ghc >= 6.10)
        Build-Depends: base >= 4
    Exposed-Modules:
        Test.HUnit.Base,
        Test.HUnit.Lang,
        Test.HUnit.Terminal,
        Test.HUnit.Text,
        Test.HUnit
    Extensions: CPP

