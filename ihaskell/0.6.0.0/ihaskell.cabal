                   IHaskell.Eval.Evaluate
                   IHaskell.Eval.Info
                   IHaskell.Eval.Lint
                   IHaskell.Eval.Parser
                   IHaskell.Eval.Hoogle
                   IHaskell.Eval.ParseShell
                   IHaskell.Eval.Util
                   IHaskell.IPython
                   IHaskell.Flags
                   IHaskell.Types
                   IHaskell.BrokenPackages
                   Paths_ihaskell
