                       vector >=0.10,

                       casadi-bindings-internal

  default-language:    Haskell2010


  extra-libraries:  stdc++

  pkgconfig-depends: casadi_core

  ghc-prof-options: -prof -fprof-auto -fprof-cafs -rtsopts
