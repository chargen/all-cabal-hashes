        pipes-network >= 0.6.4 && < 0.7,
        network-simple >= 0.4.0.2 && <= 0.5,
        aeson >=0.7 && < 0.9,
        text >= 0.11.2 && < 1.2.1,
        conceit >= 0.2.1 && < 0.3,
        network >= 2.4

