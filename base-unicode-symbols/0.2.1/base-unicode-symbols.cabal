  location: http://code.haskell.org/~roelvandijk/code/base-unicode-symbols

library
  exposed-modules: Control.Applicative.Unicode
                 , Control.Arrow.Unicode
                 , Control.Category.Unicode
                 , Control.Monad.Unicode
                 , Data.Bool.Unicode
