                     , classy-prelude        >= 0.11 && < 0.12
                     , containers            >= 0.5  && < 0.6
                     , monad-control         >= 1.0  && < 1.1
                     , persistent            >= 2.1  && < 2.2
                     , random                >= 1.1  && < 1.2
                     , stm                   >= 2.4  && < 2.5
                     , uuid                  >= 1.3  && < 1.4
                     , yesod-core            >= 1.4  && < 1.5
                     , yesod-form            >= 1.4  && < 1.5
