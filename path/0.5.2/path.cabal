name:                path
version:             0.5.2
synopsis:            Path
description:         Path
license:             BSD3
license-file:        LICENSE
author:              Chris Done
maintainer:          chrisdone@fpcomplete.com
copyright:           2015 FP Complete
category:            Filesystem
build-type:          Simple
cabal-version:       >=1.8
extra-source-files:  README.md, CHANGELOG

library
  hs-source-dirs:    src/
  ghc-options:       -Wall -O2
  exposed-modules:   Path, Path.Internal
  build-depends:     base >= 4 && <5
                   , exceptions
                   , filepath
                   , template-haskell

