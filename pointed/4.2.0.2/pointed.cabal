    comonad              >= 4       && < 5,
    data-default-class   >= 0.0.1   && < 0.1,
    hashable             >= 1.1     && < 1.3,
    kan-extensions       >= 4.2     && < 5,
    semigroups           >= 0.8.3.1 && < 1,
    semigroupoids        >= 4       && < 6,
    stm                  >= 2.1.2.1 && < 2.5,
    tagged               >= 0.5     && < 1,
    unordered-containers >= 0.2     && < 0.3

  exposed-modules:
    Data.Pointed
    Data.Copointed

  ghc-options: -Wall
  hs-source-dirs: src
