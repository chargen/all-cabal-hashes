    semigroups           >= 0.8.3.1 && < 1,
    semigroupoids        >= 4       && < 6,
    stm                  >= 2.1.2.1 && < 2.5,
    tagged               >= 0.5     && < 1,
    unordered-containers >= 0.2     && < 0.3

  exposed-modules:
    Data.Pointed
    Data.Copointed

  ghc-options: -Wall
  hs-source-dirs: src
