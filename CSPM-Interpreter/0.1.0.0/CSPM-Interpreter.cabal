    ,array >= 0.3 && <0.4
    ,syb >= 0.1 && < 0.2
 
  GHC-Options: -funbox-strict-fields -O2 -Wall
  Extensions: DeriveDataTypeable, GeneralizedNewtypeDeriving
  Hs-Source-Dirs:       src
