    Build-Depends:   base >= 4 && < 5
                   , array
                   , blaze-builder             >= 0.3.3    && < 0.4
                   , blaze-builder-conduit     >= 0.5
                   , bytestring                >= 0.9.1.4
                   , case-insensitive          >= 0.2
                   , conduit                   >= 0.5
                   , ghc-prim
                   , HTTP
                   , http-types                >= 0.7
                   , lifted-base               >= 0.1
                   , network-conduit
                   , simple-sendfile           >= 0.2.4    && < 0.3
                   , transformers              >= 0.2.2    && < 0.4
                   , unix-compat               >= 0.2
                   , void
                   , wai                       >= 2.0      && < 2.1
                   , network
                   , HUnit
                   , QuickCheck
                   , hspec                     >= 1.3
                   , time
                   , old-locale

  if (os(linux) || os(freebsd) || os(darwin)) && flag(allow-sendfilefd)
