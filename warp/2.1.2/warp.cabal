                   , array
                   , blaze-builder             >= 0.3.3  && < 0.4
                   , blaze-builder-conduit     >= 0.5      && < 1.1
                   , bytestring                >= 0.9.1.4
                   , case-insensitive          >= 0.2
                   , conduit                   >= 0.5      && < 1.1
