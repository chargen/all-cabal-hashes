name:                tamper
version:             0.3.1.0
synopsis:            An HTML templating system similar to Blaze, implemented as a monad transformer of sorts
-- description:         
license:             BSD3
license-file:        LICENSE
author:              Tobias Dammers
maintainer:          tdammers@gmail.com
-- copyright:           
category:            Text
build-type:          Simple
-- extra-source-files:  
cabal-version:       >=1.10

library
  exposed-modules: Text.Tamper
                 , Text.Tamper.DOM
                 , Text.Tamper.DOM.AttribList
                 , Text.Tamper.Render
                 , Text.Tamper.Render.Html
                 , Text.Tamper.Tags
                 , Text.Tamper.Tags.Html
                 , Text.Tamper.Tags.Html.Attributes
  other-modules: Text.Tamper.Internal
  -- other-extensions:    
  build-depends: base >=4.5 && <4.7
               , safe
               , containers >= 0.4 && < 1.0
               , mtl >= 2.0 && < 3.0
               , text >= 0.11
  -- hs-source-dirs:      
  default-language:    Haskell2010


