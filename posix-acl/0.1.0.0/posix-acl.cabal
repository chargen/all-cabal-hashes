--                        bytestring == 0.10.*,
                        containers == 0.5.*
  Ghc-Options:          -Wall
  Exposed-Modules:      System.Posix.ACL
--                        System.Posix.ACL.Internals
  Other-Modules:        System.Posix.ACL.Acl_h System.Posix.ACL.Internals
  Extra-Libraries:      acl


