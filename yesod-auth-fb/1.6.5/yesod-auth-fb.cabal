                 , text         >= 0.7     && < 1.3
                 , transformers >= 0.1.3   && < 0.5
                 , yesod-fb     == 0.3.*
                 , fb           >= 0.14    && < 1.1
                 , conduit      >= 1.0     && < 1.3
                 , bytestring   >= 0.9     && < 0.11
                 , aeson        >= 0.6
                 , time         >= 1.0     && < 1.6

  Exposed-modules: Yesod.Auth.Facebook
