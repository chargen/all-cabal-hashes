  Build-depends:   base         >= 4.3     && < 5
                 , lifted-base  >= 0.1     && < 0.3
                 , yesod-core   >= 1.2     && < 1.5
                 , yesod-auth   >= 1.3     && < 1.5
                 , hamlet
