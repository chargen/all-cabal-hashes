description:         Parsing & Quasiquoting for RFC 6570 URI Templates
homepage:            http://github.com/sanetracker/uri-templater
license:             MIT
license-file:        LICENSE
author:              Ian Duncan
maintainer:          ian@iankduncan.com
copyright:           Ian Duncan
category:            Network
build-type:          Simple
cabal-version:       >=1.8
source-repository    head
  type:                git
  location:            http://github.com/sanetracker/uri-templater

library
  exposed-modules:   Network.URI.Template,
                     Network.URI.Template.TH,
                     Network.URI.Template.Types,
                     Network.URI.Template.Parser,
                     Network.URI.Template.Internal
  build-depends:     base <5,
                     trifecta,
                     charset,
                     parsers,
                     template-haskell,
                     HTTP,
                     dlist,
                     mtl,
                     ansi-wl-pprint,
                     vector,
                     containers,
                     unordered-containers,
                     text
  hs-source-dirs:    src

test-suite test-uri-templates
   type:             exitcode-stdio-1.0
   main-is:          Test.hs
   hs-source-dirs:   test
   build-depends:    base,
                     mtl,
                     uri-templater,
                     template-haskell,
                     HUnit,
                     ansi-wl-pprint


