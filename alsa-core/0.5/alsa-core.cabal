    extensible-exceptions >=0.1.1 && <0.2,
    base >= 3 && <5

  Hs-Source-Dirs: src

  Exposed-Modules:
    Sound.ALSA.Exception

  GHC-Options: -Wall
  Includes: alsa/asoundlib.h
  Extra-libraries: asound
