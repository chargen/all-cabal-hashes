name:                  clac
version:               0.5.0
synopsis:              Simple CLI RPN calculator
description:           Simple CLI RPN calculator.
license:               GPL-3
license-file:          LICENSE
author:                Alexander Berntsen
maintainer:            alexander@plaimi.net
copyright:             Alexander Berntsen 2015
category:              Math
build-type:            Simple
cabal-version:         >=1.10

executable clac
  main-is:             clac.hs
  other-modules:       Clac.CliParser
                       Clac.IO
                       Clac.REPL
                       Clac.Stack
  other-extensions:    GADTs
  build-depends:       base                 >=4.7  && <4.8,
                       containers           >=0.5  && <0.6,
                       dsp                  >=0.2  && <0.3,
                       optparse-applicative >=0.11 && <0.12,
                       plailude             >=0.6  && <0.7,
                       pretty-tree          >=0.1  && <0.2,
                       safe                 >=0.3  && <0.4,
                       split                >=0.2  && <0.3
  hs-source-dirs:      src src-exec
  default-language:    Haskell2010


