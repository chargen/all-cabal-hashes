category:            System
build-type:          Simple
cabal-version:       >=1.8

library
  exposed-modules:     Data.ByteString.Handle
  other-modules:
                       Data.ByteString.Handle.Read
                       Data.ByteString.Handle.Write
  build-depends:       bytestring >= 0.9.1.5 && < 0.11,
                       base >= 4.2 && < 4.7
  hs-source-dirs:      src

test-suite test
