  hs-source-dirs: test
  main-is: Test.hs

  build-depends:
                  QuickCheck >= 2.1.2 && < 2.8,
                  test-framework-quickcheck2 >= 0.2.5 && < 0.4,
                  HUnit >= 1.2 && < 1.3,
                  test-framework-hunit >= 0.2.0 && < 0.4,
                  test-framework >= 0.2.0 && < 0.9,
                  bytestring >= 0.9.1.5 && < 0.11,
                  base >= 4.2 && < 4.8,
                  bytestring-handle

