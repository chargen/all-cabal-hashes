Library
  Build-Depends:
    base >= 3 && < 5,
    haskell98,
    bytestring,
    mtl >= 1.1.0.0,
    text >= 0.5,
    utf8-string >= 0.3.3,
    parallel,
    containers,
    extensible-exceptions >= 0.1 && < 0.2
  Exposed-Modules:
    Text.XML.Expat.Annotated,
    Text.XML.Expat.Cursor,
    Text.XML.Expat.Format,
    Text.XML.Expat.IO,
    Text.XML.Expat.Namespaced,
    Text.XML.Expat.Proc,
    Text.XML.Expat.Qualified,
    Text.XML.Expat.SAX,
    Text.XML.Expat.Tree
  Extra-Libraries: expat
  --ghc-options: -Wall


