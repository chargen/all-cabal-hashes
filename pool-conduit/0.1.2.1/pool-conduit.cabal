    build-depends:   base                     >= 4       && < 5
                   , resource-pool            >= 0.2.1   && < 0.3
                   , transformers             >= 0.2.1
                   , resourcet                >= 0.3     && < 0.5
                   , monad-control

    exposed-modules: Data.Conduit.Pool

    ghc-options:     -Wall

