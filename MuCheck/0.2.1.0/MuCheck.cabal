                    Test.MuCheck.MuOp,
                    Test.MuCheck.Config,
                    Test.MuCheck.Interpreter,
                    Test.MuCheck.Mutation,
                    Test.MuCheck.Operators,
                    Test.MuCheck.Utils.Syb,
                    Test.MuCheck.Utils.Common,
                    Test.MuCheck.Utils.Print,
                    Test.MuCheck.TestAdapter
  ghc-options:     -Wall
  -- other-modules:       
