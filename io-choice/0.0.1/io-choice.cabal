Cabal-Version:          >= 1.6
Build-Type:             Simple

Library
  if impl(ghc >= 6.12)
    GHC-Options:        -Wall -fno-warn-unused-do-bind
  else
    GHC-Options:        -Wall
  Exposed-Modules:      Control.Exception.IOChoice
                        Control.Exception.IOChoice.Lifted
  Build-Depends:        base >= 4 && < 5
                      , lifted-base
                      , monad-control
                      , transformers
                      , transformers-base

Source-Repository head
