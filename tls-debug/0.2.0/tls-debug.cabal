                   , cmdargs
                   , certificate >= 1.0
                   , cprng-aes >= 0.2.3
                   , tls >= 1.0 && < 1.1
                   , tls-extra >= 0.5 && < 0.6
  Buildable:         True
  ghc-options:       -Wall -fno-warn-missing-signatures

