                   monad-loops >=0.3.0,         monad-loops < 0.4,
                   hint >=0.3.1,                hint < 0.4,
                   eprocess >=1.1.0,            eprocess < 2
    exposed-modules: Language.Haskell.Interpreter.Server
    hs-source-dirs: src
    