  Exposed-Modules:
    Snap.Snaplet.AMQP

  Other-Modules:
    Paths_snaplet_amqp

  Build-Depends:
    base                >= 4.4     && < 5,
    snap                >= 0.13    && < 0.14,
    amqp                >= 0.10    && < 0.11,
    transformers        >= 0.4     && < 0.5,
    configurator        >= 0.3     && < 0.4,
    network             >= 2.5     && < 2.7,
    mtl                 >= 2       && < 3

