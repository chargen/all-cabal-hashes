  Exposed-Modules:
    Snap.Snaplet.AMQP

  Other-Modules:
    Paths_snaplet_amqp

  Build-Depends:
    base                >= 4.4     && < 5,
    snap                >= 0.13    && < 0.14,
    resource-pool       >= 0.2     && < 0.3,
