  type: git
  location: http://github.com/dmbarbour/haskell-vcache.git

Library
  hs-Source-Dirs: hsrc_lib
  default-language: Haskell2010
  Build-Depends: base (>= 4.6 && < 5)
    , bytestring
    , bytestring-builder
    , array
    , vcache (>= 0.2.3)

  Exposed-Modules:
