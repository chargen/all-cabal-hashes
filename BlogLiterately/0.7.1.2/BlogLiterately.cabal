                   blaze-html >= 0.5 && < 0.8,
                   cmdargs >= 0.9.5 && < 0.11,
                   haxr >= 3000.9 && < 3000.11,
                   pandoc >= 1.12 && < 1.13,
                   pandoc-types >= 1.12 && < 1.13,
                   pandoc-citeproc >= 0.1.2 && < 0.4,
                   highlighting-kate >= 0.5 && < 0.6,
