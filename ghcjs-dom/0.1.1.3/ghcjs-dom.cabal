    -- Make sure ghcjs gets turned off if ghcjs-base is not avaialbe
    if flag(ghcjs)
        build-depends: ghcjs-base

--    if impl(ghcjs) && flag(jsffi)
    if flag(ghcjs) && flag(jsffi)
        cpp-options: -DUSE_JAVASCRIPTFFI
        build-depends: ghcjs-base

--    if !impl(ghcjs) || flag(webkit)
    if !flag(ghcjs) || flag(webkit)
        cpp-options: -DUSE_WEBKIT
        build-depends: glib >=0.13.0.0 && <0.14,
                       transformers >=0.3.0.0 && <0.5
        if flag(gtk3)
            build-depends: gtk3 >=0.13.0.0 && <0.14, webkitgtk3 >=0.13.0.0 && <0.14
        else
            build-depends: gtk >=0.13.0.0 && <0.14, webkit >=0.13.0.0 && <0.14
        c-sources:      cbits/ghcjs_stubs.c

    hs-source-dirs: src
    exposed: True
    buildable: True

