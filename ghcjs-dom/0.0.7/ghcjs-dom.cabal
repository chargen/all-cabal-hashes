--    if impl(ghcjs) && flag(jsffi)
    if flag(ghcjs) && flag(jsffi)
        cpp-options: -DUSE_JAVASCRIPTFFI
        build-depends: ghcjs-base

--    if !impl(ghcjs) || flag(webkit)
    if !flag(ghcjs) || flag(webkit)
        cpp-options: -DUSE_WEBKIT
        build-depends:  glib >=0.12.3.1 && <0.13, transformers >=0.3.0.0 && <0.4
        if flag(gtk3)
            build-depends: gtk3 >=0.12.4 && <0.13, webkitgtk3 >=0.12.6.0 && <0.13
        else
            build-depends: gtk >=0.12.4 && <0.13, webkit >=0.12.6.0 && <0.13
        c-sources:      cbits/ghcjs_stubs.c

    hs-source-dirs: src
    exposed: True
    buildable: True

