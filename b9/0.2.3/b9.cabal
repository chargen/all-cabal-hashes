                   , B9.Content.ErlTerms
                   , B9.Content.ErlangPropList
                   , B9.Content.YamlObject
                   , B9.Content.AST
                   , B9.Content.Generator
                   , B9.Content.StringTemplate
                   , B9.QCUtil
  other-modules:   Paths_b9
  -- other-extensions:
  build-depends:     ConfigFile >= 1.1.3 && <1.2
                   , QuickCheck
                   , aeson
                   , async >=2.0.1 && <2.1
                   , base >=4.6 && <4.8
                   , binary >=0.7 && <0.8
                   , bytestring >=0.10 && <0.11
                   , conduit >=1.2 && <1.3
                   , conduit-extra >=1.1 && <1.2
                   , directory >=1.2 && <1.3
                   , filepath >=1.3 && <1.4
                   , mtl >=2.1 && <2.2
                   , old-locale >=1.0 && <1.1
                   , parsec >= 3.1.8
                   , pretty-show
                   , pretty
                   , process >=1.2 && <1.3
                   , random >=1.0 && <1.2
                   , semigroups
