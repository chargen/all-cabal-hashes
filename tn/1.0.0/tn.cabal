                     , bytestring ==0.10.*
                     , containers ==0.5.*
                     , directory ==1.2.*
                     , process ==1.2.*
                     , safe ==0.3.*
                     , text
                     , time
                     , yaml
  hs-source-dirs:      src
  default-language:    Haskell2010
  ghc-options:         -Wall

