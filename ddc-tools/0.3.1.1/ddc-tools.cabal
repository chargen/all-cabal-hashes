Executable ddci-core
    hs-source-dirs:
        src/ddci-core

    Build-depends:
        base            == 4.6.*,
        containers      == 0.5.*,
        process         == 1.1.*,
        filepath        == 1.3.*,
        haskeline       == 0.7.*,
        haskell-src-exts== 1.*,
        directory       == 1.2.*,
        mtl             == 2.1.*,
        ddc-base        == 0.3.1.*,
        ddc-core        == 0.3.1.*,
        ddc-core-eval   == 0.3.1.*,
        ddc-core-simpl  == 0.3.1.*,
        ddc-core-salt   == 0.3.1.*,
        ddc-core-llvm   == 0.3.1.*,
        ddc-build       == 0.3.1.*,
        ddc-driver      == 0.3.1.*

    Main-is:
        Main.hs
  
    Other-modules:
        DDCI.Core.Command.Eval
        DDCI.Core.Command.Help
        DDCI.Core.Command.Set
        DDCI.Core.Command.Trans
        DDCI.Core.Command.TransInteract
