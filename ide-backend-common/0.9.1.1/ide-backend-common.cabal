                      attoparsec           >= 0.10    && < 0.14,
                      template-haskell,
                      pretty-show          >= 1.3     && < 1.7

  default-language:   Haskell2010
  default-extensions: MonoLocalBinds
                      BangPatterns
                      RecordWildCards
                      NamedFieldPuns
                      RankNTypes
