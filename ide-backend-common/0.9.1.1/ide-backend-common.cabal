                      IdeSession.Strict.IORef
                      IdeSession.Strict.StateT
                      IdeSession.Types.Public
                      IdeSession.Types.Private
                      IdeSession.Types.Translation
                      IdeSession.Types.Progress

  build-depends:      base                 >= 4.6     && < 5,
                      filepath             >= 1.3     && < 1.5,
                      directory            >= 1.1     && < 1.3,
                      containers           >= 0.4.2   && < 1,
                      bytestring           >= 0.9.2   && < 1,
