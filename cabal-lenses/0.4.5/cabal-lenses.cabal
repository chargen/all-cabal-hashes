        CabalLenses.Package
        CabalLenses.Version
        CabalLenses.CondVars
        CabalLenses.Section
        CabalLenses.Traversals.BuildInfo
        CabalLenses.Traversals.Dependency
    exposed: True
    buildable: True
    cpp-options: -DCABAL
    hs-source-dirs: lib
    other-modules:
        CabalLenses.Traversals.Internal
        Paths_cabal_lenses
    ghc-options: -W
 
 
