source-repository head
  type:      darcs
  location:  http://hub.darcs.net/emax/tree-view

library
  exposed-modules:
    Data.Tree.View

  hs-source-dirs:
    src

  build-depends:
    base < 5,
    containers,
    mtl

  default-language: Haskell2010


