    t/files/a/c.h
    t/files/a/a.h
    t/files/b/b.h
    t/files/CPP2.hs
    t/files/garrs.hs
    t/files/Simple.hs
    t/files/CPP.hs
    t/files/a.c

Executable hscope
    Main-Is: HScope.hs
    GHC-Options: -Wall
    Build-Depends: base (< 5), hs-cdb, haskell-src-exts, mtl, uniplate, cereal
                             , vector, bytestring, process, directory
    Default-Language: Haskell2010

