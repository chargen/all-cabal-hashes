    t/files/CPP3.hs
    t/files/Simple.hs
    t/files/Setup.lhs
    t/files/Arrow.hs
    t/files/Fixity.hs
    t/files/a/a.h
    t/files/a/c.h
    t/files/CPP2.hs
    t/files/b/b.h
    t/files/a.c
    t/files/garrs.hs
    t/files/CPP.hs
    README.md

Executable hscope
    Main-Is: HScope.hs
    GHC-Options: -Wall
    -- uniplate >= 1.6.11 to avoid 10x performance regression
    -- uniplate 1.6.6 is what Ubuntu 12.04 has in its packages. To be removed eventually...
