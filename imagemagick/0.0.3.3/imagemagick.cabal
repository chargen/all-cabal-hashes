  build-depends:       base >= 4.0 && <5.0
                      , resourcet >= 0.3 && <0.5
                      , transformers == 0.3.*
                      , vector >= 0.9 && <0.11
                      , bytestring >= 0.9 && <0.11
                      , text >= 0.11 && <1.3
                      , system-filepath == 0.4.*
                      , MonadCatchIO-transformers == 0.3.*
  extensions:         EmptyDataDecls
  Ghc-options:        -Wall
  if os(darwin)
        cpp-options:  -DCABAL_OS_DARWIN
  if os(windows)
        cpp-options:  -DCABAL_OS_WINDOWS
  pkgconfig-depends:  ImageMagick, MagickWand
  Build-tools:        hsc2hs



