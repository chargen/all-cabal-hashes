library
  build-depends:
    base >=4 && <5,
    vector,
    statistics,
    tuple

  exposed-modules:
    Statistics.ConfusionMatrix
    Statistics.ConfusionMatrix.Instances
    Statistics.EM.GMM
    Statistics.EM.TwoGaussian
    Statistics.PerformanceMetrics
    TestData.Elements

  ghc-options:
      -O2
