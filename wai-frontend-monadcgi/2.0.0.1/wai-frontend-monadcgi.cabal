                 , cgi
                 , conduit           >= 0.5      && < 1.1
                 , http-types
                 , transformers
                 , case-insensitive
                 , wai               >= 2.0      && < 2.2
    exposed-modules: Network.Wai.Frontend.MonadCGI
    ghc-options:     -Wall

