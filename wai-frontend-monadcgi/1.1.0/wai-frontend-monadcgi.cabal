                 , cgi
                 , conduit           >= 0.2
                 , http-types
                 , transformers
                 , case-insensitive
                 , wai               >= 1.1      && < 1.2
    exposed-modules: Network.Wai.Frontend.MonadCGI
    ghc-options:     -Wall

