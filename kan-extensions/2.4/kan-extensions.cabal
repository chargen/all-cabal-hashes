name:          kan-extensions
category:      Data Structures, Monads, Comonads, Functors
version:       2.4
license:       BSD3
cabal-version: >= 1.6
license-file:  LICENSE
author:        Edward A. Kmett
maintainer:    Edward A. Kmett <ekmett@gmail.com>
stability:     provisional
homepage:      http://github.com/ekmett/kan-extensions/
copyright:     Copyright (C) 2011 Edward A. Kmett
synopsis:      Kan extensions, the Yoneda lemma, and (co)density (co)monads
description:   Kan extensions, the Yoneda lemma, and (co)density (co)monads
build-type:    Simple

source-repository head
  type: git
  location: git://github.com/ekmett/kan-extensions.git

library
  other-extensions:
    CPP
    MultiParamTypeClasses
    GADTs
    Rank2Types,
    FlexibleInstances
    FlexibleContexts
    UndecidableInstances
    TypeFamilies

  build-depends:
    array                  >= 0.3.0.2 && < 0.5,
    base                   >= 4       && < 5,
    comonad                >= 1.1.1.3 && < 1.2,
    containers             >= 0.4     && < 0.5,
    transformers           >= 0.2     && < 0.4,
    mtl                    >= 2.0.1   && < 2.2,
    semigroupoids          >= 1.3.1   && < 1.4,
    contravariant          >= 0.2.0.1 && < 0.3,
    distributive           >= 0.2.2   && < 0.3,
    comonad-transformers   >= 2.1     && < 2.2,
    comonads-fd            >= 2.1     && < 2.2,
    keys                   >= 2.1.3   && < 2.2,
    free                   >= 2.1     && < 2.2,
    adjunctions            >= 2.4     && < 2.5,
    representable-functors >= 2.4     && < 2.5,
    speculation            >= 1.4.1   && < 1.5

  exposed-modules:
    Control.Comonad.Density
    Control.Monad.Co
    Control.Monad.Codensity
    Control.Monad.Free.Church
    Data.Functor.KanExtension
    Data.Functor.Yoneda
    Data.Functor.Yoneda.Contravariant

  ghc-options: -Wall

