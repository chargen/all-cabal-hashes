name:               hist-pl-fusion
version:            0.5.1
synopsis:           Merging historical dictionary with PoliMorf
description:
    The library provides functions for merging historical dictionary
    with PoliMorf morphological dictionary.
license:            BSD3
license-file:       LICENSE
cabal-version:      >= 1.6
copyright:          Copyright (c) 2013 IPI PAN
author:             Jakub Waszczuk
maintainer:         waszczuk.kuba@gmail.com
stability:          experimental
category:           Natural Language Processing
homepage:           https://github.com/kawu/hist-pl/tree/master/fusion
build-type:         Simple

library
    hs-source-dirs:   src
    build-depends:
        base >= 4 && < 5
      , containers
      , text
      , binary
      , text-binary
      , dawg >= 0.9 && < 0.10
      , polimorf >= 0.7.1 && < 0.8
      , hist-pl-dawg >= 0.1 && < 0.2
      , hist-pl-lexicon >= 0.5 && < 0.6

    exposed-modules:
        NLP.HistPL.Fusion

    ghc-options: -Wall

source-repository head
    type: git
    location: https://github.com/kawu/hist-pl.git
