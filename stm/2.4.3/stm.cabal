    type:     git
    location: http://git.haskell.org/packages/stm.git
    tag:      stm-2.4.3-release

library
    default-language: Haskell98
    other-extensions:
        CPP
        DeriveDataTypeable
        FlexibleInstances
        MagicHash
        MultiParamTypeClasses
        UnboxedTuples
    if impl(ghc >= 7.2)
        other-extensions: Trustworthy

    build-depends:
        base  >= 4.2 && < 4.8,
        array >= 0.3 && < 0.6

    exposed-modules:
