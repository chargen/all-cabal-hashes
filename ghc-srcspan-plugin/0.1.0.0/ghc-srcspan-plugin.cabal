author:              Eric Seidel
maintainer:          eseidel@galois.com
copyright:           Copyright (c) 2014, Galois Inc.
category:            Language
build-type:          Simple
cabal-version:       >=1.10

source-repository    head
  type:     git
  location: https://github.com/GaloisInc/ghc-srcspan-plugin

library
  exposed-modules:     GHC.Plugins.SrcSpan
  build-depends:       base >= 4.7 && < 5,
                       ghc >= 7.8,
                       array,
                       containers,
                       hpc
  hs-source-dirs:      src
  default-language:    Haskell2010
