                   , hamlet                  >= 1.1       && < 1.2
                   , shakespeare-css         >= 1.0       && < 1.1
                   , yesod-json              >= 1.1       && < 1.2
                   , containers
                   , unordered-containers
                   , yesod-form              >= 1.1       && < 1.2
                   , transformers            >= 0.2.2     && < 0.4
                   , persistent              >= 1.0       && < 1.1
                   , persistent-template     >= 1.0       && < 1.1
                   , SHA                     >= 1.4.1.3   && < 1.6
                   , http-conduit            >= 1.5       && < 1.7
                   , aeson                   >= 0.5
