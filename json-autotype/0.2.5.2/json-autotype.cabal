                       containers           >=0.3  && <0.6,
                       filepath             >=1.3  && <1.4,
                       hashable             >=1.2  && <1.3,
                       hflags               >=0.4  && <0.5,
                       lens                 >=4.1  && <4.5,
                       mtl                  >=2.1  && <2.2,
                       pretty               >=1.1  && <1.3,
                       process              >=1.2  && <1.4,
                       scientific           >=0.3  && <0.5,
                       text                 >=1.1  && <1.3,
                       uniplate             >=1.6  && <1.7,
                       unordered-containers >=0.2  && <0.3,
                       vector               >=0.9  && <0.11
