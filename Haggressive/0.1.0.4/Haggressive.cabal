name:                 Haggressive
version:              0.1.0.4
synopsis:             Aggression analysis for Tweets on Twitter
description:          Aggression analysis for Tweets on Twitter
homepage:             https://github.com/Pold87/Haggressive
license:              GPL-2
license-file:         LICENSE
author:               Volker Strobel
maintainer:           volker.strobel87@gmail.com
category:             Web
build-type:           Simple

cabal-version:        >=1.10

library
  build-depends:        base >= 4 && < 5,
                        Haggressive,
                        Cabal,
                        HUnit,
                        vector,
                        containers,
                        Cabal,
                        HUnit,
                        text,
                        cassava,
                        tuple,
                        bytestring, 
                        directory,
                        tokenize,
                        PSQueue
  hs-source-dirs:       src-lib
  default-language:     Haskell2010
  exposed-modules: Data.Haggressive.Hag, 
                   Data.Haggressive.Helpers,
                   Data.Haggressive.Preprocess,
                   Data.Haggressive.Tweets	
  ghc-options: -fllvm

test-suite tests
  type:                 detailed-0.9
  test-module:          My
  hs-source-dirs:       testsuite/tests
  build-depends:        base
                      , Haggressive
                      , HUnit
                      , containers
                      , vector
                      , Cabal
                      , tuple
  default-language:     Haskell2010

