  type:                exitcode-stdio-1.0
  hs-source-dirs:      Tests
  Main-is:             Tests.hs
  Build-Depends:       base == 4.*
                     , bytestring
                     , mtl
                     , tasty
                     , tasty-quickcheck
                     , tasty-hunit
                     , bspack
  ghc-options:         -Wall -fno-warn-orphans -fno-warn-missing-signatures
