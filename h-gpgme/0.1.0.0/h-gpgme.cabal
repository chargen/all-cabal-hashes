Tested-With:         GHC==7.8.2
Category:            Cryptography
Build-Type:          Simple
Cabal-Version:       >=1.10

source-repository head
  type:     git
  location: https://github.com/rethab/h-gpgme

library
  hs-source-dirs:      src
  ghc-options:         -Wall
                       -O2
                       -fno-warn-orphans
  exposed-modules:     Crypto.Gpgme
