                     , Crypto.Gpgme.Ctx
                     , Crypto.Gpgme.Crypto
                     , Crypto.Gpgme.Internal
                     , Crypto.Gpgme.Types
  build-depends:       base           == 4.*
                     , bindings-gpgme >= 0.1 && <0.2
                     , bytestring     >= 0.9
                     , unix           >= 2.5
  default-language:    Haskell2010

