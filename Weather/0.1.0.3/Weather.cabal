  exposed-modules:     Net.Weather
  -- other-modules:
  -- other-extensions:
  build-depends: base == 4.*
               , HTTP >= 4000.2.12
               , text >= 1.1.0
               , unordered-containers >= 0.2.5
               , aeson >= 0.8.0
               , bytestring >= 0.10.4
  hs-source-dirs:      src
  default-language:    Haskell2010


