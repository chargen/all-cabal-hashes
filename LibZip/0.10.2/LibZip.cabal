  , runTests.hs, Tests/Common.hs
  , Tests/MonadicTests.hs, Tests/test.zip

Library
  Exposed-Modules:
      Codec.Archive.LibZip
      Codec.Archive.LibZip.Types
  Other-Modules:
      Codec.Archive.LibZip.Errors
  Build-Depends:
      base >= 4.0 && < 5.0
    , bindings-libzip >= 0.10 && < 0.11
    , bytestring
    , filepath
    , time
    , mtl
  GHC-Options:
      -Wall
