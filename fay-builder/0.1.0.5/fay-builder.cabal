name:                fay-builder
version:             0.1.0.5
synopsis:            Compile Fay code on cabal install, and ad-hoc recompile during development
license:             OtherLicense
author:              Silk B.V.
bug-reports:         https://github.com/silkapp/fay-builder/issues
maintainer:          code@silk.co
copyright:           2013 Silk B.V.
category:            Web
build-type:          Simple
cabal-version:       >=1.10

extra-source-files:
  CHANGELOG.md
  README.md

source-repository head
  type:              git
  location:          https://github.com/silkapp/fay-builder.git

library
  default-language:  Haskell2010
  ghc-options:       -Wall
  hs-source-dirs:    src
  exposed-modules:   Fay.Builder
  build-depends:
      base >= 4.5 && < 4.8
    , Cabal >= 1.4 && < 1.19
    , data-default >= 0.5.3 && < 0.6
    , directory >= 1.1 && < 1.3
    , fay == 0.19.*
    , filepath == 1.3.*
    , split >= 0.2.2 && < 0.3
    , text >= 0.11 && < 1.2


