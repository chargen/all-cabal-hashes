                   , Crypto.Cipher.DES.Serialization
  ghc-options:       -Wall -optc-O3 -fno-cse -fwarn-tabs
  if (arch(i386) || arch(x86_64))
    CPP-options: -DARCH_IS_LITTLE_ENDIAN

