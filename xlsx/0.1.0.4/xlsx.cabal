  Hs-source-dirs:    src
  Exposed-modules:   Codec.Xlsx
                     , Codec.Xlsx.Parser
                     , Codec.Xlsx.Types
                     , Codec.Xlsx.Writer
                     , Codec.Xlsx.Lens

  Build-depends:     base         == 4.*
                     , containers   >= 0.5.0.0
                     , transformers >= 0.3.0.0
                     , bytestring   >= 0.10.0.2
                     , text         >= 0.11.3.1
                     , lens         >= 3.8 && < 5
                     , conduit      >= 1.0.0
