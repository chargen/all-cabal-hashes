                       test-framework-quickcheck2 >= 0.3 && < 0.4,
                       parsec >= 3,
                       haskell-src-exts >= 1.13 && < 1.14,
                       lens >= 3.8 && < 3.10,
                       diagrams-haddock
  hs-source-dirs:      test
  default-language:    Haskell2010
