                       tasty-quickcheck >= 0.8 && < 0.9,
                       parsec >= 3,
                       lens >= 3.8 && < 4.10,
                       haskell-src-exts >= 1.16 && < 1.17,
                       diagrams-haddock
  hs-source-dirs:      test
  default-language:    Haskell2010
