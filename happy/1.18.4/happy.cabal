flag small_base
  description: Choose the new smaller, split-up base package.

executable happy
--  executable: happy
  hs-source-dirs: src
  main-is: Main.lhs

  if flag(small_base)
    build-depends: base >= 2.1 && < 4.4, array, containers
  else
    build-depends: base >= 1.0 && < 2.1

  build-depends: haskell98 >= 1.0, mtl >= 1.0

  extensions: CPP, MagicHash
  ghc-options: -W
  other-modules:
        AbsSyn
        First
        GenUtils
        Grammar
        Info
        LALR
        Lexer
        Main
        ParseMonad
        Parser
        ProduceCode
        ProduceGLRCode
        Set
        NameSet
        Target
        AttrGrammar
        AttrGrammarParser
        ParamRules


