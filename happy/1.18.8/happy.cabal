flag small_base
  description: Choose the new smaller, split-up base package.

executable happy
  hs-source-dirs: src
  main-is: Main.lhs

  if flag(small_base)
    build-depends: base >= 2.1, array, containers
  else
    build-depends: base >= 1.0 && < 2.1

  build-depends: base < 4.5, mtl >= 1.0
  build-tools: perl

  extensions: CPP, MagicHash
