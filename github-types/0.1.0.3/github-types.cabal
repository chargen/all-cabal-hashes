    hs-source-dirs:      src

    build-depends:       aeson
    build-depends:       base >= 4 && < 5
    build-depends:       text

    exposed-modules:     GitHub.Types
    exposed-modules:     GitHub.Types.Base
    exposed-modules:     GitHub.Types.Repository
    exposed-modules:     GitHub.Types.Events


