                        semigroupoids,
                        colour                  >= 2.3.3 && < 3.0,
                        HCodecs                 >= 0.3 && < 0.4,
                        vector-space,
                        vector-space-points     == 0.1.3,
                        musicxml2               == 1.7,
                        lilypond                == 1.7,
                        music-pitch-literal     == 1.7,
