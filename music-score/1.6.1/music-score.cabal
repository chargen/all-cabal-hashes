                        musicxml2 == 1.6,
                        lilypond == 1.6,
                        HCodecs                 >= 0.3 && < 0.4,
                        vector-space,
                        vector-space-points     == 0.1.3,
                        music-pitch-literal     == 1.6,
                        music-dynamics-literal  == 1.6,
                        prettify,
                        parsec
