Extra-source-files:	README
			, examples/Echo.hs
			, examples/Queues.hs
			, examples/Channels.hs
Exposed-modules:	Data.Stream
			, Test.IOSpec
			, Test.IOSpec.Teletype
			, Test.IOSpec.IORef
			, Test.IOSpec.Concurrent
