  Type:        darcs
  Location:    http://code.haskell.org/~thielema/non-negative/

Library
  Build-Depends:
    utility-ht >= 0.0.1 && <0.1,
    QuickCheck >= 2.1 && <3
  If flag(splitBase)
    Build-Depends: base >= 2 && < 5
  Else
    Build-Depends: base >= 1.0 && < 2

  Default-Language: Haskell98
