name:                ekg-log
version:             0.1.0.3
synopsis:            Push metrics to a log file.
description:
  This library lets you push system metrics to a log file.
homepage:            https://github.com/ixmatus/ekg-log
bug-reports:         https://github.com/ixmatus/ekg-log/issues
license:             BSD3
license-file:        LICENSE
author:              Parnell Springmeyer
maintainer:          parnell@digitalmentat.com
category:            System
build-type:          Simple
cabal-version:       >=1.10

library
  HS-Source-Dirs:       src
  exposed-modules:     
    System.Local.Monitoring.Log

  build-depends:
    base                      >= 4.4     && < 5,
    aeson                     >= 0.7     && < 0.9,
    bytestring                              < 1.0,
    directory                 >= 1.0     && < 2.0,
    ekg-core                  >= 0.1     && < 1.0,
    text                                    < 1.3,
    fast-logger               >= 2.0     && < 3.0,
    unix                      >= 2.0     && < 3.0,
    filepath                  >= 1.3     && < 1.4,
    time                                    < 1.5,
    unordered-containers                    < 0.3

  default-language:    Haskell2010

  ghc-options: -Wall

source-repository head
  type:     git
  location: https://github.com/ixmatus/ekg-log.git


