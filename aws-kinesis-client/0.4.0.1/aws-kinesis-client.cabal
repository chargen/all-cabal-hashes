license-file:        LICENSE
author:              Jon Sterling
maintainer:          jon@jonmsterling.com
copyright:           Copyright (c) 2013-2015 PivotCloud, Inc.
category:            Cloud
build-type:          Simple
cabal-version:       >=1.10
extra-source-files:  CHANGELOG.md

