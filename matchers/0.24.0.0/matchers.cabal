source-repository head
    type: git
    location: git://github.com/massysett/matchers.git

Library
    Build-depends:
          base >=4.5.0.0 && < 5
        , bytestring >=0.9.2.1 && < 0.11
        , text >=0.11.2.0 && < 1.3
        , prednote >= 0.28.0.0 && < 0.29


    Exposed-modules:
        Matchers
      , Matchers.Pcre
      , Matchers.Pcre.Base

   extra-libraries: pcre

   ghc-options: -Wall
   hs-source-dirs: lib


