    blaze-builder-enumerator >= 0.2    && < 0.3,
    bytestring               >= 0.9    && < 0.11,
    case-insensitive         >= 0.3    && < 0.5,
    containers               >= 0.3    && < 0.6,
    enumerator               >= 0.4.13 && < 0.5,
