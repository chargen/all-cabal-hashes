                   exceptions             == 0.6.*,
                   filepath               == 1.3.*,
                   happstack-authenticate == 0.10.15,
                   hint                   == 0.4.*,
                   hint-server            == 1.4.*,
                   hscolour               == 1.22.*,
                   ixset                  == 1.0.*,
                   mime-mail              == 0.4.*,
                   mtl                    == 2.1.*,
                   deepseq                == 1.3.*,
                   network                == 2.4.*,
                   old-locale             == 1.0.*,
                   random                 == 1.0.*,
                   safe                   == 0.3.*,
                   safecopy               == 0.8.*,
                   stm                    == 2.4.*,
                   tar                    == 0.4.*,
                   temporary              == 1.1.*,
                   template-haskell       >= 2.8 && < 2.10,
                   text                   == 1.1.*,
                   time                   == 1.4.*
