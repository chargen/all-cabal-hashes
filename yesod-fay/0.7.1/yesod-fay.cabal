                     , fay >= 0.21.2.1
                     , fay-dom >= 0.5
                     , transformers >= 0.2
                     , aeson >= 0.6
                     , bytestring >= 0.9
                     , data-default >= 0.4
                     , directory >= 1.1.0.2
                     , text >= 0.11
                     , system-fileio >= 0.3
                     , system-filepath >= 0.4
                     , template-haskell
                     , process
                     , shakespeare
                     , yesod-core >= 1.4
                     , yesod-form >= 1.4
                     , yesod-static >= 1.4
                     , pureMD5 >= 2.1.2.1
                     , utf8-string >= 0.3.7
                     , monad-loops >= 0.3.3.0
