library
  exposed-modules:
        Language.CoreErlang.Parser,
        Language.CoreErlang.Pretty,
        Language.CoreErlang.Syntax
  if flag(split-base)
    build-depends:	base >= 3, parsec == 2.1.0.0, pretty
  else
    build-depends:	base < 3, parsec == 2.1.0.0
