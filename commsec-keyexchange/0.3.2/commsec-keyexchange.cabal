  build-depends:       base <5,
                       network,
                       crypto-pubkey-types,
                       cipher-aes128,
                       crypto-api >= 0.12.2,
                       bytestring,
                       commsec >= 0.3.5,
                       RSA,
                       cryptohash-cryptoapi,
                       DRBG >= 0.5,
                       monadcryptorandom,
                       cereal
