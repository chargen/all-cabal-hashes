    Paths_haskell_packages
  build-depends:
      base >=4.5 && < 5
    , optparse-applicative >= 0.5.1
    , aeson == 0.6.*
    , bytestring >=0.9
    , Cabal >= 1.16 && < 1.21
    , deepseq
    , directory
    , filepath
    , containers
    , mtl
    , hse-cpp
    , EitherT
    , haskell-src-exts >= 1.14
    , tagged


