                       linear >= 1.10 && < 1.19,
                       lens >= 3.0 && < 4.12,
                       containers >=0.4 && < 0.6,
                       data-default-class >= 0.0.1 && < 0.1
  hs-source-dirs:      src
  default-language:    Haskell2010
