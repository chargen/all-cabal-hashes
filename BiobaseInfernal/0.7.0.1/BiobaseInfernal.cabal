    attoparsec          == 0.10.*   ,
    attoparsec-conduit  == 0.5.*    ,
    biocore             == 0.2      ,
    bytestring                      ,
    bytestring-lexing   == 0.4.*    ,
    conduit             == 0.5.*    ,
    containers,
    either-unwrap       == 1.1      ,
    lens                == 3.*      ,
    primitive           >= 0.5      ,
    transformers        == 0.3.*    ,
    tuple               == 0.2.*    ,
    vector              >= 0.10     ,
    BiobaseXNA          == 0.6.3.*  ,
    PrimitiveArray      >= 0.4.0.1

