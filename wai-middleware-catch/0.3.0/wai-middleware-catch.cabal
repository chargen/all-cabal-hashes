                   base >= 4 && < 5,
                   bytestring >= 0.9 && < 0.10,
                   lifted-base >= 0.1.0.1 && < 0.2,
                   http-types >= 0.6 && < 0.7,
                   wai >= 1.0
  ghc-options:      -Wall -rtsopts
  exposed-modules:  Network.Wai.Middleware.Catch



