    build-depends:     base           (==4.*)
                     , binary         (>=0.7 && <0.8)
                     , bytestring     (>=0.9.2.1)
                     , pipes          (>=3.3 && <3.4)
                     , pipes-parse    (>=1.0 && <1.1)
    exposed-modules:   Control.Proxy.Binary
    other-modules:     Control.Proxy.Binary.Internal
    ghc-options: -Wall -O2


