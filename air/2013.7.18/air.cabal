Name:                 air
Version:              2013.7.18
Build-type:           Simple
Synopsis:             air
Description:          An alternative Haskell Prelude library.
License:              BSD3
Author:               Jinjing Wang
Maintainer:           Jinjing Wang <nfjinjing@gmail.com>
Build-Depends:        base
Cabal-version:        >= 1.2
category:             Development
license-file:         LICENSE
homepage:             https://github.com/nfjinjing/air
data-files:           readme.md, changelog.md, known-issues.md

library

  build-depends:  
                  base >= 4 && < 5
                , containers
                , array
                , parallel
                , bytestring >= 0.9
                , directory
                , filepath
                , template-haskell
                , dlist
                , mtl
                , time
                , text

  hs-source-dirs: src/

  exposed-modules:  
                    Air
                  , Air.Light
                  , Air.Env
                  , Air.TH.Here
                  , Air.TH.Default
                  , Air.TH.Air
                  , Air.TH
                  , Air.SimpleMath
                  , Air.Control.Monad.ListBuilder
                  , Air.Control.Monad.ObjectBuilder
                  , Air.Data.Record.SimpleLabel
                  , Air.Data.Record.SimpleLabel.TH
                  , Air.Data.Default
                  , Air.Data.Monoid
                  , Air.Data.Managed


