                       array >= 0.3 && < 0.6,
                       semigroups >= 0.1 && < 0.17,
                       semigroupoids >= 1.2 && < 4.3,
                       vector-space >= 0.8 && < 0.9,
                       newtype >= 0.2 && < 0.3,

                       QuickCheck >= 2.4.2 && < 2.8
    hs-source-dirs:    src, test
    default-language:  Haskell2010
