                   , monad-control         >= 0.2      && < 0.4
                   , time                  >= 1.1
                   , aeson                 >= 0.5
                   , conduit               >= 0.4      && < 0.5
    exposed-modules: Database.Persist.Postgresql
    ghc-options:     -Wall

source-repository head
