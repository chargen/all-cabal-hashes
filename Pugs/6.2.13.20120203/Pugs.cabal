    hs-source-dirs:     src
    extra-lib-dirs:     /usr/lib /opt/local/lib

    build-depends:
        base >= 4 && < 5, filepath, mtl < 2.0.0.0, parsec >= 3.0.0.0, network,
        pretty, time, random, process, containers, bytestring,
        array, directory, utf8-string, binary, haskeline >= 0.6.4.0, FindBin,
        control-timeout >= 0.1.2,

        MetaObject       >= 0.0.4,
        HsParrot         >= 0.0.2,
        pugs-compat      >= 0.0.5,
        pugs-DrIFT       >= 2.2.3.2,
        stringtable-atom >= 0.0.4,
        HsSyck           >= 0.44,
        stm              >= 2.1.2.1
        -- HsPerl5

    if flag(Perl5)
        cpp-options: -DPUGS_HAVE_PERL5=1

    c-sources:
        cbits/Prelude_pm.c cbits/Test_pm.c
        perl5/p5embed.c

    includes:
        perl5/p5embed.h
