license-file:        LICENSE
author:              David Johnson
maintainer:          djohnson.m@gmail.com
copyright:           David Johnson (c) 2014
category:            Web, IO-Streams
build-type:          Simple
cabal-version:       >=1.10

library
  exposed-modules:     System.IO.Streams.HTTP
  hs-source-dirs:      src
