license:             MIT
license-file:        LICENSE
author:              Geraud Boyer
maintainer:          geraud@gmail.com
-- copyright:
category:            Text
build-type:          Simple
extra-source-files:  README.md
cabal-version:       >=1.10
tested-with:
    GHC == 7.6
  , GHC == 7.8
  , GHC == 7.10

library
  build-depends:
    base >=4.5 && <4.9
    , text >= 1.1
    , attoparsec >= 0.12
