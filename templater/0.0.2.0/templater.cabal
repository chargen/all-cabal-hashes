  build-depends:
    base
    , templater
    , hspec == 2.*
    , hspec-attoparsec >= 0.1.0
    , HUnit
    , QuickCheck
    , text
  default-language:
    Haskell2010
  ghc-options:
    -Wall
    -Werror
  hs-source-dirs:
    test
  type:
    exitcode-stdio-1.0
