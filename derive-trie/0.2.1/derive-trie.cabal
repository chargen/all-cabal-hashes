    Default-Language:
        Haskell2010

    Build-Depends:
        base             >= 4.3 && < 4.5,
        array            == 0.3.*,
        containers       >= 0.3 && < 0.5,
        template-haskell >= 2.5 && < 2.7

    Ghc-Options:
        -Wall

    Hs-Source-Dirs:
        src

    Exposed-modules:
        Data.Derive.Trie,
        Data.KeyMap

