                    template-haskell, derive, binary, regex-tdfa
     exposed-modules:
         Manatee.Extension.Curl
         Manatee.Extension.Curl.PageMode
         Manatee.Extension.Curl.CurlBuffer
         Manatee.Extension.Curl.CurlView	 
         Manatee.Extension.Curl.Types
     other-modules:							
     extensions:  
