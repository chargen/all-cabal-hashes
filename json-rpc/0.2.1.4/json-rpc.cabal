  other-modules:        Network.JsonRpc.Data,
                        Network.JsonRpc.Conduit
  build-depends:        base                        >= 4.6      && < 5,
                        aeson                       >= 0.7      && < 0.9,
                        attoparsec                  >= 0.11,
                        async                       >= 2.0      && < 2.1,
