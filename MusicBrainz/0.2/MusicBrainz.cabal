               , bytestring
               , conduit       >= 1.0.0
               , text          ==0.11.*
               , time
               , vector        >=0.9
               , xml-types     ==0.3.*
               , http-conduit  >= 1.8.8
               , http-types    >= 0.6
               , old-locale    ==1.0.*
               , xml-conduit   >= 1.1
               , HTTP          >= 4000.0
               , aeson         >= 0.6.1
