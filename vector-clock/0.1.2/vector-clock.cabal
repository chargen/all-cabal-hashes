  Type:                 git
  Location:             git://github.com/scvalex/vector-clock.git

Library
  Build-Depends:        base >= 4 && <5, binary
  Ghc-Options:          -Wall
  Extensions:           Safe
  Exposed-Modules:      Data.VectorClock, Data.VectorClock.Simple

