name:                  duplo
version:               1.6.2
synopsis:              Frontend development build tool
description:           Intuitive, simple building blocks for building composable, completely self-managed web applications
license:               MIT
license-file:          LICENSE
author:                Kenneth Kan
maintainer:            ken@pixbi.com
category:              Web
build-type:            Simple
extra-source-files:    README.md
cabal-version:         >=1.10

source-repository head
  type:                git
  location:            https://github.com/pixbi/duplo.git

executable duplo
  main-is            : Duplo.hs
  build-depends      : base                 == 4.7.*
                     , MissingH             == 1.3.*
                     , aeson                == 0.8.*
                     , aeson-pretty         == 0.7.*
                     , base64-bytestring    == 1.0.*
                     , bytestring           == 0.10.*
                     , containers           == 0.5.*
                     , directory            == 1.2.*
                     , executable-path      == 0.0.*
                     , filepath             == 1.3.*
                     , filepather           == 0.3.*
                     , fsnotify             == 0.1.*
                     , http-types           == 0.8.*
                     , language-javascript  == 0.5.*
                     , lens                 == 4.5.*
                     , mtl                  == 2.2.*
                     , process              == 1.2.*
                     , regex-compat         == 0.95.*
                     , scotty               == 0.9.*
                     , shake                == 0.14.*
                     , system-fileio        == 0.3.*
                     , system-filepath      == 0.4.*
                     , text                 == 1.1.*
                     , transformers         == 0.4.*
                     , unordered-containers == 0.2.*
                     , utf8-string          == 0.3.*
                     , wai                  == 3.0.*
                     , warp                 == 3.0.*
                     , ansi-terminal        == 0.5.*
                     , text-format          == 0.3.*
                     , alex                 == 3.1.*
  hs-source-dirs     : src/
  default-language   : Haskell2010
  default-extensions : OverloadedStrings


