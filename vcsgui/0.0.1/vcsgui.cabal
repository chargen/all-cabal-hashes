               gtk >=0.12.0 && <0.13
    buildable: True
    hs-source-dirs: src/exe/askpass src
    other-modules: VCSGui.Svn.Helper VCSGui.Common.Process
               VCSGui.Common.ConflictsResolved VCSGui.Common.MergeTool
