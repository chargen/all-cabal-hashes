                     , template-haskell
                     , apiary               >=1.1   && <1.3
                     , purescript           ==0.6.2.*

                     , filepath             >=1.3   && <1.4
                     , utf8-string          >=0.3   && <0.4
                     , text                 >=1.1   && <1.3
                     , bytestring           >=0.10  && <0.11
                     , unordered-containers >=0.2   && <0.3
                     , parsec               >=3.1   && <3.2
                     , Glob                 >=0.7   && <0.8
                     , data-default-class   >=0.0   && <0.1

  hs-source-dirs:      src
  ghc-options:         -O2 -Wall
  default-language:    Haskell2010

source-repository head
  type:     git
  location: git://github.com/philopon/apiary.git
