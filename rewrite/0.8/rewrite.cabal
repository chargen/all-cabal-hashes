copyright:           2013 Omari Norman
category:            System
build-type:          Simple
cabal-version:       >=1.8

source-repository head
  type: git
  location: git://github.com/massysett/rewrite.git

executable rewrite
  main-is: rewrite.hs
  build-depends:
      base ==4.6.*
    , multiarg ==0.24.*
    , directory ==1.2.*
    , bytestring ==0.10.*
    , process ==1.1.*

  ghc-options: -Wall -threaded
