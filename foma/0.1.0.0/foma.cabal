name:                foma
version:             0.1.0.0
synopsis:            Simple Haskell bindings for Foma.
description:         Foma is a free and open source finite-state toolkit.
                     This package contains some simple Haskell bindings to
                     to <http://code.google.com/p/foma/ Foma>.
homepage:            http://github.com/joom/foma.hs
license:             MIT
license-file:        LICENSE
author:              Joomy Korkut
maintainer:          joomy@cattheory.com
category:            Language
build-type:          Simple
cabal-version:       >=1.10

library
  exposed-modules:     Language.Foma
  build-depends:       base >=4.7 && <4.9
  hs-source-dirs:      src
  default-language:    Haskell2010
  extra-libraries:     foma
