    , random                    == 1.0.*
    , rest-types                == 1.9.*
    , safe                      >= 0.2    &&  < 0.4
    , split                     >= 0.1    &&  < 0.3
    , text                      >= 0.11   &&  < 1.2
    , tostring                  == 0.2.*
