  Build-Depends:	base >= 3 && < 5, containers >= 0.3, array
  Exposed-Modules:	Algebra.CommutativeMonoid.Unification
                        Algebra.CommutativeMonoid.LinDiophEq
  Hs-Source-Dirs:	src
  GHC-Options:
    -Wall -fno-warn-name-shadowing -fwarn-unused-imports

