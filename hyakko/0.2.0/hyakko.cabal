                  process >= 1,
                  filepath >= 1.2,
                  regex-compat >= 0.9,
                  regex-pcre >= 0.9,
                  containers >= 0.4,
                  directory >= 1,
                  pandoc >= 1.8.2
  hs-source-dirs: src
  ghc-options:    -O2 -Wall
  main-is:        Hyakko.hs
