name:             hyakko
version:          0.5.2
cabal-version:    >= 1.6
build-type:       Simple
license:          MIT
license-file:     LICENSE
copyright:        (c) 2013 Jeremy Hull
author:           Jeremy Hull <sourdrums@gmail.com>
maintainer:       Jeremy Hull <sourdrums@gmail.com>
bug-reports:      https://github.com/sourrust/hyakko/issues
stability:        beta
homepage:         http://sourrust.github.io/hyakko/
category:         Documentation
tested-with:      GHC == 7.4.2
synopsis:         Literate-style Documentation Generator
description:      Hyakko is a Haskell port of Docco: the original
                  quick-and-dirty, hundred-line-long,
                  literate-programming-style documentation generator.
data-files:
                  -- HTML template and CSS sytle
                  resources/hyakko.html
                  resources/hyakko.css
extra-source-files:
                  src/Text/Markdown.hs

source-repository head
  type:           git
  location:       git://github.com/sourrust/hyakko.git

Flag threaded
  default:        True

Executable hyakko
  build-depends:  base >= 4 && < 5,
                  ghc >= 7,
                  filepath >= 1.2,
                  regex-pcre >= 0.9,
                  containers >= 0.4,
                  directory >= 1,
                  pandoc >= 1.10,
                  bytestring >= 0.9,
                  text >= 0.11,
                  highlighting-kate >= 0.5,
                  blaze-html >= 0.5,
                  cmdargs >= 0.10
  hs-source-dirs: src
  ghc-options:    -O2 -Wall
  main-is:        Hyakko.lhs
