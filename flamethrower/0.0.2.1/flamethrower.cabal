    import Flamethrower
    import Flamethrower.Escape
    import Data.Text
    import qualified Data.Text.IO
    .
    render name = [flamethrower|
    p id: &#34;greeting&#34;
        &#9;&#34;Hello, &#34;
        &#9;em &#34;&#35;&#123;name&#125;&#34;
