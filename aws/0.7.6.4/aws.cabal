                       containers           >= 0.4,
                       crypto-api           >= 0.9,
                       cryptohash           >= 0.6     && < 0.10,
                       cryptohash-cryptoapi == 0.1.*,
                       directory            >= 1.0     && < 1.3,
                       failure              >= 0.2.0.1 && < 0.3,
                       filepath             >= 1.1     && < 1.4,
                       http-conduit         >= 1.6     && < 1.10,
                       http-types           >= 0.7     && < 0.9,
                       lifted-base          >= 0.1     && < 0.3,
