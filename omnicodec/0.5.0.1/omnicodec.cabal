synopsis: data encoding and decoding command line utilities
description: Two simple command line tools built on dataenc.
build-type: Simple
category: Codec
cabal-version: >= 1.6

executable odec
    main-is: odec.hs
    hs-source-dirs: src
    build-depends: base ==4.3.*, cmdargs ==0.6.*, bytestring ==0.9.*,
        mtl ==2.0.*, dataenc ==0.14.*
    ghc-options: -Wall

executable oenc
    main-is: oenc.hs
    hs-source-dirs: src
    ghc-options: -Wall


