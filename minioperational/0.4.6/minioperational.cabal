  location: https://github.com/fumieval/minioperational.git

library
  default-language:   Haskell2010
  ghc-options: -Wall -O2
  exposed-modules:     Control.Monad.Operational.Mini
    , Control.Monad.Trans.Operational.Mini
    , Control.Monad.Operational.Class
