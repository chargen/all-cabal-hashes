    GHC-Options:        -Wall -fno-warn-unused-do-bind
  else
    GHC-Options:        -Wall
  Exposed-Modules:      Data.Set.RBTree
                        Data.Set.LLRBTree
                        Data.Set.WBTree
                        Data.Set.Splay
                        Data.Heap.Skew
                        Data.Heap.Leftist
                        Data.Heap.Splay
                        Data.Heap.Binominal
  Build-Depends:        base >= 4 && < 5

