                   utf8-string >= 0.3.7,
                   containers >= 0.4.2.1,
                   zeromq-haskell >= 0.8.4,
                   enumerator >= 0.4.18,
                   mtl         >= 2.0.1.0,
                   time        >= 1.4

  default-language: Haskell98

