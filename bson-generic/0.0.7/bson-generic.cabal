name:                bson-generic
version:             0.0.7
synopsis:            Generic functionality for BSON
description:         This package offers easy conversion from and to BSON data type for most of user defined data types.
                     The interface may change at will.
license:             BSD3
license-file:        LICENSE
author:              Petr PilaÅ™
maintainer:          the.palmik+maintainer@gmail.com
category:            Data
build-type:          Simple
cabal-version:       >= 1.8
stability:           experimental

library
  hs-source-dirs: src

  exposed-modules:
      Data.Bson.Generic

  build-depends:
      base     == 4.5.*
    , bson     == 0.2.*
    , ghc-prim == 0.2.*
    , text     == 0.11.*

  ghc-options: -O2



