                       lens >= 3.8 && < 3.10,
                       binary < 0.7,
                       unordered-containers ==0.2.*,
                       containers >=0.4.2 && < 0.6,
                       text ==0.11.*,
                       time < 1.5,
                       intervals < 0.3
  
