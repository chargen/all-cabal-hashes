    default-extensions: ForeignFunctionInterface
    ghc-options: -Wall
    build-depends:
          base                 >= 3 && < 5
        , gitlib
        , gitlib-utils
        , hlibgit2             >= 0.18.0.1
        , attempt              >= 0.4.0
        , bytestring           >= 0.9.2.1
        , conduit              >= 0.5.5
        , containers           >= 0.4.2
        , data-default         >= 0.5.0
