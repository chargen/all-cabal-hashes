name:               hfann
version:            0.2
cabal-version:      >= 1.2
build-type:         Simple
license:            BSD3
license-file:       LICENSE
copyright:          (c) Olivier Boudry 2008
author:             Olivier Boudry
maintainer:         Olivier Boudry <olivier.boudry@gmail.com>
stability:          Experimental
homepage:           http://code.haskell.org/~oboudry/hfann/
package-url:
synopsis:           Haskell binding to the FANN library
description:        hfann is a Haskell binding to the Fast Artificial Neural
                    Network (FANN) library <http://leenissen.dk/fann/>. It
                    provides functions to easily create, train, test and use
                    Artificial Neural Networks.
category:           AI
tested-with:        GHC == 6.8.2
data-files:         
extra-source-files: AUTHORS,
                    CHANGELOG,
                    README,
                    examples\xor\Train.hs,
                    examples\xor\Use.hs,
                    examples\xor\xor.data
extra-tmp-files:

library
  buildable:        True
  build-depends:    base >= 2 && < 4
  exposed-modules:  HFANN
                    HFANN.Base
                    HFANN.Data
                    HFANN.IO
                    HFANN.Train
  other-modules:
  extensions:
  hs-source-dirs:   src
  ghc-options:      -Wall -O2
  ghc-prof-options:
  ghc-shared-options:
  hugs-options:
  nhc98-options:
  includes:
  install-includes:
  c-sources:
  extra-libraries:    doublefann
  extra-lib-dirs:     
  include-dirs:       
  cc-options:
  ld-options:
  pkgconfig-depends:


executable            HFANNTest
  buildable:          True
  build-depends:      base
  main-is:            HFANNTest.hs
  build-tools:
  ghc-options:        -Wall -O2
  cpp-options:
  cc-options:
  ld-options:
  pkgconfig-depends:
  frameworks:
  c-sources:
  extra-libraries:    doublefann
  extra-lib-dirs:     
  include-dirs:       
  hs-source-dirs:     src
  other-modules:      HFANN
                      HFANN.Base
                      HFANN.Data
                      HFANN.IO
                      HFANN.Train


