copyright:           2013, SeeReason Partners LLC
author:              Jeremy Shaw
maintainer:          jeremy@n-heptane.com
category:            Happstack
build-type:          Simple
cabal-version:       >=1.8

data-files:
  src/HTML.hs

library
  hs-source-dirs: src
  exposed-modules:     HTML

  build-depends:       fay-base         == 0.14.*,
                       fay-jquery       == 0.3.*


