  Build-Depends: base >= 3 && < 5,
                 bytestring >= 0.9,
                 cereal >= 0.2,
                 pretty >= 1.1.2
  hs-source-dirs:
  exposed-modules: Data.Ethernet, Data.IP, Data.IPv6, Data.Header, Data.TCP, Data.UDP, Data.CSum
  ghc-options:

