maintainer:          neil@twistedsquare.com
copyright:           Copyright (c) 2009, Neil Brown
stability:           Provisional
build-depends:       base, SG, OpenGL, GLUT
build-type:          Simple

executable:          sgdemo
main-is:             SGdemo.hs
ghc-options:         
extensions:          MultiParamTypeClasses FlexibleContexts
