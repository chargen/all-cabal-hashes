                   , tls                           >= 1.2.1
                   , network                       >= 2.2.1
                   , cprng-aes                     >= 0.5.0
                   , streaming-commons
  Exposed-modules:   Network.Wai.Handler.WarpTLS
  ghc-options:       -Wall

