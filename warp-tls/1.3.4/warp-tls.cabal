                   , pem                           >= 0.1      && < 0.2
                   , cryptocipher                  >= 0.3      && < 0.4
                   , tls-extra                     >= 0.4.5    && < 0.6
                   , tls                           >= 0.9.6    && < 1.1
                   , crypto-api                    >= 0.8      && < 0.11
                   , network                       >= 2.2.1
  Exposed-modules:   Network.Wai.Handler.WarpTLS
  ghc-options:       -Wall

source-repository head
