               , diagrams-core >= 1.1 && < 1.2
               , diagrams-lib >= 1.1 && < 1.2
               , diagrams-svg >= 0.8 && < 1.2
               , diagrams-postscript >= 0.7 && < 1.2
               , SVGFonts >= 1.4 && < 1.5
               , colour >= 2.2.1 && < 2.4
               , blaze-svg >= 0.3.3
               , bytestring >= 0.9 && < 1.0
               , operational >= 0.2.2 && < 0.3
