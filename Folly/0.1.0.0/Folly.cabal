  exposed-modules:     Folly.Formula, Folly.Unification, Folly.Resolution, Folly.Utils
  -- other-modules:       
  build-depends:       base < 6, containers
  hs-source-dirs:      src

executable Folly
  main-is:             Main.hs
  -- other-modules:       
  build-depends:       base < 6, containers, parsec
  hs-source-dirs:      src

executable Folly-tests
