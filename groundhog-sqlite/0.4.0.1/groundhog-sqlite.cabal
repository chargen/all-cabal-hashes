                   , monad-control           >= 0.3
                   , monad-logger            >= 0.3
                   , containers              >= 0.2
                   , text                    >= 0.8
                   , direct-sqlite           >= 2.3.5
                   , resource-pool           >= 0.2.1
                   , unordered-containers
    exposed-modules: Database.Groundhog.Sqlite
    ghc-options:     -Wall -fno-warn-unused-do-bind

source-repository head
