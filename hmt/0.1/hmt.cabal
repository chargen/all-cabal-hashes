                   Help/hmt.help.lhs

Library
  Build-Depends:   base == 3.*,
                   containers,
                   parsec,
                   permutation
  GHC-Options:     -Wall -fwarn-tabs
  Exposed-modules: Music.Theory
                   Music.Theory.Parse
                   Music.Theory.Pct
                   Music.Theory.Pitch
                   Music.Theory.Prime
                   Music.Theory.Set
                   Music.Theory.Table
                   Music.Theory.Permutations


