  Build-depends:       egison, base >= 4.0 && < 5, array, containers, haskeline, transformers, mtl, parsec >= 3.0, directory, ghc, ghc-paths
  Other-modules:       Paths_egison
  Hs-Source-Dirs:      hs-src/Interpreter
  
