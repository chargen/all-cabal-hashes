license-file:        LICENSE
author:              Omari Norman
maintainer:          omari@smileystation.com
copyright:           2013 Omari Norman
category:            Data
build-type:          Simple
cabal-version:       >=1.8

source-repository head
    type: git
    location: git://github.com/massysett/prednote.git

library

