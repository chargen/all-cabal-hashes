      Data.Prednote.Pdct
    , Data.Prednote.Expressions
    , Data.Prednote.Expressions.Infix
    , Data.Prednote.Expressions.RPN
    , Data.Prednote.TestTree

  build-depends:
      base >= 4.6 && < 5
    , explicit-exception ==0.1.*
    , rainbow ==0.2.*
    , split ==0.2.*
    , text == 0.11.*

  ghc-options: -Wall
