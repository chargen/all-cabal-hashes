                   , bytestring
                   , random
                   , crypto-api >= 0.8
                   , entropy >= 0.2
                   , cryptocipher >= 0.2.5 && < 0.4.0
                   , cereal >= 0.3.0 && < 0.4.0
  Exposed-modules:   Crypto.Random.AESCtr
  ghc-options:       -Wall

