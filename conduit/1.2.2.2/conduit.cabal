    main-is: main.hs
    other-modules: Data.Conduit.Extra.ZipConduitSpec
                 , Data.Conduit.StreamSpec
    type: exitcode-stdio-1.0
    cpp-options:   -DTEST
    build-depends:   conduit
                   , base
                   , hspec >= 1.3
                   , QuickCheck >= 2.5
                   , transformers
                   , mtl
                   , resourcet
                   , void
                   , containers
                   , exceptions >= 0.6
                   , safe
    ghc-options:     -Wall

