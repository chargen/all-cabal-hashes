                      , test-framework       >= 0.8  && < 0.9
                      , test-framework-hunit >= 0.3  && < 0.4
                      , text                 >= 1.2  && < 1.3
                      , unordered-containers >= 0.2  && < 0.3
                      , vector               >= 0.10 && < 0.11

source-repository head
  type:               git
  location:           git://github.com/seagreen/hjcase.git
