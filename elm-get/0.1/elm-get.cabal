                       binary,
                       bytestring,
                       cmdargs,
                       containers,
                       directory,
                       Elm >= 0.10.1,
                       filepath,
                       HTTP,
                       http-conduit >= 1.9, http-conduit < 2.0,
                       http-types,
                       json,
                       mtl,
                       network,
                       pretty,
                       process,
                       resourcet,
                       text,
                       vector
