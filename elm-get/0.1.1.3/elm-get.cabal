  ghc-options:         -threaded -O2 -W
  Hs-Source-Dirs:      src
  Main-is:             Get/Main.hs
  other-modules:       Get.Dependencies,
                       Get.Install,
                       Get.Library,
                       Get.Options,
                       Get.Publish,
                       Get.Registry,
                       Utils.Commands,
                       Utils.Http,
                       Utils.Paths
