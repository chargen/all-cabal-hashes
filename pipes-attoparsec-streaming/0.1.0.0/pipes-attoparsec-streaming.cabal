    Control.Pipe.Attoparsec.Stream
  -- other-modules:
  build-depends:
    base == 4.5.*,
    pipes-core == 0.1.*,
    attoparsec == 0.10.*,
    transformers >= 0.3,
    bytestring >= 0.9

executable MimeParser
  main-is: MimeParser.hs
  hs-source-dirs: examples
  build-depends:
    base == 4.5.*,
    pipes-core == 0.1.*,
    attoparsec == 0.10.*,
    transformers >= 0.3,
    bytestring >= 0.9,
    pipes-attoparsec-streaming



