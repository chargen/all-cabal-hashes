  Location: https://github.com/MichelBoucey/ip6addr.git

executable ip6addr
  GHC-Options: -Wall -O2
  main-is:             Main.hs
  other-extensions:    DeriveDataTypeable, OverloadedStrings
  build-depends:       base >=4.7 && <4.9, cmdargs,IPv6Addr >= 0.6, text >= 1.2
  default-language:    Haskell2010


