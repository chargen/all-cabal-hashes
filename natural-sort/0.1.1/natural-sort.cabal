author: John Millikin <jmillikin@gmail.com>
maintainer: John Millikin <jmillikin@gmail.com>
copyright: 2010 John Millikin <jmillikin@gmail.com>
build-type: Simple
cabal-version: >=1.2
category: Algorithms

library
  build-depends:
      base >= 2 && < 5
    , parsec >= 3
    , text
    , bytestring

  exposed-modules:
    Algorithms.NaturalSort

  ghc-options: -Wall


