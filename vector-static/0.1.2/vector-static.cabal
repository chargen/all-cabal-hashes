  Tag:      v0.1.2

Library
  Exposed-Modules:
        Data.Nat,
        Data.Fin,
        
        Data.Vector.Generic.Static,
        Data.Vector.Generic.Mutable.Static,

        Data.Vector.Static,
        Data.Vector.Mutable.Static,
        
        Data.Vector.Unboxed.Fin,      
        Data.Vector.Unboxed.Static,
        Data.Vector.Unboxed.Mutable.Static,

        Data.Vector.Storable.Static,
        Data.Vector.Storable.Mutable.Static
        

  Build-Depends: base >= 2 && < 5, vector >= 0.5, primitive >= 0.2.1


