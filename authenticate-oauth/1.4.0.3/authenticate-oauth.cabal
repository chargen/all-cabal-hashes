                   , random
                   , http-types                    >= 0.6      && < 0.8
                   , blaze-builder
                   , conduit                       >= 0.4      && < 0.6
                   , resourcet                     >= 0.3      && < 0.5
                   , blaze-builder-conduit         >= 0.4      && < 0.6
                   , monad-control                 >= 0.3      && < 0.4
    exposed-modules: Web.Authenticate.OAuth, Web.Authenticate.OAuth.IO
    ghc-options:     -Wall

source-repository head
