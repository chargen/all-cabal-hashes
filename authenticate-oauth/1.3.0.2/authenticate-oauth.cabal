                   , random
                   , http-types                    >= 0.6      && < 0.7
                   , blaze-builder
                   , conduit                       >= 0.4      && < 0.5
                   , resourcet                     >= 0.3      && < 0.4
                   , blaze-builder-conduit         >= 0.4      && < 0.5
                   , monad-control                 >= 0.3      && < 0.4
    exposed-modules: Web.Authenticate.OAuth, Web.Authenticate.OAuth.IO
    ghc-options:     -Wall

source-repository head
