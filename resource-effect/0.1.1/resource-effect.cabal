  exposed-modules:     Control.Eff.Resource

  build-depends:       base == 4.6.*
                     , containers == 0.5.*
                     , extensible-effects == 1.2.*

  hs-source-dirs:      src/
  default-language:    Haskell2010

test-suite tests
  type: exitcode-stdio-1.0
  main-is: Test.hs
  hs-source-dirs: test/

  ghc-options: -rtsopts=all -threaded

  build-depends:
      base == 4.6.*
    , QuickCheck == 2.*
    , HUnit == 1.2.*
    , test-framework == 0.8.*
    , test-framework-hunit == 0.3.*
    , test-framework-quickcheck2 == 0.3.*
    , containers == 0.5.*
    , extensible-effects == 1.2.*
    , mtl == 2.1.*
    , resource-effect

  default-language:    Haskell2010

