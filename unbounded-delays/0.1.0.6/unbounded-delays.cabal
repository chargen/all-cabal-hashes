source-repository head
  Type: git
  Location: git://github.com/basvandijk/unbounded-delays.git

library
  build-depends: base >= 4 && < 4.8
  exposed-modules: Control.Concurrent.Thread.Delay
                 , Control.Concurrent.Timeout
  ghc-options: -Wall
