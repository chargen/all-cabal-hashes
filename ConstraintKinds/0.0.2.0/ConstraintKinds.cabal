        base                    >= 3 && < 5,
        ghc-prim,
        vector                  >= 0.9,
        dlist                   >= 0.5
        
    hs-source-dirs:     src
    ghc-options:        -rtsopts -auto-all -caf-all
    Exposed-modules:
        Control.ConstraintKinds
        Control.ConstraintKinds.Applicative
        Control.ConstraintKinds.Filterable
        Control.ConstraintKinds.Foldable
        Control.ConstraintKinds.Functor
