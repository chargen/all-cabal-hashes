    Lvm.Core.NoShadow
    Lvm.Core.Normalize
    Lvm.Core.Parsing.Layout
    Lvm.Core.Parsing.Lexer
    Lvm.Core.Parsing.Parser
    Lvm.Core.Parsing.Token
    Lvm.Core.PrettyId
    Lvm.Core.RemoveDead
    Lvm.Core.Saturate
    Lvm.Core.ToAsm
    Lvm.Core.Type
    Lvm.Core.Utils
    Lvm.Data
    Lvm.Import
    Lvm.Instr.Data
    Lvm.Instr.Resolve
    Lvm.Instr.Rewrite
    Lvm.Path
