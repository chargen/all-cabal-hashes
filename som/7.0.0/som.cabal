  build-depends:   base ==4.*,
                   binary ==0.7.*,
                   containers ==0.5.*,
                   grid ==7.*,
                   MonadRandom ==0.1.*
  ghc-options:     -Wall
  exposed-modules: Data.Datamining.Clustering.SOM,
