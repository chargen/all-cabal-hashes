    , system-fileio   >= 0.3.9
    , time            >= 1.4
    , text            >= 0.11.2
    , containers      >= 0.4.2
    , bytestring      >= 0.9.2.1
    , stringable      >= 0.1
    , lens            >= 2.8
  exposed-modules:
