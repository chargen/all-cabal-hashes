    , containers      >= 0.4.2
    , lens            >= 2.8
    , parallel-io     >= 0.3.2.1
    , stringable      >= 0.1.1
    , system-fileio   >= 0.3.9
    , system-filepath >= 0.4.7
    , text            >= 0.11.2
