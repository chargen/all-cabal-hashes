Name:		hoodle-render
Version:	0.5
Synopsis:       Hoodle file renderer
Description: 	Rendering library using cairo for hoodle file format
License: 	BSD3
License-file:	LICENSE
Author:		Ian-Woo Kim
Maintainer: 	Ian-Woo Kim <ianwookim@gmail.com>
Category:       Graphics
Build-Type: 	Simple
Cabal-Version:  >= 1.8
data-files:     
Source-repository head
  type: git
  location: http://www.github.com/wavewave/hoodle-render

flag gtk3
  description: compile with gtk3
  default: False

Library
  hs-source-dirs: src
  ghc-options: 	-Wall -fno-warn-orphans -funbox-strict-fields -fno-warn-unused-do-bind
  ghc-prof-options: -caf-all -auto-all

  Build-Depends: 
                 base == 4.*, 
                 base64-bytestring >= 0.1, 
                 bytestring >= 0.9,
                 cairo >= 0.12, 
                 containers >= 0.4, 
                 directory, 
                 filepath, 
                 gd >= 3000.7,  
                 hashable,
                 hoodle-types >= 0.3.0,
                 lens >= 2.5, 
                 monad-loops >= 0.3,  
                 mtl == 2.*, 
                 poppler >= 0.12.2.2,
                 stm,
                 strict >= 0.3, 
                 svgcairo >= 0.12,
                 time,
                 transformers >= 0.3,
                 unix >= 2.5,
                 unordered-containers, 
                 uuid >= 1.2 
  if flag(gtk3)
    build-depends: gtk3 >= 0.13
  else 
    build-depends: gtk > 0.13


