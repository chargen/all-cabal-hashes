                       text >= 0.7.2,
                       filepath >= 1.3.0,
                       zlib >= 0.5.4,
                       xmlgen == 0.4.0.3,
                       optparse-applicative >= 0.5.2,
                       vector >= 0.10.0,
                       hopfli >= 0.1.0.0

