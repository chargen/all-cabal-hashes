cabal-version:       >=1.10

library
  exposed-modules:     Text.Inflections
  ghc-options:         -Wall
  build-depends:       base >=4.6 && <4.7, parsec, containers
  default-language:    Haskell2010

test-suite test
  type: exitcode-stdio-1.0
  hs-source-dirs: test
  main-is: Suite.hs
  build-depends:
      inflections
      , base >=4.6 && <4.7
      , test-framework
      , HUnit
      , QuickCheck
      , test-framework-hunit
      , test-framework-quickcheck2
      , containers
  default-language:    Haskell2010
