    main-is: Tests.hs
    type: exitcode-stdio-1.0
    build-depends:     base >= 4 && < 5
                     , bytestring
                     , text
                     , conduit >= 0.0.2
                     , QuickCheck
                     , HUnit
                     , hspec >= 1.3
                     , tagstream-conduit
    ghc-options:     -Wall
