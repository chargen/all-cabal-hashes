                     , bytestring               >= 0.9
                     , zlib-bindings            >= 0.1          && < 0.2
                     , conduit                  >= 0.5          && < 0.6
                     , void
  ghc-options:     -Wall

test-suite test
    hs-source-dirs: test
    main-is: main.hs
    type: exitcode-stdio-1.0
    cpp-options:   -DTEST
    build-depends:   conduit
                   , base
                   , hspec >= 1.3
                   , QuickCheck
                   , bytestring
                   , transformers
                   , zlib-conduit
                   , resourcet
    ghc-options:     -Wall

