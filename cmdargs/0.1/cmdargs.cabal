    exposed-modules:
        System.Console.CmdArgs
    other-modules:
        System.Console.CmdArgs.UI
        System.Console.CmdArgs.Help
        System.Console.CmdArgs.Flag
        System.Console.CmdArgs.Type
        System.Console.CmdArgs.Expand

executable cmdargs
    main-is: Main.hs
