author:              Chris Done
maintainer:          chrisdone@gmail.com
category:            Development
build-type:          Simple
cabal-version:       >=1.8

executable cabal-sign
  main-is:             Main.hs
  ghc-options: -O2
  hs-source-dirs:      src
  build-depends:       base > 4 && < 5,
                       SHA2,
                       optparse-applicative,
                       zlib,
                       tar,
                       bytestring,
                       directory,
                       cereal,
