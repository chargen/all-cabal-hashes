    , wai                       >= 1.3      && < 1.4
    , warp                      >= 1.3      && < 1.4
    , transformers

  ghc-options:     -Wall
  hs-source-dirs:  src
  exposed-modules:
    Network.Wai.EventSource
    Network.Wai.EventSource.EventStream

