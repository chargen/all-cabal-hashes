  build-depends:
    attoparsec >= 0.10 && < 0.13,
    text >= 1 && < 1.3,
    list-t == 0.4.*,
    either == 4.*,
    transformers >= 0.3 && < 0.5,
    base-prelude >= 0.1.19 && < 0.2


