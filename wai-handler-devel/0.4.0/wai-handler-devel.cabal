                   , wai                   >= 0.4           && < 0.5
                   , wai-extra             >= 0.4           && < 0.5
                   , http-types            >= 0.6           && < 0.7
                   , warp                  >= 0.4           && < 0.5
                   , directory             >= 1.0           && < 1.2
                   , network               >= 2.2           && < 2.4
                   , bytestring            >= 0.9           && < 0.10
                   , hint                  >= 0.3.2.3       && < 0.4
