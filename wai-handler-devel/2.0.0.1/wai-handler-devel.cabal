                   , wai                   >= 2.0           && < 2.2
                   , wai-extra             >= 2.0           && < 2.2
                   , http-types            >= 0.7
                   , warp                  >= 2.0           && < 2.2
                   , directory             >= 1.0.1
                   , network               >= 2.2.1.5
                   , bytestring            >= 0.9.1.4
                   , hint                  >= 0.3.2.3       && < 0.5
