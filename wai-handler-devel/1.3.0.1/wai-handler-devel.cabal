                   , wai                   >= 1.3           && < 1.4
                   , wai-extra             >= 1.3           && < 1.4
                   , http-types            >= 0.7           && < 0.8
                   , warp                  >= 1.3           && < 1.4
                   , directory             >= 1.0.1
                   , network               >= 2.2.1.5       && < 2.4
                   , bytestring            >= 0.9.1.4
                   , hint                  >= 0.3.2.3       && < 0.4
                   , text                  >= 0.7
                   , old-time
                   , transformers          >= 0.2.2
                   , attoparsec            >= 0.10
  Exposed-modules:   Network.Wai.Handler.DevelServer
                     Network.Wai.Application.Devel
  ghc-options:       -Wall

Executable           wai-handler-devel
