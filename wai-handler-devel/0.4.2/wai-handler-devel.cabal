                   , wai                   >= 0.4           && < 0.5
                   , wai-extra             >= 0.4           && < 0.5
                   , http-types            >= 0.6           && < 0.7
                   , warp                  >= 0.4           && < 0.5
                   , directory             >= 1.0           && < 1.2
                   , network               >= 2.2           && < 2.4
                   , bytestring            >= 0.9           && < 0.10
                   , hint                  >= 0.3.2.3       && < 0.4
                   , text                  >= 0.7           && < 0.12
                   , old-time              >= 1.0           && < 1.1
                   , enumerator            >= 0.4.6         && < 0.5
                   , transformers          >= 0.2.2         && < 0.3
                   , attoparsec-text       >= 0.8.2         && < 0.9
  Exposed-modules:   Network.Wai.Handler.DevelServer
                     Network.Wai.Application.Devel
  ghc-options:       -Wall

Executable           wai-handler-devel
