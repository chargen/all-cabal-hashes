name:                range-space
version:             0.1.0.1
synopsis:            A Range type with vector-space instances
description:         Provides functions for converting between ranges and spans
homepage:            https://github.com/JohnLato/range-space
license:             BSD3
license-file:        LICENSE
author:              John Lato
maintainer:          jwlato@gmail.com
-- copyright:           
category:            Math
build-type:          Simple
cabal-version:       >=1.10

library
  default-language:    Haskell2010
  ghc-options:         -Wall
  exposed-modules:     Data.RangeSpace
                       Data.RangeSpace.TwoD
  -- other-modules:       
  build-depends:       base >= 4.5 && < 5
                      ,vector-space == 0.8.*
                      ,vector-space-points == 0.1.*

Test-Suite test
  default-language: Haskell2010
  type: exitcode-stdio-1.0
  main-is: Properties.hs
  hs-source-dirs: tests .
  ghc-options:    -fhpc -hpcdir dist/hpc/mix/range-space-0.1.0.1

  build-depends:
      base,
      range-space,
      vector-space,
      vector-space-points,
      time                       == 1.4.*,
      QuickCheck                 >= 2   && < 3,
      test-framework             >= 0.3 && < 0.7,
      test-framework-quickcheck2 >= 0.2 && < 0.3

source-repository head
