stability:          experimental
category:           Natural Language Processing
homepage:           https://github.com/kawu/hist-pl/tree/master/dawg
build-type:         Simple

library
  hs-source-dirs:   src
  exposed-modules:    NLP.HistPL.DAWG
  build-depends:      base >= 4 && < 5 
                    , containers
                    , text
                    , binary
                    , text-binary
                    , dawg >= 0.11 && < 0.12

  ghc-options: -Wall

