    array,
    attoparsec,
    base -any,
    bimap,
    binary,
    binary-conduit,
    bytestring,
    conduit,
    conduit-combinators,
    conduit-extra,
    containers,
    csv-conduit,
    directory,
    exceptions,
    ghc-prim,
    lens,
    monad-loops,
    mtl,
    newtype,
    old-locale,
    parsec,
    primitive,
    quickcheck-instances,
    reinterpret-cast,
    resourcet,
    test-framework,
    test-framework-hunit,
    test-framework-quickcheck2,
    text,
    text-binary,
    time,
    transformers,
    vector,
    xml-conduit
  main-is: Tests/Main.hs
  buildable: True
  hs-source-dirs: src
