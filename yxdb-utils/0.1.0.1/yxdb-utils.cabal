category:            Database

build-type:          Simple

-- Constraint on the version of Cabal needed to build this package.
cabal-version:       >=1.8

extra-source-files:
  test-data/*.csv
  test-data/*.yxdb

executable csv2yxdb
  hs-source-dirs: bin-src
  main-is: Csv2Yxdb.hs
  build-depends:
    base == 4.*,
    yxdb-utils

