        mtl == 2.*, stm == 2.2.*
        ,
        -- util
        old-time == 1.*, random == 1.*, time == 1.*
        ,
        -- data structures
        array == 0.4.*, containers == 0.4.*, hashable == 1.*
        ,
        -- packing
        snappy == 0.2.*
        ,
        -- bytestring
        bytestring == 0.9.*, base16-bytestring == 0.1.*, base64-bytestring == 0.1.*,
        cereal == 0.3.*
        ,
        -- files
        directory == 1.*, filepath == 1.*
        ,
        -- crypto
        cprng-aes == 0.2.*, entropy >= 0.2


    ghc-options:
        -funbox-strict-fields
