  Build-Depends:        base
                      , attoparsec
                      , binary
                      , blaze-builder
                      , bytestring
                      , conduit >= 1.1
                      , conduit-extra >= 1.1
                      , containers
                      , dns
                      , hspec
                      , iproute >= 1.2.4
                      , mtl
                      , network >= 2.3
                      , random
