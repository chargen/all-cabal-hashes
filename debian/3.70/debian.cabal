   base >= 4 && < 5,
   bytestring,
   bzlib,
   containers,
   directory,
   filepath,
   HaXml >= 1.20,
   HUnit,
   mtl,
   network >= 2.4,
   old-locale,
   parsec >= 2 && <4,
   pretty,
   process,
   process-extras,
   pureMD5,
