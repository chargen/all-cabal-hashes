  main-is:  tests/suite.hs
  ghc-options: -Wall
  default-language: Haskell2010
  build-depends: hledger-lib
               , base >= 4.3 && < 5
               , cmdargs
               , containers
               , csv
               -- , data-pprint >= 0.2.3 && < 0.3
               , directory
               , filepath
               , HUnit
               , mtl
               , old-locale
               , old-time
               , parsec
               , regex-tdfa
               , regexpr
               , safe
               , split
               , test-framework
               , test-framework-hunit
               , time
               , transformers
  if impl(ghc >= 7.4)
    build-depends: pretty-show >= 1.6.4

