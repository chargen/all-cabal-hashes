category:            Database
build-type:          Simple
cabal-version:       >=1.8

library
  exposed-modules:     Database.HaskellDB.Connect.HDBC.CatchIO

  build-depends:         base < 5
                       , HDBC
                       , haskelldb
                       , haskelldb-connect-hdbc
                       , mtl
                       , MonadCatchIO-mtl
  hs-source-dirs:      src

