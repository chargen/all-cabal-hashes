                    attoparsec >= 0.8.6.1,
                    bytestring,
                    containers,
                    hashable >= 1.1.2.0,
                    text >= 0.11.0.2,
                    template-haskell >= 2.4,
                    time,
                    unordered-containers >= 0.1.3.0,
                    vector >= 0.7.1

