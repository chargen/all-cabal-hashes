  Build-Depends: 
        base                 == 4.*,
        ghc-prim             == 0.2.*,
        vector               >= 0.7 && < 0.8,
        QuickCheck           >= 2.3 && < 2.5,
        template-haskell     >= 2.5 && < 2.6

  ghc-options:
        -Wall -fno-warn-missing-signatures
        -Odph
        -funbox-strict-fields
