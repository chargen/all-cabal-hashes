  Build-Depends: 
        base                 == 4.7.*,
        template-haskell     == 2.9.*,
        vector               == 0.10.*,
        ghc-prim             == 0.3.*,
        bytestring           == 0.10.*,
        QuickCheck           == 2.7.*

  ghc-options:
        -Wall -fno-warn-missing-signatures
        -Odph
        -funbox-strict-fields
