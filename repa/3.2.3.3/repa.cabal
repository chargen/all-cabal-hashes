  Build-Depends: 
        base                 == 4.6.*,
        ghc-prim             == 0.3.*,
        vector               == 0.10.*,
        bytestring           == 0.10.*,
        template-haskell     == 2.8.*,
        QuickCheck           >= 2.3 && < 2.7

  ghc-options:
        -Wall -fno-warn-missing-signatures
        -Odph
        -funbox-strict-fields
        -fcpr-off

