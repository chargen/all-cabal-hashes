    array                >= 0.3.0.2 && < 0.6,
    base                 >= 4       && < 5,
    comonad              >= 4       && < 5,
    containers           >= 0.3     && < 0.6,
    free                 >= 4       && < 5,
    semigroupoids        >= 4       && < 5,
    semigroups           >= 0.8.3.1 && < 1,
    transformers         >= 0.2     && < 0.4

  exposed-modules:
    Data.Key

  ghc-options: -Wall
  hs-source-dirs: src


