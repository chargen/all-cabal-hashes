    semigroupoids        >= 3       && < 4,
    comonad-transformers >= 3       && < 4,
    comonads-fd          >= 3       && < 4,
    free                 >= 3       && < 4

  exposed-modules:
    Data.Key

  ghc-options: -Wall
  hs-source-dirs: src


