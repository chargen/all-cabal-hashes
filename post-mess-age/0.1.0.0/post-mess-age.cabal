maintainer:          dhelta.diaz@gmail.com
category:            Concurrency
build-type:          Simple
cabal-version:       >=1.10

library
  exposed-modules:     Control.Concurrent.PostMessAge
  build-depends:       base == 4.*
  default-language:    Haskell2010


