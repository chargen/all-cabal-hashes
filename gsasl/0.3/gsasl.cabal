name: gsasl
version: 0.3
synopsis: Bindings for GNU SASL
license: GPL-3
license-file: License.txt
author: John Millikin
maintainer: jmillikin@gmail.com
build-type: Simple
cabal-version: >=1.6
category: Network
stability: unstable
bug-reports: mailto:jmillikin@gmail.com
homepage: http://ianen.org/haskell/bindings/gsasl/
tested-with: GHC==6.12.1

source-repository head
  type: darcs
  location: http://ianen.org/haskell/bindings/gsasl/

library
  build-depends:
      base >=3 && < 5
    , transformers >= 0.2 && < 0.3
    , bytestring >= 0.9 && < 0.10

  exposed-modules: Network.Protocol.SASL.GNU

  ghc-options: -Wall -fno-warn-unused-do-bind
  extra-libraries: gsasl
  pkgconfig-depends: libgsasl
  c-sources: hsgsasl-shim.c


