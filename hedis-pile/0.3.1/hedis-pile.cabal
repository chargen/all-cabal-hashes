                   base >= 4 && < 5,
                   bytestring >= 0.9 && < 0.10,
                   hedis >= 0.3 && < 0.4,
                   hedis-tags >= 0.1,
                   transformers >= 0.2 && < 0.3
  ghc-options:     -Wall
  exposed-modules: Database.Redis.Pile

