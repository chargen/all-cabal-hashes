  type: git
  location: https://github.com/haskell-compat/base-orphans

library
  hs-source-dirs: src
  exposed-modules:
      Data.Orphans
  build-depends:
      base >= 4.3 && < 5

    , ghc-prim
  ghc-options: -Wall
  default-language: Haskell2010

test-suite spec
