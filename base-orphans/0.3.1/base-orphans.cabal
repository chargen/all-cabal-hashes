      Data.Version.OrphansSpec
      Foreign.Storable.OrphansSpec
      GHC.Fingerprint.OrphansSpec
  build-depends:
      base >= 4.3 && < 5

    , base-orphans
    , hspec == 2.*
  ghc-options: -Wall
  default-language: Haskell2010


