                        glib >= 0.11.0 && < 0.12.0,
                        bytestring
						
        exposed-modules:
          Audio.TagLib.TagLib
        other-modules:

        extensions: ForeignFunctionInterface, CPP
		
        include-dirs:   .
        Extra-libraries: tag_c
