    , smallcheck        >= 1.0

test-suite spec
  type:
      exitcode-stdio-1.0
  ghc-options:
      -Wall -Werror
  hs-source-dirs:
      test
  main-is:
      Spec.hs
  build-depends:
      base              == 4.*
    , hspec
    , smallcheck

    , hspec-smallcheck
    , QuickCheck
