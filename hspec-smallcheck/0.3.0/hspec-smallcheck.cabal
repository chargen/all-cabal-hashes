name:             hspec-smallcheck
version:          0.3.0
license:          MIT
license-file:     LICENSE
copyright:        (c) 2013 Simon Hengel
maintainer:       Simon Hengel <sol@typeful.net>
build-type:       Simple
cabal-version:    >= 1.8
category:         Testing
bug-reports:      https://github.com/hspec/hspec-smallcheck/issues
homepage:         http://hspec.github.io/
synopsis:         SmallCheck support for the Hspec testing framework
description:      SmallCheck support for the Hspec testing framework

source-repository head
  type: git
  location: https://github.com/hspec/hspec-smallcheck

library
  ghc-options:
      -Wall
  hs-source-dirs:
      src
  exposed-modules:
      Test.Hspec.SmallCheck
  build-depends:
      base              == 4.*
    , hspec             >= 1.10
    , smallcheck        >= 1.0

test-suite spec
  type:
      exitcode-stdio-1.0
  ghc-options:
      -Wall -Werror
  hs-source-dirs:
      test
  main-is:
      Spec.hs
  build-depends:
      base              == 4.*
    , hspec
    , smallcheck

    , hspec-smallcheck
    , QuickCheck


