  build-tools:        ghc >= 6.9
  build-depends:      lighttpd-conf >= 0.4,
                      haskell-src-exts,
                      template-haskell >= 2.2,
                      base, array, bytestring
  extensions:         TemplateHaskell
  ghc-options:        -O2 -fglasgow-exts
  ghc-prof-options:
  hs-source-dirs:     src
  exposed-modules:    Lighttpd.Conf.QQ


