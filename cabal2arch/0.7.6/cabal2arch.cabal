name:               cabal2arch
version:            0.7.6
homepage:           http://github.com/archhaskell/
synopsis:           Create Arch Linux packages from Cabal packages.
description:        Create Arch Linux packages from Cabal packages.
category:           Distribution
license:            BSD3
license-file:       LICENSE
author:             Don Stewart <dons@galois.com>
maintainer:         ArchHaskell Team <arch-haskell@haskell.org>
cabal-version:      >= 1.6
build-type:         Simple

source-repository head
  type:                 git
  location:             git://github.com/archhaskell/cabal2arch.git

executable cabal2arch
    main-is:            Main.hs
    ghc-options:        -Wall

    build-depends:
        base >= 4 && <= 6,
        pretty,
        process,
        directory,
        containers,
        bytestring,
        Cabal > 1.8,
        filepath,
        archlinux >= 0.3.5,
        cmdargs


