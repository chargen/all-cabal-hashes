                        time,
                        mtl,
                        pandoc,
                        gtk,
                        cairo,
                        uglymemo,
                        hcwiid
  other-modules:        Paths_carettah Config Render WrapPaths
  ghc-options:          -Wall
  ghc-prof-options:     -auto-all -caf-all

