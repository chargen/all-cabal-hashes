    base >= 4 && < 5,
    array >= 0.3.0.2 && < 0.5,
    transformers >= 0.2.0 && < 0.3,
    stm >= 2.1.2.1 && < 2.3

  exposed-modules:
    Control.Monad.Array.Class
    Data.Array.MArray.Extras

  ghc-options: -Wall


