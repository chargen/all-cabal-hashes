                   base >= 4,
                   HUnit >= 1.2 && < 2,
                   test-framework >= 0.4.1,
                   test-framework-hunit,

                   bytestring >= 0.9 && < 0.11,
                   http-types >= 0.7 && < 0.8,
                   text,
                   wai >= 1.3,
                   wai-test >= 1.3,
                   wai-middleware-route
  ghc-options:     -Wall -rtsopts
  hs-source-dirs:  test
  main-is:         Main.hs

