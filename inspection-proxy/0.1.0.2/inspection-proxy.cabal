name:                inspection-proxy
version:             0.1.0.2
synopsis:            A simple proxy for debugging plaintext protocols communication
description:         A command line utility to be launched as an intermediary plaintext service (http, memcache, etc) for easy inspection and debugging
license:             OtherLicense
license-file:        LICENSE
author:              Marius Ghita
maintainer:          mhitza@gmail.com
-- copyright:           
category:            Testing
build-type:          Simple
cabal-version:       >=1.8

source-repository head
    type:            git
    location:        https://github.com/mhitza/inspection-proxy

executable inspection-proxy
  main-is:             Main.hs
  ghc-options:         -Wall -threaded -O3
  -- other-modules:       
  build-depends:       base ==4.*, pipes-network >= 0.6, pipes >= 4.0, bytestring, async, cmdargs


