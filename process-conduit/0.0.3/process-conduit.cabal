                     , conduit         == 0.2.*
                     , process-conduit
  if !flag(examples)
    Buildable:         False

Executable process-qq
  Hs-source-dirs:      examples
  Main-is:             TestQQ.hs
  Build-depends:       base            == 4.*
                     , process-conduit
  if !flag(examples)
    Buildable:         False
