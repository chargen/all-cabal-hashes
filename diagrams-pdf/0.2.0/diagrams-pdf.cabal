                       diagrams-lib >= 0.7 && < 0.8,
                       diagrams-core >= 0.7 && < 0.8,
                       filepath >= 1.3 && < 1.4,
                       split >= 0.2.2 && < 0.3,
                       cmdargs >= 0.10 && < 0.11,
                       colour >= 2.3.3 && < 2.4
  Hs-source-dirs:      src

  Ghc-options:         -Wall

  Default-language:  Haskell2010
