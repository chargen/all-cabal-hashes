                       filepath >= 1.3 && < 1.4,
                       split >= 0.2.2 && < 0.3,
                       cmdargs >= 0.10 && < 0.11,
                       colour >= 2.3.3 && < 2.4,
                       lens >= 3.9 && < 3.10
