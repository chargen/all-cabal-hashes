                        , filepath
                        , unix          >= 2.7.0.1
                        , process
                        , random
                        , http-conduit  >= 2.1.1
                        , lens          >= 4.1.2
                        , lens-aeson
                        , aeson
                        , text
  ghc-options:          -Wall
  hs-source-dirs:       src
  default-language:     Haskell2010

executable test-holy-project
  hs-source-dirs:       test
  ghc-options:          -Wall
  main-is:              Test.hs
  other-modules:        HolyProject.GithubAPI.Test
                        , HolyProject.StringUtils.Test
                        , Paths_holy_project
  default-language:     Haskell2010
  build-depends:        base >=4.6 && < 5
                        , Cabal >= 1.18.0
                        , bytestring >= 0.10.4.0
