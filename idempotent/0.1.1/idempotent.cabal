name:                idempotent
version:             0.1.1
synopsis:            Idempotent monoids
description:
  A typeclass, and instances, for monoids that are idempotent.
homepage:            https://github.com/prophile/idempotent
bug-reports:         https://github.com/prophile/idempotent/issues
license:             MIT
license-file:        LICENSE
author:              Alistair Lynn
maintainer:          arplynn+idem@gmail.com
copyright:           Â© Alistair Lynn, 2014
category:            Data
build-type:          Simple
cabal-version:       >=1.8

library
  build-depends:
    base == 4.*
  exposed-modules:
    Data.Monoid.Idempotent,
    Data.Monoid.Extrema
  GHC-options:         -Wall

test-suite tests
  type:              exitcode-stdio-1.0
  main-is:           Tests.hs
  build-depends:
    base == 4.*,
    QuickCheck == 2.*,
    hspec >= 1.8 && < 2



