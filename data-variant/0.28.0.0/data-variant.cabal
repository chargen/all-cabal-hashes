name:                data-variant
version:             0.28.0.0
synopsis:            A variant data type, useful for modeling dynamically-typed programming languages
description:         Implements a 'variant' data type that can hold the
                     usual suspects: scalars (integers, floats, strings),
                     lists, association lists, booleans, null, and first-class
                     functions.
homepage:            https://bitbucket.org/tdammers/data-variant
license:             BSD3
license-file:        LICENSE
author:              Tobias Dammers
maintainer:          tdammers@gmail.com
-- copyright:           
category:            Data
build-type:          Simple
-- extra-source-files:  
cabal-version:       >=1.10

library
  exposed-modules:     Data.Variant
                 ,     Data.Variant.ToFrom
  -- other-modules:       
  -- other-extensions:    
  build-depends:       base >=4.5 && <5.0
               ,       safe >= 0.3.0 && < 1.0
  -- hs-source-dirs:      
  default-language:    Haskell2010


