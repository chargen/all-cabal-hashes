    hs-source-dirs: . test
    main-is:        TestMain.hs
    GHC-Options:    -Wall -fno-warn-missing-signatures

    build-depends : base
                  , hashable
                  , refcount
                  , HUnit == 1.2.*
                  , QuickCheck
                  , test-framework-quickcheck2 == 0.3.*
                  , test-framework-hunit == 0.3.*
                  , test-framework-th
                  , test-framework
                  , unordered-containers
                  , Cabal
