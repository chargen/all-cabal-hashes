  Build-Depends:
        base >= 4, base < 5, ghc-prim, containers, transformers, Boolean >= 0.1.0, colour >= 2.3, data-default,
        wl-pprint, stable-maps >= 0.0.3.3, csound-expression-dynamic >= 0.0.1
  Hs-Source-Dirs:      src/
  Exposed-Modules:
    Csound.Typed

    Csound.Typed.Types
    Csound.Typed.Control
    Csound.Typed.Render

    Csound.Typed.Gui

    Csound.Typed.Types.Prim
    Csound.Typed.Types.Evt
    Csound.Typed.Types.Tuple
