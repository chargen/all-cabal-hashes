Name: discordian-calendar
Version: 0.1
Cabal-Version: >= 1.6
License: BSD3
License-File: LICENSE
Author: Adam Curtis
Maintainer: acurtis@spsu.edu
Homepage: https://github.com/kallisti-dev/discordian-calendar
Category: Date
Synopsis: library for handling Discordian calendar dates
Build-type: Simple

source-repository head
  type: git
  location: git://github.com/kallisti-dev/discordian-calendar.git

library
  hs-source-dirs: src
  ghc-options: -Wall
  build-depends:   base == 4.*
                 , time >= 1.0 && < 1.5

  exposed-modules: Data.Time.Calendar.Discordian



