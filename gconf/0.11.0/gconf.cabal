  type:         darcs
  location:     http://code.haskell.org/gtk2hs/
  subdir:       gconf

Library
        build-depends:  base >= 4 && < 5, array, containers, haskell98, mtl,
                        glib >= 0.11 && < 0.12
						
        build-tools:    gtk2hsC2hs, gtk2hsHookGenerator, gtk2hsTypeGen
						
        exposed-modules:
