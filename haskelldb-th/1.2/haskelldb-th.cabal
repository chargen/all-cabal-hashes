  Build-depends: mtl
  Build-depends: base >= 3.0, haskelldb >= 0.11, template-haskell >= 2.2.0.0
  Extensions:
    TemplateHaskell
  Exposed-Modules:
        Database.HaskellDB.CodeGen
  Hs-source-dirs: src
  ghc-options: -O2


