                     , Snap.Snaplet.ReCaptcha.Example

  build-depends:       base < 5
                     , lens
                     , heist >= 0.14 && < 0.15
                     , snap >= 0.13 && < 0.15
                     , blaze-builder
                     , MonadCatchIO-transformers >= 0.3 && < 0.5
                     , transformers
                     , mtl
                     , aeson
                     , bytestring
                     , configurator
                     , text
                     , http-conduit >=2.1

