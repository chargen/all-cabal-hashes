  MIT
license-file:
  LICENSE
build-type:
  Simple
cabal-version:
  >=1.10


source-repository head
  type:
    git
  location:
    git://github.com/nikita-volkov/base-prelude.git


library
  hs-source-dirs:
    library
  other-modules:
  exposed-modules:
    BasePrelude
  build-depends:
    base >= 4.6 && < 4.9
  default-language:
    Haskell2010
