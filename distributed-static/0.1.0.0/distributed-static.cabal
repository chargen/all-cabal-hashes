  Build-Depends:       base >= 4 && < 5,
                       rank1dynamic >= 0.1 && < 0.2,
                       containers >= 0.4 && < 0.5,
                       bytestring >= 0.9 && < 0.11,
                       binary >= 0.5 && < 0.6 
  HS-Source-Dirs:      src
