        build-depends:    haskell-src >= 1.0.1.2
        build-depends:    ipprint >= 0.3
        build-depends:    fgl >= 5.4.2.2
        build-depends:    pretty >= 1.0.1.0
        other-modules:    GraphRewrite.Main.CmdLineOpts,
