Library {
    exposed-modules:
            System.Directory
    c-sources:
            cbits/directory.c
    include-dirs: include
    includes: HsDirectory.h
    install-includes: HsDirectory.h HsDirectoryConfig.h
    extensions: CPP, ForeignFunctionInterface
    build-depends: base >= 4.1 && < 4.3,
                   old-time >= 1.0 && < 1.1,
                   filepath >= 1.1 && < 1.2
