  hspec/samples/1.html


source-repository head
  type:
    git
  location:
    git://github.com/nikita-volkov/list-t-html-parser.git


library
  hs-source-dirs:
    library
  other-modules:
    ListT.HTMLParser.Renderer
  exposed-modules:
    ListT.HTMLParser
