  default-language:
    Haskell2010
  build-depends:
    list-t == 0.4.*,
    html-tokenizer >= 0.2.1 && < 0.3,
    html-entities >= 1.0.1 && < 1.2,
    case-insensitive == 1.2.*,
    text >= 1 && < 1.3,
    either == 4.*,
    mtl-prelude >= 1 && < 3,
    base-prelude >= 0.1.19 && < 0.2


