                   , HDBC >= 2.3.1.0 && < 2.4
                   , HDBC-sqlite3 >= 2.3.3.0 && < 2.4
                   , old-locale >= 1.0 && < 1.1
                   , text >= 0.11 && < 0.12
                   , time >= 1.2 && < 1.3
                   , transformers >= 0.2.2 && < 0.3
  Default-extensions: DeriveDataTypeable
