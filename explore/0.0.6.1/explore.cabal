                     39-nouna.output,
                     Help.hs

Executable           explore
  buildable:         True
  main-is:           Main.hs
  ghc-options:       -O2 -fglasgow-exts -funbox-strict-fields
  build-depends:     base <= 5,
                     array -any,
                     pngload -any
  extensions:        ViewPatterns
