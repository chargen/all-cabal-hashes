  Exposed-Modules:	System.Libnotify, System.Libnotify.Internal, System.Libnotify.Server, System.Libnotify.Types
  Pkgconfig-Depends:	libnotify
  Extensions:		DeriveDataTypeable, FlexibleInstances, FlexibleContexts

