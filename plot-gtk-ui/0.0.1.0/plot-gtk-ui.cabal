  Build-depends:       base >=4.7 && <4.8
                     , colour >=2.3 && <2.4
                     , plot >=0.2 && <0.3
                     , gtk >=0.13 && <0.14
                     , cairo >=0.13 && <0.14
                     , hmatrix >=0.16 && <0.17
                     , text >=1.2 && <1.3
                     , vector >=0.10.12.2 && <0.10.13.0
                     , fixed-vector >=0.7.0.0
  hs-source-dirs:      lib
