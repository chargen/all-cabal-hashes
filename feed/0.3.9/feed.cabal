  default: False

library
    Exposed-Modules: Text.RSS.Syntax
                     Text.RSS.Import
                     Text.RSS.Export
                     Text.RSS1.Syntax
                     Text.RSS1.Import
                     Text.RSS1.Utils
                     Text.RSS1.Export
                     Text.DublinCore.Types
                     Text.Atom.Feed
                     Text.Atom.Feed.Export
                     Text.Atom.Feed.Import
                     Text.Atom.Feed.Link
                     Text.Atom.Feed.Validate
                     Text.Atom.Pub
                     Text.Atom.Pub.Export
                     Text.Feed.Import
                     Text.Feed.Types
                     Text.Feed.Export
                     Text.Feed.Query
                     Text.Feed.Constructor
                     Text.Feed.Translate
                     Text.Feed.Util
    Ghc-Options:     -Wall
    Build-Depends:   base >= 4 && < 5,
                     old-locale >= 1,
                     old-time   >= 1,
                     xml >= 1.2.6,
                     utf8-string < 1



