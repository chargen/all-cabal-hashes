                     containers  >= 0.3    && <  0.5,
                     mtl         >= 1      && < 3.0,
                     parsec      >= 3.0    && <  3.2,
                     cmdargs     >= 0.6    && <= 0.9,
                     random      >= 1.0,
                     parsec-numbers,
                     diagrams-core >= 0.5 && < 0.6,
                     diagrams-lib >= 0.5 && < 0.6,
                     diagrams-svg >= 0.3 && < 0.4