cabal-version:       >= 1.10

source-repository head
  type:     git
  location: https://github.com/knupfer/test-invariant
                     
library
  exposed-modules:   Test.Invariant
  -- other-modules:
  -- other-extensions:
  build-depends:     base == 4.*
                   , QuickCheck
  hs-source-dirs:    src
  default-language:  Haskell2010

test-suite Tasty
  type:              exitcode-stdio-1.0
  build-depends:     base == 4.*
                   , test-invariant
                   , QuickCheck
                   , tasty
                   , tasty-quickcheck
  hs-source-dirs:    test
