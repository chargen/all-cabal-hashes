    semigroupoids          >= 1.2.2.4  && < 1.3,
    comonad                >= 1.1.1    && < 1.2,
    pointed                >= 2.0      && < 2.1,
    keys                   >= 2.1      && < 2.2,
    reducers               >= 0.1.4    && < 0.2

  exposed-modules:
    Data.Compressed.LZ78
    Data.Compressed.RunLengthEncoding
    Data.Compressed.Internal.LZ78

  ghc-options: -Wall 


