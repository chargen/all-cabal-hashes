category:            Network
build-type:          Simple
cabal-version:       >=1.8

source-repository head
  type: git
  location: git@github.com:jhickner/smtp-mail.git

library
  exposed-modules:
    Network.Mail.SMTP
    Network.Mail.SMTP.Auth
    Network.Mail.SMTP.Types

