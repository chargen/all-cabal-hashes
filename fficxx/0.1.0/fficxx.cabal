  ghc-options: 	-Wall -funbox-strict-fields -fno-warn-unused-do-bind
  ghc-prof-options: -caf-all -auto-all
  Build-Depends: base == 4.* , 
                 mtl>2, 
                 containers, 
                 filepath>1, 
                 directory, 
                 HStringTemplate, 
                 split,
                 process,
                 transformers >= 0.3, 
                 template-haskell,  
                 hashable,
                 unordered-containers,
                 lens > 3,
                 either, 
                 errors,
                 bytestring,
                 pureMD5,  
                 Cabal

