    Other-Modules:
        Subversion.Hash
        Subversion.Pool
        Subversion.Stream
        Subversion.Stream.Pipe
        Subversion.String
    GHC-Options:
        -Wall
    C-Sources:
        cbits/HsSVN.c
    Include-Dirs:
        cbits
    Install-Includes:
        HsSVN.h


