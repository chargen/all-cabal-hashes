                       Generics.SOP.Metadata
                       Generics.SOP.NP
                       Generics.SOP.NS
                       Generics.SOP.Universe
                       Generics.SOP.Sing
  build-depends:       base                 >= 4.6  && < 5,
                       template-haskell     >= 2.8  && < 2.10,
                       tagged               >= 0.7  && < 0.8,
                       ghc-prim             >= 0.3  && < 0.4
  hs-source-dirs:      src
  default-language:    Haskell2010
  ghc-options:         -Wall
