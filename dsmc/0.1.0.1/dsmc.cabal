name:                dsmc
version:             0.1.0.1
synopsis:            DSMC library for rarefied gas dynamics
license:             BSD3
license-file:        LICENSE
author:              Dmitry Dzhus
maintainer:          dima@dzhus.org
category:            Physics

build-type:          Simple
cabal-version:       >=1.8
tested-with:         GHC == 7.6.1

library
  ghc-options: -Wall -O2 -funbox-strict-fields -Odph -rtsopts -fno-liberate-case -funfolding-use-threshold1000 -funfolding-keeness-factor1000 -fllvm -optlo-O3
  hs-source-dirs: src

  exposed-modules:
    DSMC,
    DSMC.Cells,
    DSMC.Domain,
    DSMC.Surface,
    DSMC.Traceables,
    DSMC.Traceables.Parser,
    DSMC.Macroscopic,
    DSMC.Motion,
    DSMC.Particles,
    DSMC.Util.Constants,
    DSMC.Util.Vector

  other-modules:
    Control.Parallel.Stochastic,
    Data.Splittable,
    DSMC.Util

  build-depends:
    attoparsec   == 0.10.*,
    base         == 4.*,
    bytestring   == 0.10.*,
    containers   == 0.5.*,
    entropy      == 0.2.*,
    hslogger     == 1.2.*,
    mwc-random   == 0.12.*,
    parallel     == 3.2.*,
    primitive    == 0.5.*,
    repa         == 3.2.*,
    strict       == 0.3.*,
    transformers == 0.3.*,
    vector       == 0.10.*


