name:                Spock-worker
version:             0.2.1.2
synopsis:            Background workers for Spock
description:         Adds a background-job queue to Spock
homepage:            http://github.com/agrafix/Spock-worker
license:             MIT
license-file:        LICENSE
author:              Alexander Thiemann <mail@athiemann.net>
maintainer:          mail@athiemann.net
copyright:           (c) 2014 Alexander Thiemann
category:            Web
build-type:          Simple
cabal-version:       >=1.10
tested-with:         GHC==7.6.3, GHC==7.8.3

library
  exposed-modules:
                       Web.Spock.Worker,
                       Web.Spock.Worker.Internal.Queue
  other-modules:
  hs-source-dirs:      src
  default-language:    Haskell2010
  build-depends:
                       Spock >=0.7.2,
                       base >=4.6 && < 5,
                       containers >=0.5,
                       lifted-base,
                       mtl,
                       stm >=2.4,
                       text >=0.11.3.1,
                       time >=1.4,
                       transformers,
                       vector >=0.10
  ghc-options: -Wall -fno-warn-orphans

test-suite SpockWorkerTests
  type:                exitcode-stdio-1.0
  main-is:             Tests.hs
  hs-source-dirs:      test
  default-language:    Haskell2010
  build-depends:
                       HTF >=0.12.2.1,
                       Spock-worker,
                       base >=4.6 && < 5,
                       containers,
                       stm,
                       vector

source-repository head
  type:     git
  location: git://github.com/agrafix/Spock-worker.git


