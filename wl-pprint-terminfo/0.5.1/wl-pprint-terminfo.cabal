  build-depends: 
    base             >= 4     && < 5,
    semigroups       >= 0.7.1 && < 0.8,
    wl-pprint-extras >= 1.4   && < 1.5,
    hscurses         >= 1.4   && < 1.5,
    terminfo         >= 0.3.2 && < 0.4,
    transformers     >= 0.2.2 && < 0.3

  exposed-modules:
    System.Console.Terminfo.PrettyPrint

  ghc-options:      -Wall 


