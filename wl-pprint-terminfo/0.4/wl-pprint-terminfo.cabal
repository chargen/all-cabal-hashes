    hscurses         >= 1.4   && < 1.5,
    terminfo         >= 0.3.2 && < 0.4,
    transformers     >= 0.2.2 && < 0.3

  exposed-modules:
    System.Console.Terminfo.PrettyPrint

  ghc-options:      -Wall 


