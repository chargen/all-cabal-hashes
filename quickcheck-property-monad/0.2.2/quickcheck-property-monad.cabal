library
    build-depends:
        base >=4.5 && <4.8,
        either >=4.1.1 && <4.4,
        transformers >=0.3.0.0 && <0.4,
        QuickCheck >=2.5.1.1 && <2.8
    exposed-modules:
        Test.QuickCheck.Property.Monad
    exposed: True
    buildable: True
    default-language: Haskell2010
    hs-source-dirs: src
    ghc-options: -Wall
 
test-suite doctests
    build-depends:
        base >=4.5 && <4.8,
        directory >=1.1.0.2 && <1.3,
        doctest >=0.9.11 && <0.10,
        filepath >=1.3.0.0 && <1.4
     
    if impl(ghc <7.6.1)
        buildable: True
        ghc-options: -Werror
     
    if flag(tests)
        build-depends:
            QuickCheck ==2.7.*
        buildable: True
    type: exitcode-stdio-1.0
    main-is: doctests.hs
    buildable: False
    default-language: Haskell2010
    hs-source-dirs: tests
