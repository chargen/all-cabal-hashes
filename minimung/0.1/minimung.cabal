  build-depends:   base >= 4.0 && < 5.0, haskell98, unix, GLUT
  Main-Is:         Minimung.hs
  hs-source-dirs:  .
  Other-modules:   Mac
                   Mac.Carbon
                   Mac.QuickTime
                   Mac.QuickDraw
                   Data.Image
                   Data.Geometry
                   Data.Camera
  frameworks:      QuickTime Carbon
  ld-options:      -threaded
