                    Data.Char.CEDICT.Reader.ListRebuild
                    Data.Char.CEDICT.Reader.RoseBush
                    Data.Char.CEDICT.Reader.Parser
  includes:         c/data.h
  c-sources:        c/data.c

executable          zi4pu3
  main-is:          Zi4Pu3.hs
  includes:         c/data.h
  c-sources:        c/data.c



