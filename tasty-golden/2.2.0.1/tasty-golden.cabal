                       Test.Tasty.Golden.Advanced
                       Test.Tasty.Golden.Manage
  other-modules:
                       Test.Tasty.Golden.Internal
  build-depends:
    base ==4.*,
    tasty >= 0.5,
    bytestring,
    process,
    mtl,
    optparse-applicative,
    filepath,
    temporary >= 1.1,
    tagged
