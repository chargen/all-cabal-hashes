    -- N.B., stm >= 2.1.2 is required for fast readTVarIO. And
    -- stm >= ?.?.? is required for fast tryReadTChan, peekTChan,
    -- tryPeekTChan.
    Build-Depends: base >= 4.1 && < 5
                 , stm  >= 2.1.1
    
    Hs-Source-Dirs:  src
    Exposed-Modules: Control.Concurrent.STM.TBChan
                   , Control.Concurrent.STM.TBMChan
