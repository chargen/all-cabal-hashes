                   , containers                >= 0.2
                   , xml-types                 >= 0.3.4    && < 0.4
                   , attoparsec                >= 0.10
                   , blaze-builder             >= 0.2      && < 0.4
                   , transformers              >= 0.2      && < 0.4
                   , failure                   >= 0.1      && < 0.3
                   , data-default
                   , system-filepath           >= 0.4      && < 0.5
                   , monad-control             >= 0.3      && < 0.4
                   , blaze-markup              >= 0.5
                   , blaze-html                >= 0.5
                   , deepseq                   >= 1.1.0.0
    exposed-modules: Text.XML.Stream.Parse
                     Text.XML.Stream.Render
