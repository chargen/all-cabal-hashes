                   , attoparsec                >= 0.10
                   , blaze-builder             >= 0.2      && < 0.4
                   , transformers              >= 0.2      && < 0.4
                   , failure                   >= 0.1      && < 0.3
                   , data-default
                   , system-filepath           >= 0.4      && < 0.5
                   , monad-control             >= 0.3      && < 0.4
                   , blaze-markup              >= 0.5      && < 0.6
                   , blaze-html                >= 0.5      && < 0.6
    exposed-modules: Text.XML.Stream.Parse
