license-file:        LICENSE
author:              Omari Norman
maintainer:          omari@smileystation.com
copyright:           2013 Omari Norman
category:            Console
build-type:          Simple
cabal-version:       >=1.8

source-repository head
    type: git
    location: git://github.com/massysett/dapi.git

executable dapi
  main-is: dapi.hs
  ghc-options: -Wall
  build-depends:
      base ==4.6.*
    , parsec == 3.1.*
    , text ==0.11.*
    , containers ==0.5.*
    , transformers ==0.3.*
    , time ==1.4.*
    , explicit-exception ==0.1.*
    , prednote ==0.6.*
    , multiarg ==0.12.*
    , old-locale ==1.0.*
    , rainbow ==0.2.*
