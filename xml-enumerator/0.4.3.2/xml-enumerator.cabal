                   , attoparsec-text           >= 0.8.2    && < 0.9
                   , blaze-builder             >= 0.2      && < 0.4
                   , blaze-builder-enumerator  >= 0.2      && < 0.3
                   , transformers              >= 0.2      && < 0.3
                   , failure                   >= 0.1      && < 0.2
