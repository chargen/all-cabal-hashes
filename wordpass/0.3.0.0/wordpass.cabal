  main-is:             WordPass.hs
  other-modules:    Data.Random.RVar.Enum   
  other-extensions:    OverlappingInstances, MultiParamTypeClasses, FlexibleInstances
  build-depends:       base          >=4.6  && <4.7,
                       text          >=1.1  && <1.2,
                       containers    >=0.5  && <0.6,
                       random-fu     >=0.2  && <0.3,
                       random-source >=0.3  && <0.4,
                       vector        >=0.10 && <0.11,
                       directory     >= 1.2 && <1.4,
                       hflags        >= 0.4 && <0.5
