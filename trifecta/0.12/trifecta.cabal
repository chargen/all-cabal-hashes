    fingertree         >= 0.0.1   && < 0.1,
    reducers           >= 0.1.2   && < 0.2,
    parsec             >= 3.1.1   && < 3.2,
    utf8-string        >= 0.3.6   && < 0.4,
    semigroupoids      >= 1.2.4   && < 1.3,
    parallel           >= 3.1.0.1 && < 3.2,
    transformers       >= 0.2.2   && < 0.3,
    comonad            >= 1.1.1   && < 1.2,
