  doc/examples/Makefile
  doc/examples/*.lhs
  doc/library/Makefile
  doc/library/build/*.tex
  doc/library/include/*.tex
  doc/testsuite/Makefile
  doc/testsuite/build/*.tex
  doc/testsuite/include/*.tex

Library
  build-depends:    base
  hs-source-dirs:   src
  exposed-modules:
    Data.PcSets
    Data.PcSets.Catalog
    Data.PcSets.Compact
    Data.PcSets.Notes
    Data.PcSets.Svg
  ghc-options:      -Wall

