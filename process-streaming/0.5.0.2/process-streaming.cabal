      , pipes-text >= 0.0.0.10 && < 0.0.2
      , text >= 0.11.2 && < 1.2
      , pipes-concurrency >= 2.0.2 && < 3
      , pipes-safe >= 2.2.0 && < 3
      , pipes-parse >=3.0.1 && <3.1
      , exceptions >= 0.6.0 && < 0.7
      , void >= 0.6 && < 0.7
      , semigroups >= 0.15 && < 0.16
      , containers >= 0.4
      , process-streaming
      , tasty >= 0.9
      , tasty-hunit >= 0.9
      , attoparsec >= 0.11
      , pipes-attoparsec >= 0.5
