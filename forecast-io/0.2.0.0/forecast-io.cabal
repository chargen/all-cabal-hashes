tested-with:         GHC == 7.8

homepage:            https://github.com/stormont/forecast-io

bug-reports:         https://github.com/stormont/forecast-io/issues

source-repository head
  type:      git
  location:  https://github.com/stormont/forecast-io.git

library
  -- Modules exported by the library.
  exposed-modules:
    ForecastIO.V2.Types,
    ForecastIO.V2.URI
  
