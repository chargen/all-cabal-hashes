  exposed-modules: Static.Resources
                   Static.Resources.Types
                   Static.Resources.Spec
                   Static.Resources.Checker
                   Static.Resources.Import
                   Static.Resources.Generation


  hs-source-dirs:  src
  ghc-options:     -Wall
  extensions:      DeriveDataTypeable
