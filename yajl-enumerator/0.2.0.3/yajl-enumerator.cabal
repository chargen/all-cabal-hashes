  ghc-options: -Wall -fno-warn-unused-do-bind

  build-depends:
      base >= 4 && < 5
    , bytestring >= 0.9 && < 0.10
    , text >= 0.7 && < 0.11
    , enumerator >= 0.4 && < 0.5
    , transformers >= 0.2 && < 0.3
    , yajl >= 0.3 && < 0.4

  exposed-modules:
    Text.JSON.YAJL.Enumerator
