cabal-version:      >= 1.1.4
build-type:         Simple
name:               hoogle
version:            4.0.0.1
license:            GPL
license-file:       LICENSE
category:           Development
author:             Neil Mitchell <ndmitchell@gmail.com>
maintainer:         Neil Mitchell <ndmitchell@gmail.com>
copyright:          Neil Mitchell 2004-2008
synopsis:           Haskell API Search
description:
    Hoogle is a Haskell API search engine, which allows you to
    search many standard Haskell libraries by either function name,
    or by approximate type signature.
homepage:           http://www.haskell.org/hoogle/
stability:          Beta
build-depends:      base, parsec, filepath, directory, process, safe, containers, uniplate, mtl, array
extra-source-files: README.txt
data-files:         default.hoo

executable:         hoogle
