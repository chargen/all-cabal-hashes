        base                 == 4.*,
        dph-prim-par         == 0.4.*,
        repa                 == 1.1.*,
        bytestring           == 0.9.1.*

  ghc-options:
        -Odph -Wall -fno-warn-missing-signatures

  Exposed-modules:
        Data.Array.Repa.ByteString
      


