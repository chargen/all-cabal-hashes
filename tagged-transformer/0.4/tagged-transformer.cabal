    distributive              >= 0.3      && < 1,
    exceptions                >= 0.1.1    && < 0.2,
    mtl                       >= 2.0.1    && < 2.2,
    reflection                >= 1.1.6    && < 2,
    semigroupoids             >= 3.0.2    && < 4,
    tagged                    >= 0.4.4    && < 1,
    transformers              >= 0.2      && < 0.4

  exposed-modules:
    Data.Functor.Trans.Tagged

  hs-source-dirs: src
  ghc-options: -Wall
