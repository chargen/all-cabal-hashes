name:           tagged-transformer
version:        0.3.1
license:        BSD3
license-file:   LICENSE
author:         Edward A. Kmett
maintainer:     Edward A. Kmett <ekmett@gmail.com>
stability:      experimental
category:       Data, Phantom Types
synopsis:       Provides newtype wrappers for phantom types to avoid unsafely passing dummy arguments
homepage:       http://github.com/ekmett/tagged-transformer
copyright:      2011 Edward A. Kmett
description:    Provides newtype wrappers for phantom types to avoid unsafely passing dummy arguments
build-type:     Simple
cabal-version:  >=1.6
extra-source-files:
  .travis.yml
  .vim.custom
  .ghci
  .gitignore
  CHANGELOG.markdown
  README.markdown

source-repository head
  type: git
  location: git://github.com/ekmett/tagged-transformer.git

library
  build-depends:
    base                      >= 4        && < 5,
    array                     >= 0.3.0.2  && < 0.5,
    comonad-transformers      >= 3        && < 4,
    contravariant             >= 0.3      && < 1,
    distributive              >= 0.3      && < 1,
    mtl                       >= 2.0.1    && < 2.2,
    comonad                   >= 3        && < 4,
    reflection                >= 1.1.6    && < 2,
    semigroupoids             >= 3.0.2    && < 4,
    tagged                    >= 0.4.4    && < 1,
    transformers              >= 0.2      && < 0.4

  exposed-modules:
    Data.Functor.Trans.Tagged

  hs-source-dirs: src
  ghc-options: -Wall
