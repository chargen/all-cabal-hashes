  Exposed-modules:     Data.IntervalMap, Data.IntervalMap.Interval
  Build-depends:       base >= 4 && < 5, containers, deepseq
  ghc-options: -Wall
  if impl(ghc >= 6.8)
    ghc-options: -fwarn-tabs

