  .travis.yml
cabal-version:       >=1.10

library
  exposed-modules:     Control.Object
                     , Control.Monad.Objective
                     , Control.Monad.Objective.Class
                     , Control.Monad.Objective.IO
                     , Control.Monad.Objective.ST
                     , Data.Functor.Request
                     , Data.Functor.PushPull
