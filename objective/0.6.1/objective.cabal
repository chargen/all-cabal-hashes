  build-depends:       base >=4.5 && <5, transformers >= 0.3 && <0.5, clean-unions < 0.2, elevator, containers, minioperational >= 0.4 && <0.5, profunctors >= 4.0 && <5
  ghc-options: -Wall
  hs-source-dirs:      src
  default-language:    Haskell2010


