Name:                 nemesis
Version:              2013.7.27
Build-type:           Simple
Synopsis:             a task management tool for Haskell
Description:          Organize common shell tasks into a meaningful tree like structure with dependency tracking

License:              BSD3
License-file:         LICENSE
Author:               Jinjing Wang
Maintainer:           Jinjing Wang <nfjinjing@gmail.com>
Build-Depends:        base
Cabal-version:        >= 1.6
category:             Web
homepage:             http://github.com/nfjinjing/nemesis

data-files:           readme.md
                    , changelog.md
                    , known-issues.md
                    , Nemesis
                    , test/N1.hs
                    , test/N2.hs
                    , test/N3.hs
                    
source-repository head
  type: git
  location: https://github.com/nfjinjing/nemesis.git

library
  ghc-options: -Wall
  build-depends:      base >= 4 && < 100
                    , time
                    , mtl
                    , process
                    , containers
                    , air >= 2013.7.18
                    , dlist
                    , Glob
                    , directory
                      
  hs-source-dirs: src/
  exposed-modules:  
                      System.Nemesis
                      System.Nemesis.DSL
                      System.Nemesis.Env
  other-modules:
                      System.Nemesis.Util
                      
   



