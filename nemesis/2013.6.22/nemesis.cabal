                    , mtl
                    , process
                    , containers
                    , air >= 2013.6.22
                    , dlist
                    , Glob
                    , directory
                      
  hs-source-dirs: src/
  exposed-modules:  
                      System.Nemesis
                      System.Nemesis.DSL
                      System.Nemesis.Env
  other-modules:
