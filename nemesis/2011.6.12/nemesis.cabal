Name:                 nemesis
Version:              2011.6.12
Build-type:           Simple
Synopsis:             a Rake like task management tool
Description:          smart per project code snippets

License:              BSD3
License-file:         LICENSE
Author:               Jinjing Wang
Maintainer:           Jinjing Wang <nfjinjing@gmail.com>
Build-Depends:        base
Cabal-version:        >= 1.2
category:             Web
homepage:             http://github.com/nfjinjing/nemesis
data-files:           readme.md, changelog.md, known-issues.md, Nemesis

library
  ghc-options: -Wall
  build-depends: base >= 4 && < 5, old-time, time, haskell98, mtl, process, containers, data-default, Glob >= 0.4, air >= 2011.6.12
  hs-source-dirs: src/
  exposed-modules:  
                      System.Nemesis
                      System.Nemesis.DSL
                      System.Nemesis.Env
  other-modules:
                      System.Nemesis.Util
                      

Executable            nemesis
  build-depends:      base >= 4 && < 5, haskell98, mtl, process, containers, data-default, directory, air >= 2011.6.12
  hs-source-dirs:     src/
  main-is:            System/Nemesis/Runner.hs
  other-modules:      System.Nemesis.Util
                      System.Nemesis.DSL
                      


