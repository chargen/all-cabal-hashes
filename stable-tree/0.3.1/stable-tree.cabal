name:                stable-tree
version:             0.3.1
synopsis:            Trees whose branches are resistant to change
-- description:         
homepage:            https://github.com/tsuraan/stable-tree
license:             BSD3
license-file:        LICENSE
author:              jay groven
maintainer:          tsuraan@gmail.com
-- copyright:           
category:            Data
build-type:          Simple
-- extra-source-files:  
cabal-version:       >=1.10

executable demo
  build-depends:       base >=4.6 && <4.8
                     , containers
                     , stable-tree
  hs-source-dirs:      demo
  main-is:             Main.hs
  default-language:    Haskell2010
  ghc-options:         -Wall -fllvm

library
  exposed-modules:     Data.StableTree
                     , Data.StableTree.Types
                     , Data.StableTree.Types.Key
                     , Data.StableTree.Persist
                     , Data.StableTree.Persist.Ram
  -- other-modules:       
  -- other-extensions:    
  build-depends:       base >=4.6 && <4.8
                     , binary
                     , blaze-builder
                     , bytestring
                     , cereal
                     , containers
                     , cryptohash >= 0.5.1
                     , mtl >= 2.2.1
                     , text
  hs-source-dirs:      src
  default-language:    Haskell2010
  ghc-options:         -Wall -fllvm

test-suite test-all
  type:                exitcode-stdio-1.0
  main-is:             TestAll.hs
  build-depends:       base >=4.6 && < 4.8
                     , bytestring-arbitrary
                     , containers
                     , QuickCheck >= 2.1
                     , stable-tree
                     , tasty
                     , tasty-quickcheck
  hs-source-dirs:      tests
  default-language:    Haskell2010
  ghc-options:         -Wall


