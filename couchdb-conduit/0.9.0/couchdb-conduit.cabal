                   couchdb-conduit,
                   aeson >= 0.6 && < 0.7,
                   attoparsec >= 0.8 && < 0.11,
                   attoparsec-conduit >= 0.5 && < 0.6,
                   blaze-builder >= 0.2.1 && < 0.4,
                   bytestring >= 0.9 && < 0.10,
                   conduit >= 0.5 && < 0.6,
                   containers >= 0.2,
