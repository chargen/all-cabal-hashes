  x-uses-tf:       true
  build-depends:   
                   base >= 4,
                   HUnit >= 1.2 && < 2,
                   QuickCheck >= 2.4,
                   test-framework >= 0.4.1,
                   test-framework-quickcheck2,
                   test-framework-hunit,
                   couchdb-conduit,
                   conduit == 0.1.*,
                   attoparsec >= 0.8 && < 0.11,
                   attoparsec-conduit >= 0.0 && < 0.1,
                   http-conduit == 1.2.*,
                   http-types >= 0.6 && < 0.7,
                   monad-control >= 0.3 && < 0.4,
                   transformers == 0.2.*,
                   transformers-base == 0.4.*,
                   lifted-base >= 0.1 && < 0.2,
                   bytestring >= 0.9 && < 0.10,
                   utf8-string >= 0.3 && < 0.4,
                   aeson >= 0.5 && < 0.6,
                   text >= 0.11 && < 0.12,
                   unordered-containers >= 0.1 && < 0.2,
                   syb,
                   containers >= 0.2,
                   data-default,
                   blaze-builder >= 0.2.1 && < 0.4
  ghc-options:     -Wall -rtsopts -threaded
