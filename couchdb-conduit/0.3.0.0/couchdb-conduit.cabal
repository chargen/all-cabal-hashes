  other-modules:   
                   Database.CouchDB.Conduit.Test.Explicit,
                   Database.CouchDB.Conduit.Test.Util,
                   Database.CouchDB.Conduit.Test.View,
                   Database.CouchDB.Conduit.Test.Generic,
                   CouchDBAuth

