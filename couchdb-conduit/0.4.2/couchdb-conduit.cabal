                   couchdb-conduit,
                   aeson >= 0.6 && < 0.7,
                   attoparsec >= 0.8 && < 0.11,
                   attoparsec-conduit >= 0.2 && < 0.3,
                   bytestring >= 0.9 && < 0.10,
                   conduit >= 0.2 && < 0.3,
                   containers >= 0.2,
                   http-conduit >= 1.2 && < 1.3,
                   http-types >= 0.6 && < 0.7,
