  x-uses-tf:       true
  build-depends:   
                   base >= 4,
                   HUnit >= 1.2 && < 2,
                   test-framework >= 0.4.1,
                   test-framework-hunit,
                   couchdb-conduit,
                   aeson >= 0.6 && < 0.7,
                   attoparsec >= 0.8 && < 0.11,
                   attoparsec-conduit >= 0.2 && < 0.3,
                   bytestring >= 0.9 && < 0.10,
                   conduit >= 0.2 && < 0.3,
                   containers >= 0.2,
                   http-conduit >= 1.2 && < 1.3,
                   http-types >= 0.6 && < 0.7,
                   monad-control >= 0.3 && < 0.4,
                   transformers >= 0.2 && < 0.3,
                   transformers-base >= 0.4 && < 0.5,
                   lifted-base >= 0.1 && < 0.2,
                   utf8-string >= 0.3 && < 0.4,
                   text >= 0.11 && < 0.12,
                   unordered-containers >= 0.1,
                   syb,
                   data-default,
                   blaze-builder >= 0.2.1 && < 0.4
                   
