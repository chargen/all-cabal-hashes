  x-uses-tf:       true
  build-depends:   
                   base >= 4,
                   HUnit >= 1.2 && < 2,
                   test-framework >= 0.4.1,
                   test-framework-hunit,
                   couchdb-conduit,
                   aeson >= 0.6 && < 0.7,
                   attoparsec >= 0.8 && < 0.11,
                   attoparsec-conduit >= 0.4 && < 0.5,
                   blaze-builder >= 0.2.1 && < 0.4,
                   bytestring >= 0.9 && < 0.10,
                   conduit >= 0.4 && < 0.5,
                   containers >= 0.2,
                   data-default,
                   http-conduit >= 1.4 && < 1.5,
                   http-types >= 0.6 && < 0.7,
                   lifted-base >= 0.1 && < 0.2,
                   monad-control >= 0.3 && < 0.4,
                   string-conversions,
                   syb,
                   transformers >= 0.2 && < 0.4,
                   transformers-base >= 0.4 && < 0.5,
                   text >= 0.11 && < 0.12,
                   unordered-containers >= 0.1
                   
