                   test-framework >= 0.4.1,
                   test-framework-quickcheck2,
                   test-framework-hunit,
                   couchdb-conduit,
                   conduit >= 0.0.0.2 && < 0.1,
                   attoparsec >= 0.8 && < 0.11,
                   attoparsec-conduit >= 0.0 && < 0.1,
                   http-conduit >= 1.1.0.1 && < 1.2,
                   http-types >= 0.6 && < 0.7,
                   monad-control >= 0.3 && < 0.4,
                   transformers >= 0.2 && < 0.3,
                   transformers-base >= 0.4.1 && < 0.5,
                   lifted-base >= 0.1 && < 0.2,
