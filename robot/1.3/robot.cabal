    Other-Modules:
        Test.Robot.Types.Core
        Test.Robot.Types.Keys

    Build-Depends:    base == 4.*
                    , containers
                    , exceptions == 0.6.*
                    , transformers
                    , xhb

    GHC-Options: -Wall

