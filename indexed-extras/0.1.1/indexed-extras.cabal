  location:             git://github.com/reinerp/indexed-extras.git

Library
  Exposed-modules:
     Control.Monad.Indexed.Cont,
     Control.Monad.Indexed.State
  Build-depends:
     base < 5,
     indexed < 0.2,
     mtl < 2.2,
     pointed < 2.2,
     bifunctors < 0.2
