name:                monad-bool
version:             0.1.1
synopsis:            Short-circuiting, value-returning boolean monoids and monads
-- description:         
homepage:            http://github.com/jwiegley/monad-bool
license:             BSD3
license-file:        LICENSE
author:              John Wiegley
maintainer:          johnw@newartisans.com
-- copyright:           
category:            Control
build-type:          Simple
cabal-version:       >=1.10

library
  Default-language:  Haskell98
  exposed-modules:     Control.Monad.Bool
  -- other-modules:       
  build-depends:       base ==4.5.*
                     , attempt      >= 0.4.0
                     , cond         >= 0.4.0.2
                     , either       >= 3.0.3
                     , transformers >= 0.3.0.0

Test-suite doctests
  Default-language: Haskell98
  Type:    exitcode-stdio-1.0
  Main-is: Doctest.hs
  Hs-source-dirs: test
  Build-depends: base == 4.*
    , directory >= 1.0 && < 1.3
    , doctest   >= 0.8 && <= 0.10
    , filepath  >= 1.3 && < 1.4


