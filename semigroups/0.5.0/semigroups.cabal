name:          semigroups
category:      Algebra, Data, Data Structures, Math
version:       0.5.0
license:       BSD3
cabal-version: >= 1.6
license-file:  LICENSE
author:        Edward A. Kmett
maintainer:    Edward A. Kmett <ekmett@gmail.com>
stability:     provisional
homepage:      git://github.com/ekmett/semigroups/
copyright:     Copyright (C) 2011 Edward A. Kmett
synopsis:      Haskell 98 semigroups
description:   Haskell 98 semigroups
build-type:    Simple  

source-repository head
  type: git
  location: git://github.com/ekmett/semigroups.git

flag DeriveDataTypeable
  description: DeriveDataTypeable available
  default: True
  manual: False

library
  build-depends: 
    base >= 4 && < 4.4, 
    containers >= 0.3 && < 0.5

  ghc-options: -Wall
  extensions: CPP
  if flag(DeriveDataTypeable)
    extensions: DeriveDataTypeable
    cpp-options: -DLANGUAGE_DeriveDataTypeable

  exposed-modules:
    Data.Semigroup
    Data.List.NonEmpty


