Source-Repository head
  Type:                 git
  Location:             git://github.com/peti/hsyslog.git

Library
  Build-Depends:        base >= 3 && < 5
  Extensions:           ForeignFunctionInterface
  Exposed-Modules:      System.Posix.Syslog
  Ghc-Options:          -Wall


