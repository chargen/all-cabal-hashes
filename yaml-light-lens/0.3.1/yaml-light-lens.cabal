name:                yaml-light-lens
version:             0.3.1
synopsis:            Lens interface to yaml-light.
-- description:         
license:             BSD3
license-file:        LICENSE
author:              Anthony Cowley
maintainer:          acowley@gmail.com
copyright:           Copyright (C) 2012-2013 Anthony Cowley
category:            Data
build-type:          Simple
extra-source-files:  tests/doctests.hs
cabal-version:       >= 1.10

source-repository head
  type:     git
  location: http://github.com/acowley/yaml-light-lens.git

library
  exposed-modules:     Data.Yaml.YamlLight.Lens
  build-depends:       base >= 4.5 && < 5,
                       bytestring,
                       bytestring-lexing >= 0.4.3,
                       containers,
                       lens >= 4.0 && < 4.2,
                       yaml-light >= 0.1 && < 0.2
  hs-source-dirs:      src
  ghc-options:         -Wall -fno-warn-orphans
  default-language:    Haskell2010

test-suite doctests
  type:           exitcode-stdio-1.0
  main-is:        doctests.hs
  ghc-options:    -Wall -Werror -threaded
  hs-source-dirs: tests
  default-language:    Haskell2010
  build-depends:
    base,
    doctest
