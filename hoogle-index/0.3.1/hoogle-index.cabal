  location:            git://github.com/bgamari/hoogle-index

executable hoogle-index
  main-is:             Main.hs
  other-extensions:    TupleSections
  build-depends:       base >=4.6 && <4.8,
                       transformers >=0.3 && <0.5,
                       directory >=1.2 && <1.3,
                       process >=1.2 && <1.3,
                       filepath >=1.3 && <1.4,
                       errors >=1.4 && <1.5,
                       bytestring >=0.9 && <1.11,
                       temporary >=1.2 && <1.3,
                       Cabal >=1.20 && <1.21,
                       containers >=0.4 && <0.6,
