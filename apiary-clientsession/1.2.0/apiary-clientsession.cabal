                     , vault              >=0.3   && <0.4
                     , cereal             >=0.4   && <0.5
                     , bytestring         >=0.10  && <0.11
                     , time               >=1.4   && <1.5
                     , unix-compat        >=0.4   && <0.5
                     , data-default-class >=0.0   && <0.1

  hs-source-dirs:      src
  ghc-options:         -O2 -Wall
  default-language:    Haskell2010

