    , xhtml, utf8-string >= 0.3.3, parsec >= 2, gravatar >= 0.3
    , data-default >= 0.2
    , mps >= 2009.6.25, hcheat >= 2009.6.25
    , hack >= 2009.5.19, hack-contrib >= 2009.6.25
    , bamboo >= 2009.6.25
  hs-source-dirs: src/
  exposed-modules:  
                    Bamboo.Theme.Blueprint
