name:                filediff
version:             0.1.0.7
synopsis:            Diffing and patching module
description:         `filediff` is a Haskell library for creating diffs, and applying diffs to files and directories.
homepage:            https://github.com/bgwines/filediff
license:             BSD3
license-file:        LICENSE
author:              Brett Wines
maintainer:          bgwines@cs.stanford.edu
-- copyright:           
category:            Data
build-type:          Simple
extra-source-files:  README.md
cabal-version:       >=1.10
source-repository head
    type:     git
    location: git://github.com/bgwines/filediff.git

library
  exposed-modules: Filediff, Filediff.Types, Filediff.Sequence, Filediff.Stats
  other-modules:   Filediff.Utils
  -- other-extensions:
  build-depends:       base >=4.7 && <4.8, mtl, time, directory, either, transformers, data-memocombinators, Zora >=1.1.22, text, data-default,  tasty, tasty-hunit
  hs-source-dirs:      src
  default-language:    Haskell2010

