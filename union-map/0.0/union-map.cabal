library
  exposed-modules:     Data.UnionMap, Data.UnionIntMap
                     , Data.OpenUnion , Data.OpenUnion.Internal
  other-modules:       Data.UnionMap.Internal, Data.UnionIntMap.Internal
  build-depends:       base >= 4.6 && < 5
                     , containers >= 0.4.2
  hs-source-dirs:      src
  ghc-options:         -Wall
  default-language:    Haskell2010


