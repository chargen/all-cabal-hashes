copyright:           Philipp Balzarek
category:            Network
build-type:          Simple
cabal-version:       >=1.8
bug-reports:         https://github.com/Philonous/hs-stun/issues

library
  exposed-modules:     Network.Stun
                     , Network.Stun.Internal
