  Type:             exitcode-stdio-1.0
  Hs-source-dirs:   test
  Main-is:          Test.hs

  Build-depends:    base           == 4.*
                  , bytestring     == 0.9.*
                  , QuickCheck     == 2.4.*
                  , test-framework == 0.5.*
