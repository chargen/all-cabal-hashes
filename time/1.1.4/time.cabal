name:           time
version:        1.1.4
x-revision: 1
stability:      stable
license:        BSD3
license-file:   LICENSE
author:         Ashley Yakeley
maintainer:     <ashley@semantic.org>
homepage:       http://semantic.org/TimeLib/
synopsis:       A time library
description:    A time library
category:       System
build-type:  Custom
cabal-version:   >=1.6
x-follows-version-policy:

extra-source-files:
    aclocal.m4
    configure.ac
    configure
    include/HsTime.h
    include/HsTimeConfig.h.in
extra-tmp-files:
    config.log
    config.status
    autom4te.cache
