name:                hsfilt
version:             0.1.0.0
synopsis:            Z-decoder.
description:         Inspired by c++filt from GNU binutils. Reads mangled(z-encoded) names from stdin,
                     prints demangled(z-decoded) names to stdout.
license:             MIT
license-file:        LICENSE
author:              Ömer Sinan Ağacan
maintainer:          omeragacan@gmail.com
category:            Development
build-type:          Simple
cabal-version:       >=1.10

source-repository head
  type:                git
  location:            https://github.com/osa1/hsfilt.git

executable hsfilt
  main-is:             Main.hs
  build-depends:       base >=4.7 && <4.8, ghc
  hs-source-dirs:      src
  ghc-options:         -Wall
  default-language:    Haskell2010
