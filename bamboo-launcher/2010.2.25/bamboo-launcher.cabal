Name:                 bamboo-launcher
Version:              2010.2.25
Build-type:           Simple
Synopsis:             bamboo-launcher
Description:
        
    A standalone bamboo launcher to simplify deployment.

License:              GPL
License-file:         LICENSE
Author:               Wang, Jinjing
Maintainer:           Wang, Jinjing <nfjinjing@gmail.com>
Build-Depends:        base
Cabal-version:        >= 1.2
category:             Web
homepage:             http://github.com/nfjinjing/bamboo-launcher
data-files:           readme.md, changelog.md

Executable            bamboo
  ghc-options:        -Wall
  build-depends:      base >= 4 && < 5
                    , hack >= 2009.5.19
                    , hack-contrib >= 2009.5.19
                    , bamboo >= 2009.6.8
                    , hack-handler-hyena >= 2009.6.5
                    , mps >= 2009.5.13
                    , process
                    , haskell98
                    , bamboo-theme-blueprint >= 2009.6.8
                    , data-default >= 0.2
                    , bytestring
  hs-source-dirs:     src/
  main-is:            Launcher.hs


