name:                indices
version:             1.6.0
synopsis:            Static indices
-- description:         
license:             MIT
license-file:        LICENSE
author:              Mike Ledger
maintainer:          eleventynine@gmail.com
-- copyright:           
category:            Data
build-type:          Simple
-- extra-source-files:  
cabal-version:       >=1.10

library
  exposed-modules:     Data.Index
  -- other-modules:       
  other-extensions:    DataKinds
                     , GADTs
                     , PolyKinds
                     , BangPatterns
                     , TemplateHaskell
                     , ScopedTypeVariables
                     , TypeOperators
                     , MultiParamTypeClasses
                     , TypeFamilies
                     , UndecidableInstances
                     , FlexibleContexts
                     , FlexibleInstances
                     , DeriveGeneric
                     , DeriveDataTypeable
                     , DeriveFunctor
                     , DeriveFoldable
                     , DeriveTraversable
                     , PatternGuards
                     , QuasiQuotes
                     , Rank2Types
                     , KindSignatures

  build-depends:       base >=4.7 && <5.0
                     , tagged >=0.7 && <0.8
                     , template-haskell >=2.9 && <2.10

  hs-source-dirs:      src
  default-language:    Haskell2010
  ghc-options:         -Wall

benchmark unroll
  hs-source-dirs:      benchmarks
  main-is:             Unroll.hs 
  type:                exitcode-stdio-1.0
  build-depends:       base >=4.7 && <5.0
                     , indices >= 1.6.0
                     , criterion
  default-language:    Haskell2010

test-suite tests 
  hs-source-dirs:      tests
  main-is:             Tests.hs 
  type:                exitcode-stdio-1.0
  build-depends:       base >=4.7 && <5.0
                     , indices >= 1.6.0
                     , QuickCheck
  default-language:    Haskell2010


  



