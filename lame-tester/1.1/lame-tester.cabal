  default-language: Haskell2010
  build-depends:
    base                       >= 4.6 && < 5,
    containers                 >= 0.5,
    HUnit                      >= 1.2,
    lame-tester                >= 1.1,
    test-framework             >= 0.8,
    test-framework-hunit       >= 0.3
