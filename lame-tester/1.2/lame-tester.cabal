  main-is:          Main.hs
  hs-source-dirs:   test
  default-language: Haskell2010
  build-depends:
    base        >= 4.6 && < 5,
    containers  >= 0.5,
    lame-tester >= 1.1,
    tasty       >= 0.10,
    tasty-hunit >= 0.9
