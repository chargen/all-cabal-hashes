                     , Calculator.Parser.Cmd
                     , Calculator.Parser.Expr
                     , Calculator.Parser.Statement
                     , Calculator.Prim.Base
                     , Calculator.Prim.Cmd
                     , Calculator.Prim.Expr
                     , Calculator.Prim.Statement
                     , Model.Arithmetic
  hs-source-dirs:      tests/ src/
