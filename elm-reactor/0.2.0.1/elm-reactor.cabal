    filepath,
    fsnotify >= 0.1.0.2,
    HTTP,
    mtl,
    process,
    snap-core,
    snap-server,
    system-filepath,
    text,
    time,
    transformers,
    unordered-containers,
    websockets >= 0.9 && < 0.10,
    websockets-snap >= 0.8.2.1
