                   , text                    >= 0.7       && < 0.12
                   , yesod-form              >= 1.1       && < 1.3
                   , transformers            >= 0.2.2     && < 0.4
                   , lifted-base             >= 0.2       && < 0.3
    exposed-modules: Yesod.Auth.OAuth
    ghc-options:     -Wall

source-repository head
  type:     git
  location: https://github.com/yesodweb/yesod
