  hs-source-dirs: spec, src
  main-is: Spec.hs
  build-depends:       base >=4.5 && <4.9
                       , octohat >=0.1.2
                       , text
                       , hspec
                       , keyword-args
                       , optparse-applicative
                       , parsec
                       , keyword-args

  default-language:    Haskell2010
  ghc-options:         -Wall

source-repository head
