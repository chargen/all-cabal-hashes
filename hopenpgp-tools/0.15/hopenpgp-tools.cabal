               ,       errors
               ,       hOpenPGP               >= 2.0
               ,       lens
               ,       monad-loops
               ,       openpgp-asciiarmor     >= 0.1
               ,       optparse-applicative   >= 0.11.0
               ,       resourcet
               ,       text
               ,       transformers
               ,       wl-pprint-extras
               ,       yaml
  build-tools:         alex
               ,       happy
  default-language: Haskell2010

