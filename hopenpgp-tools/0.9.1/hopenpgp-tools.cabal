name:                hopenpgp-tools
version:             0.9.1
synopsis:            hOpenPGP-based command-line tools
description:         command-line tools for performing some OpenPGP-related operations
homepage:            http://floss.scru.org/hopenpgp-tools
license:             OtherLicense
license-file:        LICENSE
author:              Clint Adams
maintainer:          Clint Adams <clint@debian.org>
copyright:           2012-2014 Clint Adams
category:            Codec, Data
build-type:          Simple
cabal-version:       >=1.10

executable hot
  main-is:             hot.hs
  ghc-options:         -Wall
  other-modules:       HOpenPGP.Tools.Common
               ,       HOpenPGP.Tools.ExpressionParsing
  build-depends:       base                   > 4    && < 5
               ,       attoparsec             >= 0.10.0.0
               ,       bytestring
               ,       cereal
               ,       cereal-conduit
               ,       conduit                >= 1.1
               ,       conduit-extra          >= 1.1
               ,       errors
               ,       hOpenPGP               >= 1.5.2
               ,       lens
               ,       openpgp-asciiarmor     >= 0.1
               ,       optparse-applicative   >= 0.10.0
               ,       resourcet
               ,       text
               ,       transformers
  default-language: Haskell2010

executable hokey
  main-is:             hokey.hs
  ghc-options:         -Wall
  other-modules:       HOpenPGP.Tools.Common
  build-depends:       base                   > 4    && < 5
               ,       aeson
               ,       ansi-wl-pprint
               ,       base16-bytestring
               ,       bytestring
               ,       cereal
               ,       cereal-conduit
               ,       conduit                >= 1.1
               ,       conduit-extra          >= 1.1
               ,       containers
               ,       crypto-pubkey
               ,       cryptohash             >= 0.7.7
               ,       directory
               ,       errors
               ,       hOpenPGP               >= 1.5.1
               ,       ixset
               ,       lens
               ,       old-locale
               ,       optparse-applicative   >= 0.10.0
               ,       resourcet
               ,       text
               ,       time
               ,       transformers
               ,       yaml
  default-language: Haskell2010

executable hkt
  main-is:             hkt.hs
  ghc-options:         -Wall
  other-modules:       HOpenPGP.Tools.Common
               ,       HOpenPGP.Tools.ExpressionParsing
  build-depends:       base                   > 4    && < 5
               ,       attoparsec             >= 0.10.0.0
               ,       bytestring
               ,       cereal
               ,       cereal-conduit
               ,       conduit                >= 1.1
               ,       conduit-extra          >= 1.1
               ,       containers
               ,       crypto-pubkey
               ,       directory
               ,       fgl
               ,       graphviz
               ,       hOpenPGP               >= 1.4
               ,       ixset
               ,       lens
               ,       optparse-applicative   >= 0.10.0
               ,       resourcet
               ,       text
               ,       time
               ,       transformers
               ,       unordered-containers
  default-language: Haskell2010

source-repository head
  type:     git
  location: git://git.debian.org/users/clint/hopenpgp-tools.git

source-repository this
  type:     git
  location: git://git.debian.org/users/clint/hopenpgp-tools.git
  tag:      hopenpgp-tools/0.9.1


