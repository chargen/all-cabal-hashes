  main-is:             hokey.hs
  ghc-options:         -Wall
  build-depends:       base                   > 4    && < 5
               ,       ansi-wl-pprint
               ,       bytestring
               ,       cereal
               ,       cereal-conduit
               ,       conduit
               ,       crypto-pubkey
               ,       directory
               ,       hOpenPGP               >= 0.10.2
               ,       ixset
               ,       lens
               ,       optparse-applicative   >= 0.5.0
               ,       text
               ,       transformers
  default-language: Haskell2010

executable hkt
  main-is:             hkt.hs
  ghc-options:         -Wall
