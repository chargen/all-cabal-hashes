executable hot
  main-is:             hot.hs
  ghc-options:         -Wall
  build-depends:       base                   > 4    && < 5
               ,       attoparsec             >= 0.10.0.0
               ,       bytestring
               ,       cereal
               ,       cereal-conduit
               ,       conduit
