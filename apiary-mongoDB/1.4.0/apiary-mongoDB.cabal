                     , data-default-class >=0.0   && <0.1
                     , transformers       >=0.2   && <0.5
                     , time               >=1.4   && <1.6
                     , lifted-base        >=0.2   && <0.3
                     , bson               >=0.3   && <0.4
                     , monad-control      >=0.3   && <1.1
                     , text               >=1.1   && <1.3
                     , types-compat
  hs-source-dirs:      src
  ghc-options:         -O2 -Wall
  default-language:    Haskell2010

source-repository head
  type:     git
  location: git://github.com/philopon/apiary.git


