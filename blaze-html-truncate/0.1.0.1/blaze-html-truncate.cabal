  type: git
  location: git://github.com/mruegenberg/blaze-html-truncate.git

library
  exposed-modules:     Text.Blaze.Truncate
  -- other-modules:       
  build-depends:       base >= 4.5.0 && < 4.7
                     , blaze-html >= 0.4.0.0 && < 0.5.0.0
