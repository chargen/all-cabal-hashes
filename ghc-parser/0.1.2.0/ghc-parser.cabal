  build-depends:       base                 >=4.6 && <4.8,
                       ghc                  >=7.6 && <7.10

  if impl(ghc >= 7.6) && impl(ghc < 7.8)
    hs-source-dirs:      generic-src src-7.6
  else
    hs-source-dirs:      generic-src src-7.8

  default-language:    Haskell2010
