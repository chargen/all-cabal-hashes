  main-is:             Main.hs
  build-depends:       base >=4.7 && <4.8
                     , aeson >= 0.8.0.0
                     , bytestring
                     , attoparsec >= 0.12.1.0
                     , containers
                     , unordered-containers
                     , text >= 1.2.0.0
                     , vector
                     , scientific
                     , optparse-applicative
