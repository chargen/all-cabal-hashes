  Extensions:      CPP, TemplateHaskell, MagicHash, TypeSynonymInstances, FlexibleInstances
  Hs-Source-Dirs:  src
  Build-Depends:   base >= 3 && < 5

  if impl(ghc < 6.12)
    Build-Depends: packedstring == 0.1.*,
                   template-haskell >= 2.2 && < 2.4
  else
    Build-Depends: template-haskell >= 2.4 && < 2.10

