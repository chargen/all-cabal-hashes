copyright:           (c) 2013-2014 Edward L. Blake
category:            Data
build-type:          Simple
cabal-version:       >=1.8

extra-source-files:
    README.md
    Test/General.hs

library
  exposed-modules:
    Data.PHPSession
    Data.PHPSession.Types
    Data.PHPSession.Conv
    Data.PHPSession.ImplicitConv.ConvBool
