build-type:          Simple
homepage:            https://github.com/maxpow4h/editable
bug-reports:         https://github.com/maxpow4h/editable/issues
extra-source-files:  demo.hs
cabal-version:       >=1.10

source-repository head
  type:              git
  location:          git://github.com/maxpow4h/editable.git

library
