                     elisp/shm-manipulation.el
                     elisp/shm-nav.el
                     elisp/shm-node.el
                     elisp/shm-overlays.el
                     elisp/shm-simple-indent.el
                     elisp/shm-slot.el
                     elisp/shm-test.el
                     elisp/shm-tests.el
                     elisp/shm-type.el
                     elisp/shm-yank-kill.el

executable structured-haskell-mode
