name:                wai-static-pages
version:             0.3
synopsis:            generate static html pages from a WAI application
description:         helpers for generating static html pages in a WAI application.
license:             MIT
author:              Greg Weber
maintainer:          greg@gregweber.info
category:            Web
build-type:          Simple
cabal-version:       >=1.8
Homepage:            https://github.com/gregwebs/wai-static-pages

library
  exposed-modules:     Network.WAI.Application.StaticPages
  build-depends:      base == 4.*
                    , wai >= 3.0
                    , wai-extra
                    , http-types
                    , conduit
                    , blaze-builder
                    , bytestring
                    , text
                    , directory

