    semigroups              >= 0.8.3.1 && < 0.9,
    semigroupoids           >= 1.3.1.2 && < 1.4,
    representable-functors  >= 2.4.0.1 && < 2.5,
    representable-tries     >= 2.4.0.1 && < 2.5,
