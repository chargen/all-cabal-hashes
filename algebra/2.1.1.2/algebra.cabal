    array                   >= 0.3.0.2 && < 0.5,
    base                    >= 4       && < 5,
    distributive            >= 0.2.2   && < 0.3,
    transformers            >= 0.2     && < 0.4,
    tagged                  >= 0.4.2   && < 0.5,
    categories              >= 1.0     && < 1.1,
    containers              >= 0.3     && < 0.6,
    keys                    >= 2.1.3.1 && < 2.2,
