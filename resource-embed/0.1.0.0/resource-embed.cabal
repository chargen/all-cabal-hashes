license:             MIT
license-file:        LICENSE
author:              Tobias Dammers
maintainer:          tdammers@gmail.com
-- copyright:           
category:            Development
build-type:          Simple
-- extra-source-files:  
cabal-version:       >=1.10

executable resource-embed
  main-is:             Main.hs
  -- other-modules:       
  -- other-extensions:    
  build-depends: base >=4.5 && <5.0
               , bytestring >=0.9 && <1.0
               , directory >=1.1 && <2.0
  hs-source-dirs:      src
  default-language:    Haskell2010


