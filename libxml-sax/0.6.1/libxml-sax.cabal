name: libxml-sax
version: 0.6.1
synopsis: Bindings for the libXML2 SAX interface
license: MIT
license-file: license.txt
author: John Millikin
maintainer: jmillikin@gmail.com
copyright: Copyright (c) John Millikin 2010
build-type: Simple
cabal-version: >=1.6
category: Foreign, Text, XML, Parsing
stability: experimental
homepage: http://john-millikin.com/software/bindings/libxml-sax/
tested-with: GHC==6.12.1

source-repository head
  type: bazaar
  location: http://john-millikin.com/software/bindings/libxml-sax/

library
  ghc-options: -Wall

  build-depends:
      base >= 4 && < 5
    , bytestring >= 0.9 && < 0.10
    , text >= 0.7 && < 0.11
    , xml-types >= 0.1 && < 0.2

  build-tools:
    c2hs >= 0.15

  exposed-modules:
    Text.XML.LibXML.SAX

  extra-libraries: xml2
  pkgconfig-depends: libxml-2.0


