                       , quickcheck-simple
                       , relational-query
                       , containers
                       , transformers

  type:                exitcode-stdio-1.0
  main-is:             sqlsEq.hs
  other-modules:
                       Lex
                       Model

  hs-source-dirs:      test
  ghc-options:         -Wall

  default-language:     Haskell2010


