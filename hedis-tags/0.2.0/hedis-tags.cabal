                   test-framework >= 0.4.1,
                   test-framework-quickcheck2,
                   test-framework-hunit,
                  
                   hedis-tags,
                   
                   -- from lib
                   bytestring >= 0.9 && < 0.10,
                   hedis >= 0.5 && < 0.6,
 
                   lifted-base,
                   transformers
  ghc-options:     -Wall -rtsopts -threaded
  main-is:         Main.hs
  other-modules:   Database.Redis.Tags.Test.Tags

