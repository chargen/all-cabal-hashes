  build-depends: 
    base >= 4 && < 4.4,
    transformers >= 0.2.0 && < 0.3,
    comonad >= 1.0 && < 1.1,
    comonad-transformers >= 1.0 && < 1.5,
    streams >= 0.6 && < 0.7

  exposed-modules:
    Data.Functor.Foldable

  ghc-options: -Wall 
