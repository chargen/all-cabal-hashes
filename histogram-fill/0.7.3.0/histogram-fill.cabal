  hs-source-dirs: test
  main-is:        QC.hs
  other-modules:  QC.Instances
  ghc-options:    -Wall
  build-depends:  base >=3 && < 5,
                  histogram-fill,
                  vector,
                  QuickCheck >= 2,
                  test-framework,
