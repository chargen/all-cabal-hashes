    base >= 4 && < 4.1,
    containers >= 0.2 && < 0.3, 
    text >= 0.1 && < 0.2, 
    parsec >= 3.0 && < 3.1,
    fingertree >= 0.0 && < 0.1, 
    bytestring >= 0.9 && < 1.0, 
    category-extras >= 0.53 && < 0.60, 
    parallel >= 1.1 && < 1.2, 
    mtl >= 1.0 && < 1.2, 
    stm >= 2.1 && < 2.2, 
    bitset >= 1.0 && < 1.1, 
    QuickCheck < 2.2, 
    array >= 0.2 && < 0.3
