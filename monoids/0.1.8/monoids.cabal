    Data.Group
    Data.Group.Combinators
    Data.Group.Sugar
    Data.Monoid.Additive
    Data.Monoid.Additive.Sugar
    Data.Monoid.Applicative
    Data.Monoid.Categorical
    Data.Monoid.Combinators
