name:		monoids
version:	0.3.2
license:	BSD3
license-file:   LICENSE
author:		Edward A. Kmett
maintainer:	Edward A. Kmett <ekmett@gmail.com>
stability:	experimental
homepage:	http://github.com/ekmett/monoids
category:	Data, Math, Numerical
synopsis:	Deprecated: Use 'reducers'
description:    Deprecated: Use 'reducers'
copyright:      (c) 2009 Edward A. Kmett
build-type:     Simple
cabal-version:  >=1.6

source-repository head
  type: git
  location: git://github.com/ekmett/monoids.git

flag optimize
  description: Enable optimizations
  default: False

library
  build-depends: 
    base >= 4 && < 5,
    array >= 0.2 && < 0.5,
    containers >= 0.2 && < 0.5,
    bytestring >= 0.9 && < 1.0,
    fingertree >= 0.0.1 && < 0.2,
    text >= 0.1 && < 0.12,
    parallel >= 1.1 && < 3.3

  exposed-modules:
    Data.Generator
    Data.Generator.Combinators
    Data.Group
    Data.Group.Combinators
    Data.Group.Sugar
    Data.Monoid.Additive
    Data.Monoid.Applicative
    Data.Monoid.Combinators
    Data.Monoid.Monad
    Data.Monoid.Multiplicative
    Data.Monoid.Ord
    Data.Monoid.Reducer
    Data.Monoid.Self
    Data.Monoid.Sugar
    Data.Monoid.Union

  ghc-options: -Wall -fno-warn-duplicate-exports

  if flag (optimize)
    ghc-options: -funbox-strict-fields -O2 -fdicts-cheap
