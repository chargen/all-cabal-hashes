               , data-default         >=0.2
               , dns                  >=0.3.0
               , hslogger             >=1.1.0
               , iproute              >=1.2.4
               , lifted-base          >=0.1.0.1
               , mtl                  >=2.0.0.0
               , network              >=2.4.1.0
               , pureMD5              >=2.1.2.1
               , resourcet            >=0.3.0
               , random               >=1.0.0.0
