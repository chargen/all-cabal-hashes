    base >= 4 && < 5,
    containers >= 0.3.0.0 && < 0.5,
    vector >= 0.9 && < 0.10

  exposed-modules:
   Text.Hyphenation

  other-modules:
   Paths_hyphenation

  ghc-options: -Wall


