Name:                 hack2-contrib-extra
Version:              2014.12.20
Build-type:           Simple
Synopsis:             Hack2 contrib extra
Description:          Extra middlewares and utilities for Hack2
License:              BSD3
License-file:         LICENSE
Author:               Jinjing Wang
Maintainer:           Jinjing Wang <nfjinjing@gmail.com>
Build-Depends:        base
Cabal-version:        >= 1.2
category:             Web
homepage:             https://github.com/nfjinjing/hack2-contrib
data-files:           readme.md, changelog.md
tested-with:          GHC==7.6.3

library

  build-depends:
                    base >=4 && < 100
                  , cgi
                  , network
                  , old-locale
                  , old-time
                  , directory
                  , filepath
                  , containers
                  , bytestring
                  , data-default
                  , time
                  , air >= 2013.6.22
                  , air-extra
                  , hack2
                  , hack2-contrib

  hs-source-dirs: src/

  exposed-modules:

                    Hack2.Contrib.RequestExtra
                    Hack2.Contrib.Middleware.RegexpRouter
                    Hack2.Contrib.Middleware.UTF8Body


