  location: http://code.haskell.org/Agda/
  tag:      2.2.6

executable agda
  main-is:       Main.hs
  build-depends: Agda == 2.2.6,
                 -- Nothing is used from the following package, except
                 -- for the prelude.
                 base >= 3 && < 6
