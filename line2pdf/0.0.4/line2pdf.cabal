    if flag(small_base)
        build-depends: base >= 3, containers, bytestring
    else
        build-depends: base < 3
    hs-source-dirs:     src

executable line2pdf
    main-is:        line2pdf.hs
    extensions:     ImplicitParams, ExistentialQuantification
    if flag(small_base)
        build-depends: base >= 3, containers, bytestring
    else
        build-depends: base < 3
    hs-source-dirs:     . src
