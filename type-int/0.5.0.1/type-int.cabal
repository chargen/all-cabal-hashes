name:          type-int
category:      Type
version:       0.5.0.1
license:       BSD3
license-file:  LICENSE
author:        Edward Kmett
maintainer:    ekmett@gmail.com
stability:     alpha
homepage:      http://github.com/ekmett/type-int
synopsis:      Type Level 2s- and 16s- Complement Integers
copyright:     Copyright (C) 2006-2011 Edward Kmett. All Rights Reserved
description:   Type level 2s- and 16s- complement Integers (positive and negative),
               Booleans, Ord and Eq.
cabal-version: >= 1.6
build-type:    Simple

source-repository head
  type: git
  location: git://github.com/ekmett/type-int.git

Library
  exposed-modules:
    Data.Type.Boolean
    Data.Type.Ord
    Data.Type.Sign
    Data.Type.Binary
    Data.Type.Binary.Internals
    Data.Type.Binary.TH
    Data.Type.Hex
    Data.Type.Hex.Stage1
    Data.Type.Hex.Stage2
    Data.Type.Hex.Stage3
    Data.Type.Maybe

  other-extensions: 
    MultiParamTypeClasses,
    FunctionalDependencies,
    TemplateHaskell,
    UndecidableInstances,
    CPP,
    EmptyDataDecls,
    FlexibleContexts,
    FlexibleInstances,
    ScopedTypeVariables
  build-depends: 
    base >= 4 && < 5,
    template-haskell >= 2.5 && < 3
  ghc-options: -O2 -funbox-strict-fields
